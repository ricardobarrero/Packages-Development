                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
BUSINESSDIVISIONID, LOCALSKU, FULLPATH, 
SS_BDivision = case when level2 - Level1 &gt; 0 then  substring(FULLPATH, Level1+1, (level2 - Level1 -1) ) else '' end,
SS_CategoryL1 = case when level3 - Level2 &gt; 0 then  substring(FULLPATH, Level2+1, (level3 - Level2) -1) else '' end,
SS_CategoryL2 = case when level4 - Level3 &gt; 0 then  substring(FULLPATH, Level3+1, (level4 - Level3) -1) else '' end,
SS_CategoryL3 = case when level5 - Level4 &gt; 0 then  substring(FULLPATH, Level4+1, (level5 - Level4) -1) else '' end
--into #temp_SS
from(
select dbo.DIM_GROUP.GROUPID, BUSINESSDIVISIONID, LOCALSKU, FULLPATH, 
Level1 = CHARINDEX('/', FULLPATH,0),
level2 = CHARINDEX('/', FULLPATH,2),
level3 = CHARINDEX('/', FULLPATH,CHARINDEX('/', FULLPATH,2)+1),
level4 =  CHARINDEX('/', FULLPATH,(CHARINDEX('/', FULLPATH,CHARINDEX('/', FULLPATH,2)+1))+1),
level5 = len(FULLPATH)
/*FULLPATH, FULLPATHPARENT */
from dbo.DIM_GROUP  INNER JOIN dbo.DIM_GROUP_PRODUCT
ON (dbo.DIM_GROUP.GroupID=dbo.DIM_GROUP_PRODUCT.GroupID  AND  dbo.DIM_GROUP.status  =  'a'
and fullpath like '%Scan%')
where businessdivisionid IN ('POS','COMMS')
) as Groups_level</property>

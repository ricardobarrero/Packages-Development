                      SQLTask:SqlStatementSource="-- ===========================================================================================&#xA;-- Paso 1. Obtiene datos para la tabla de Suppliers STG_SLIM_SUPPLIER, con la tabla&#xA;-- STG_SLIM_STOCK original sin updates&#xA;-- ===========================================================================================&#xA;DELETE [STG_SLIM_SUPPLIER]&#xA;&#xA;INSERT INTO [STG_SLIM_SUPPLIER]&#xA;             (Warehouse_code, &#xA;             Article_Code, &#xA;             Supplier_Number, &#xA;             Supplier_Name, &#xA;             Primary_Supplier, &#xA;             Lead_Time, &#xA;             MOQ, &#xA;             IOQ)&#xA;SELECT DISTINCT &#xA;             Warehouse_Code, &#xA;             Article_Code, &#xA;             UPPER(Supplier_Decimal), &#xA;             Supplier_Name, &#xA;             1, &#xA;             Lead_Time, &#xA;             MOQ, &#xA;             IOQ&#xA;FROM   STG_SLIM_STOCK s&#xA;LEFT JOIN SQLAPP2List.skucentral.dbo.Products p with(nolock) on (s.Article_code=p.localsku)&#xA;WHERE (p.BusinessDivisionID='Exclusive' or p.brandid = 'ubq')&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

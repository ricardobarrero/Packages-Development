                <DTS:Executable
                  DTS:refId="Package\Load Slim\Stock\File Stock - Supplier\UPDATE SUPPLIER"
                  DTS:CreationName="Microsoft.ExecuteSQLTask"
                  DTS:Description="Execute SQL Task"
                  DTS:DTSID="{07FD8B5C-CB15-4D34-B949-18861DF53C24}"
                  DTS:ExecutableType="Microsoft.ExecuteSQLTask"
                  DTS:LocaleID="-1"
                  DTS:ObjectName="UPDATE SUPPLIER"
                  DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2017 CTP2.1; © 2017 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
                  DTS:ThreadHint="4">
                  <DTS:Variables />
                  <DTS:ObjectData>
                    <SQLTask:SqlTaskData
                      SQLTask:Connection="{AC9E372E-B325-462E-A235-192142687C02}"
                      SQLTask:SqlStatementSource="-- ===========================================================================================&#xA;-- Paso 2. Actualiza las columnas Lead_time, MOQ y IOQ definidas en sku central para todas las &#xA;--        localidades&#xA;-- ===========================================================================================&#xA;&#xA;UPDATE       Dwstaging.dbo.STG_SLIM_SUPPLIER&#xA;SET          Lead_Time = LeadTime*7,&#xA;                    MOQ = case when Warehouse_code = 'b1_' then p.MinimumOrderQuantity else p.OrderMultiple end,&#xA;                    IOQ = OrderMultiple&#xA;FROM         SQLAPP2List.Skucentral.dbo.ProductLogisticsView p, &#xA;             Dwstaging.dbo.STG_SLIM_SUPPLIER s&#xA;WHERE        p.DestinationLocationID = ltrim(rtrim(left(s.Warehouse_code, charindex('_', s.Warehouse_code)-1)))&#xA;and          p.Localsku              = s.Article_Code&#xA;&#xA;-- ===========================================================================================&#xA;-- Paso 3. Actualiza las columnas Supplier_Number y Supplier_Name unicamente para B1 &#xA;--         con los source location definidos en Sku central, los cuales deben ser bodegas de &#xA;--              Asia. &#xA;-- ===========================================================================================&#xA;&#xA;UPDATE       Dwstaging.dbo.STG_SLIM_SUPPLIER&#xA;SET          Supplier_Number = COALESCE(sourceLocationID, sourceVmiSupplierID),&#xA;                    Supplier_Name = substring(SourceVMISuppDesc,1,60)&#xA;FROM         SQLAPP2List.Skucentral.dbo.ProductLogisticsView p, &#xA;             Dwstaging.dbo.STG_SLIM_SUPPLIER s&#xA;WHERE        p.DestinationLocationID = ltrim(rtrim(left(s.Warehouse_code, charindex('_', s.Warehouse_code)-1)))&#xA;and          p.Localsku              = s.Article_Code&#xA;and          s.Warehouse_Code           = 'b1_'&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
                  </DTS:ObjectData>
                </DTS:Executable>
                <DTS:Executable
                  DTS:refId="Package\Load Slim\Stock\File Stock - Supplier\UPDATE XGT1_"
                  DTS:CreationName="Microsoft.ExecuteSQLTask"
                  DTS:Description="Execute SQL Task"
                  DTS:DTSID="{B6A8700A-F864-4B1E-A173-606AF57FCCE5}"
                  DTS:ExecutableType="Microsoft.ExecuteSQLTask"
                  DTS:LocaleID="-1"
                  DTS:ObjectName="UPDATE XGT1_"
                  DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2017 CTP2.1; © 2017 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
                  DTS:ThreadHint="5">
                  <DTS:Variables />
                  <DTS:ObjectData>
                    <SQLTask:SqlTaskData
                      SQLTask:Connection="{AC9E372E-B325-462E-A235-192142687C02}"
                      SQLTask:SqlStatementSource="&#xA;update Dwstaging.dbo.STG_SLIM_STOCK&#xA;set Stocked_Item = gt1.Stocked_Item,&#xA;    Analysis_Code_1 = gt1.Analysis_Code_1&#xA;--select count(*)&#xA;from Dwstaging.dbo.STG_SLIM_STOCK S ,(select Article_Code, Stocked_Item, Analysis_Code_1 from Dwstaging.dbo.STG_SLIM_STOCK where Warehouse_Code='xgt1_') gt1&#xA;where S.Article_Code = gt1.Article_Code&#xA;and S.Warehouse_Code ='xgtd1_';&#xA;&#xA;&#xA;/************************************************************************************************************************/&#xA;--se adiciona este parche para sumar el oh de xgtd1 en xgt1 y se elimina xgtd1 para evitar duplicados. &#xA;/************************************************************************************************************************/&#xA;update S &#xA;set S.Stock_On_Hand = (S.Stock_On_Hand+T.Stock_On_Hand)&#xA;from  Dwstaging.dbo.STG_SLIM_STOCK S  , (select Article_Code,Stock_On_Hand from  Dwstaging.dbo.STG_SLIM_STOCK where Warehouse_Code ='xgtd1_') T where S.Article_Code=T.Article_Code and S.Warehouse_Code='xgt1_'&#xA;--order by 1&#xA;&#xA;delete from Dwstaging.dbo.STG_SLIM_STOCK&#xA;where Warehouse_Code ='xgtd1_'&#xA;&#xA;/***********************************************************************************************************/&#xA;&#xA;&#xA;update  Dwstaging.dbo.STG_SLIM_STOCK&#xA;set Warehouse_Code= 'xgt1_'&#xA;where Warehouse_Code='xgtd1_'&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
                  </DTS:ObjectData>
                </DTS:Executable>
                <DTS:Executable
                  DTS:refId="Package\Load Slim\Stock\File Stock - Supplier\UPDATES"
                  DTS:CreationName="Microsoft.ExecuteSQLTask"
                  DTS:Description="Execute SQL Task"
                  DTS:DTSID="{9075FFA4-D919-4936-841B-B7E6F632E491}"
                  DTS:ExecutableType="Microsoft.ExecuteSQLTask"
                  DTS:LocaleID="-1"
                  DTS:ObjectName="UPDATES"
                  DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
                  DTS:ThreadHint="5">
                  <DTS:Variables />
                  <DTS:ObjectData>
                    <SQLTask:SqlTaskData
                      SQLTask:Connection="{AC9E372E-B325-462E-A235-192142687C02}"
                      SQLTask:SqlStatementSource="-- ===========================================================================================&#xA;-- Calcula ABC_Class de la tabla FACT_ABC_PRODUCT de Sybase IQ&#xA;-- ===========================================================================================&#xA;&#xA;update Dwstaging.dbo.STG_SLIM_STOCK &#xA;set ABC_Class = abc.ABC_T_Corporate,&#xA;     User_Defined_10= abc.ABC_T_Company,&#xA;     User_Defined_11= abc.ABC_T_Branch&#xA;from Dwstaging.dbo.STG_SLIM_STOCK s &#xA;      left join Dwstaging.dbo.STG_slim_ABC abc on (left(s.Warehouse_code, charindex('_', s.Warehouse_code)-1) = abc.LocationID&#xA;      and s.Article_Code = abc.LocalSKU)&#xA;&#xA;&#xA;/*&#xA;update Dwstaging.dbo.STG_SLIM_STOCK &#xA;set ABC_Class = abc.ABC_T_Company&#xA;from Dwstaging.dbo.STG_SLIM_STOCK s &#xA;      left join Dwstaging.dbo.STG_slim_ABC abc on (left(s.Warehouse_code, charindex('_', s.Warehouse_code)-1) = abc.LocationID&#xA;      and s.Article_Code = abc.LocalSKU)&#xA;*/&#xA;&#xA;&#xA;-- ===========================================================================================&#xA;-- Calcula LeadTime solo para localidad B1, tabla suministrada por Boris&#xA;-- 20181101 se cambio la tabla STG_Slim_LeadTime por STG_LeadTimesAsiatoB1 esta en la ruta \\vms102us\Slim4\SourceUpdate&#xA;-- ===========================================================================================&#xA;update Dwstaging.dbo.STG_SLIM_STOCK &#xA;set Lead_Time = l.[Lead Time]&#xA;from Dwstaging.dbo.STG_SLIM_STOCK s &#xA;      left join Dwstaging.dbo.STG_LeadTimesAsiatoB1 l on (left(s.Warehouse_code, charindex('_', s.Warehouse_code)-1) = l.LocationID &#xA;      and s.Article_Code = l.ProductID)&#xA;&#xA;-- ===========================================================================================&#xA;-- Actualiza LeadTime solo para las localidades suministradas por Boris&#xA;-- Esto inlcuye localidades principales y sucursales.&#xA;-- ===========================================================================================&#xA;update Dwstaging.dbo.STG_SLIM_STOCK &#xA;set Lead_Time = l.LeadTime&#xA;from Dwstaging.dbo.STG_SLIM_STOCK s, Dwstaging.dbo.STG_Slim_LeadTimeCompanies l&#xA;where ltrim(rtrim(left(s.Warehouse_code, charindex('_', s.Warehouse_code)-1))) = L.locationID;&#xA;&#xA;/*&#xA;-- ===========================================================================================&#xA;-- Calcula LeadTime para demas localidades, tabla suministrada por Boris&#xA;-- ===========================================================================================&#xA;update Dwstaging.dbo.STG_SLIM_STOCK &#xA;set Lead_Time = l.Estimate_days&#xA;from Dwstaging.dbo.STG_SLIM_STOCK s, Dwstaging.dbo.STG_Slim_LeadTimeCompanies l&#xA;where l.CompanyID = case when s.Warehouse_Code like 'm1%' then 'XUS'&#xA;                                    when s.Warehouse_Code like 'xcb%' then 'XCB'&#xA;                                   when s.Warehouse_Code like 'xiq%' then 'XIQ'&#xA;                                   else substring(Warehouse_code,1,3) end ;&#xA;&#xA;-- ===========================================================================================&#xA;-- Calcula LeadTime para las sucursales de cada pais&#xA;-- 20170710: Se coloca a Quito como sursal porque la mercancia se recibe primero en Guayaquil&#xA;-- 20180220: Se coloca a xclf1 y xgt1 como sucursales. Se creo una nueva bodega ppal en XGT&#xA;-- 20180425: se quito a xgt1 y se coloco a xgtd1&#xA;--20180920: Se coloca a xcbu1 como sucursal&#xA;-- ===========================================================================================&#xA;update Dwstaging.dbo.STG_SLIM_STOCK &#xA;set Lead_Time = case when Warehouse_code in ('xecq1_','xgtx1_','xmxc1_', 'xmxm1_','xmxmm1_','xmxn1_','xmxq1_','xmxr1_','xmxp1_','xclf1_', 'xgtd1_','xcbu1_') then 7&#xA;&#x9;&#x9; when Warehouse_code in ('xpet1_', 'xpea1_')  then 3&#xA;&#x9;&#x9; when Warehouse_code in ('xpec1_') then 2 end &#xA;where  Warehouse_code in ('xecq1_','xgtx1_','xmxc1_', 'xmxm1_','xmxmm1_','xmxn1_','xmxq1_','xmxr1_','xclf1_', 'xgtd1_', 'xpet1_','xmxp1_', 'xpea1_','xpec1_','xcbu1_');&#xA;*/&#xA;&#xA;-- ===========================================================================================&#xA;-- Calcula LeadTime solo para localidad B1, de los productos que no estan ene l archivo enviado por Boris, se obtienen&#xA;-- directo de SKU Central y se multiplica por 7 porque en SKU Central el valor está en semanas&#xA;-- ===========================================================================================&#xA;update Dwstaging.dbo.STG_SLIM_STOCK&#xA;set  Lead_Time = LeadTime*7&#xA;from SQLAPP2List.Skucentral.dbo.ProductLogisticsView p, Dwstaging.dbo.STG_SLIM_STOCK s&#xA;where p.DestinationLocationID = ltrim(rtrim(left(s.Warehouse_code, charindex('_', s.Warehouse_code)-1)))&#xA;and p.Localsku = s.Article_Code&#xA;and p.DestinationLocationid = 'b1'&#xA;and s.lead_Time is null and p.LeadTime is not null;&#xA;&#xA;-- ===========================================================================================&#xA;-- Calcula columnas de la tabla DIM_PRODUCT: Marca, categorias, BD&#xA;-- ===========================================================================================&#xA;update Dwstaging.dbo.STG_SLIM_STOCK &#xA;set User_Defined_1 = dp.ManufacturerBrandID,&#xA;User_Defined_2 = dp.ScCategoryName_L1,&#xA;User_Defined_3 = dp.ScCategoryName_L2,&#xA;User_Defined_4 = dp.BusinessDivisionID_2011,&#xA;User_Defined_5 = case when Warehouse_code like 'm1%' then 'us'&#xA;                             when Warehouse_code like 'b1%' then 'us'&#xA;&#x9;             when Warehouse_code like 'f1%' then 'us'&#xA;                             when Warehouse_code like 'xcb%' then 'co'&#xA;                             when Warehouse_code like 'xiq%' then 'cl'&#xA;                             else lower(substring(Warehouse_code,2,2)) end&#xA;--select s.Article_code, dp.localsku --44926&#xA;from Dwstaging.dbo.STG_SLIM_STOCK s left join SQLAPP2List.skucentral.dbo.Products p with(nolock) on (s.Article_code=p.localsku&#xA;                                         and (p.BusinessDivisionID='Exclusive' or p.brandid in('ama', 'ubq','jbl')) and Len(p.LocalSKU) &lt;= 10) --26&#xA;Left Join intxdw.dbo.DIM_PRODUCT dp&#xA;on (dp.companyid = case when s.warehouse_code like 'm1%' then 'XUS' &#xA;                             when s.warehouse_code like 'b1%' then 'XUS'&#xA;&#x9;             when s.warehouse_code like 'f1%' then 'XUS'&#xA;                             else substring(Warehouse_code,1,3) end and dp.localsku = s.Article_code and dp.dw_status&lt;&gt;'D')&#xA;where (p.BusinessDivisionID='Exclusive' or p.brandid in('ama', 'ubq','jbl')) and Len(p.LocalSKU) &lt;= 10 ;&#xA;&#xA;&#xA;-- ===========================================================================================&#xA;-- Calcula columnas de la tabla ProductLogistics de SKU Central: Supplier, MOQ, IOQ&#xA;-- ===========================================================================================&#xA;update Dwstaging.dbo.STG_SLIM_STOCK&#xA;set  MOQ = case when Warehouse_code like 'b1%' then p.MinimumOrderQuantity else OrderMultiple end,&#xA;     IOQ = p.OrderMultiple,&#xA;     EOQ_Logistic_Unit_1 =  case when Warehouse_code like 'b1%' then p.MinimumOrderQuantity else OrderMultiple end&#xA;from SQLAPP2List.Skucentral.dbo.ProductLogisticsView p, Dwstaging.dbo.STG_SLIM_STOCK s&#xA;where s.Analysis_Code_2 = p.companyid and &#xA;p.DestinationLocationID = ltrim(rtrim(left(s.Warehouse_code, charindex('_', s.Warehouse_code)-1)))&#xA;and p.Localsku = Article_Code&#xA;and p.status = 'a' ;&#xA;&#xA;&#xA;-- ===========================================================================================&#xA;-- Update Supplier_Decimal-Supplier_Name&#xA;-- ===========================================================================================&#xA;&#xA;update Dwstaging.dbo.STG_SLIM_STOCK&#xA;set Supplier_Decimal = FN.FactoryCode, &#xA;      Supplier_Name = FN.Location_Factory &#xA;from Dwstaging.dbo.STG_SLIM_STOCK S, Dwstaging.dbo.STG_Slim_FactoryNames FN&#xA;where s.Article_Code = FN.ProductID&#xA;AND s.Warehouse_Code='b1_' ;&#xA;&#xA;update Dwstaging.dbo.STG_SLIM_STOCK&#xA;set  Supplier_decimal = COALESCE(p.SourceVmiSupplierId, p.SourceLocationId, 'ERROR'),&#xA;       Supplier_Name = substring (  case when p.SourceVmiSupplierId is not null then p.sourceVMISuppDesc end ,1,60)&#xA;from SQLAPP2List.Skucentral.dbo.ProductLogisticsView p, Dwstaging.dbo.STG_SLIM_STOCK s&#xA;where p.DestinationLocationID = ltrim(rtrim(left(s.Warehouse_code, charindex('_', s.Warehouse_code)-1)))&#xA;and p.Localsku = Article_Code&#xA;and s.Warehouse_Code='b1_' ;&#xA;&#xA;-- ===========================================================================================&#xA;-- 20170804 - Update Supplier_Decimal-Supplier_Name-MOQ enviada por Boris en correo proque no se puede ingresar en SkuC&#xA;-- ===========================================================================================&#xA;&#xA;--update Dwstaging.dbo.STG_SLIM_STOCK&#xA;--set supplier_decimal='acv89', Supplier_name='Chiwan',&#xA;--moq = case when Article_code in ('UI150FOR15','UI150FOR18') then 4&#xA;--&#x9;&#x9;   when Article_code in ('UI150FOR21','UI231FOR18') then 100&#xA;--&#x9;&#x9;   when Article_code in ('UI331FOR18') then 2&#xA;--&#x9;&#x9;   when Article_code in ('UP330FOR03','UP331FOR03') then 1&#xA;--&#x9;&#x9;   end                                       &#xA;--where Article_code in ('UI150FOR15','UI150FOR18','UI150FOR21','UI231FOR18', 'UI331FOR18','UP330FOR03','UP331FOR03')               estos update se reemplazaron por el archivo SourceChanges&#xA;--and warehouse_code = 'xcls1_';                                                                                                    que es enviado por Boris y se carga a la tabla DWStaging.dbo.stg_slim_products_update        &#xA;&#xA;--update Dwstaging.dbo.STG_SLIM_STOCK&#xA;--set supplier_decimal='acv110', Supplier_name='Shanghai', moq =300  &#xA;--where Article_code in ('AB355NXT09','AB356NXT09')&#xA;--and warehouse_code = 'xcls1_';&#xA;&#xA;--update Dwstaging.dbo.STG_SLIM_STOCK&#xA;--set supplier_decimal='acv42', Supplier_name='Ningbo', &#xA;--moq = case when Article_code in('AW221NXT53','AW221NXT50','AW221NXT51','AW221NXT52') then 100&#xA;--&#x9;&#x9;  when Article_code in ('AW221NXT54','AW221NXT55') then 500 &#xA;--&#x9;&#x9;  end&#xA;--where Article_code in ('AW221NXT53','AW221NXT54','AW221NXT55','AW221NXT50','AW221NXT51','AW221NXT52')&#xA;--and warehouse_code = 'xcls1_';&#xA;&#xA;--update Dwstaging.dbo.STG_SLIM_STOCK&#xA;--set supplier_decimal='acv89', Supplier_name='Zhongshan', &#xA;--moq = case when Article_code in ('UI152FOR18','UI231FOR23','UI410FOR06','UI150FOR15','UI150FOR18','UI150FOR21') then 4&#xA;--&#x9;&#x9;&#x9;when Article_code in ('UI774FOR44','UI774FOR45','UI774FOR33','UI774FOR34','UI774FOR35','UI774FOR36','UI774FOR38',&#xA;--&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;'UP330FOR03','UP331FOR03') then 1&#xA;--&#x9;&#x9;&#x9;when Article_code in ('UI331FOR23') then 2 &#xA;--&#x9;&#x9;  end&#xA;--where Article_code in ('UI152FOR18','UI231FOR23','UI410FOR06','UI331FOR23','UI774FOR44','UI774FOR45','UI774FOR33','UI774FOR34',&#xA;--'UI774FOR35','UI774FOR36','UI774FOR38','UI150FOR15','UI150FOR18','UI150FOR21','UP330FOR03','UP331FOR03')&#xA;--and warehouse_code = 'xcls1_';&#xA;&#xA;-- ===========================================================================================&#xA;-- Update Buyers &#xA;-- ===========================================================================================&#xA;update Dwstaging.dbo.STG_Slim_STOCK&#xA;set User_Defined_6 = b.Buyers&#xA;from  Dwstaging.dbo.STG_Slim_STOCK s, STG_Slim_Buyer b&#xA;where b.companyid=case when Warehouse_code in ('m1_', 'b1_') then 'xus'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;else substring(Warehouse_code,1,3) end&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;and b.BusinessDivisionID = User_Defined_4&#xA;&#x9;and b.ManufBrandID = User_Defined_1 ;&#xA;&#xA;-- ===========================================================================================&#xA;-- Update MOQ-IOQ-EOQ_Logistic_Unit_1 cuando sea 0 lo deje vacio&#xA;-- ===========================================================================================&#xA;&#xA;update Dwstaging.dbo.STG_SLIM_STOCK&#xA;set MOQ = null &#xA;where MOQ = 0 ;&#xA;&#xA;update Dwstaging.dbo.STG_SLIM_STOCK&#xA;set IOQ = null&#xA;where IOQ = 0 ;&#xA;    &#xA;update Dwstaging.dbo.STG_SLIM_STOCK&#xA;set EOQ_Logistic_Unit_1 = null&#xA;where EOQ_Logistic_Unit_1= 0 &#xA;&#xA;-- ===========================================================================================&#xA;-- Replace para excluir a Isabel Sendra d los compradores (Buyers)&#xA;-- ===========================================================================================&#xA;update Dwstaging.dbo.STG_Slim_STOCK&#xA;set User_Defined_6 = replace(User_Defined_6,'isabel sendra,','')&#xA;update Dwstaging.dbo.STG_Slim_STOCK&#xA;set User_Defined_6 = replace(User_Defined_6,',isabel sendra','')&#xA;&#xA;&#xA;--==========================================================================================&#xA;-- Update para arreglar los datos Supplier_Decimal - Supplier_Name deUbiquiti para la localidad principal&#xA;--==========================================================================================&#xA;update Dwstaging.dbo.STG_SLIM_STOCK&#xA;set Supplier_decimal = 'B1', Supplier_Name = 'Blu Warehouse Panama', Lead_time = 35&#xA;where User_Defined_1 like 'ubq%' and Warehouse_Code = 'xmxd1_'&#xA;&#xA;&#xA;&#xA;-- ===========================================================================================&#xA;-- Update LeadTime-MOQ-IOQ-Supplier_Decimal-Supplier_Name con el nuevo Archivo que enviaron&#xA;-- stg_slim_products_update solo tiene 19 registros para probar&#xA;-- ===========================================================================================&#xA;&#xA;&#xA;update Dwstaging.dbo.STG_SLIM_STOCK &#xA;set Lead_Time = leadtime,&#xA;    MOQ = U.MOQ,&#xA;&#x9;IOQ = U.IOQ,&#xA;&#x9;Supplier_Decimal = U.Supplier_Decimal,&#xA;&#x9;Supplier_Name = U.Supplier_Name&#xA;from Dwstaging.dbo.STG_SLIM_STOCK s inner join DWStaging.dbo.stg_slim_products_update U&#xA;      on s.Article_Code = U.localsku and s.Warehouse_Code=U.warehouse_code&#xA;&#xA;&#xA;--==========================================================================================&#xA;-- Update para arreglar los datos Supplier_Decimal - Supplier_Name de xgtd1 por los de xgt1&#xA;--==========================================================================================&#xA;&#xA;update  Dwstaging.dbo.STG_SLIM_STOCK&#xA;set Supplier_Decimal = 'B1',&#xA;    Supplier_Name = 'Blu Warehouse Panama'&#xA;where Warehouse_Code = 'xgt1_'&#xA;and Supplier_Decimal ='xgt1'&#xA;&#xA;&#xA;-- ===========================================================================================&#xA;-- Update LeadTime Productos Amazon BrandID=ama localidad xmxd1_ &#xA;-- Marlon D. Claro 2020-07-21&#xA;-- ===========================================================================================&#xA;&#xA;update Dwstaging.dbo.STG_SLIM_STOCK &#xA;set Lead_Time = 91&#xA;--select Lead_Time, * from Dwstaging.dbo.STG_SLIM_STOCK&#xA;where User_Defined_1='ama'&#xA;and Warehouse_Code = 'xmxd1_'&#xA;&#xA;--=====================================================================================&#xA;-- Update para actualizar el campo criterium_3 con el valor de VolumeCarton de WMSTools &#xA;-- y actualizart el campo User_Defined_12 con el valor de VolumeCarton de WMSTools&#xA;-- dividido por el IOQ de la tabla STG_SLIM_STOCK&#xA;-- Marlon D. Claro 2020-09-08&#xA;--=====================================================================================&#xA;&#xA;--1.&#x9;Calcular el volumen por producto para b1&#xA;update Dwstaging.dbo.STG_SLIM_STOCK &#xA;set Criterium_3     = volumecarton/IOQ,&#xA;    User_Defined_12 = volumeEach&#xA;from Dwstaging.dbo.STG_SLIM_STOCK S , SQLAPP2List.wmstools.dbo.productdimensions W&#xA;where S.Article_Code = W.localsku ;&#xA;&#xA;--2.&#x9;Colocar el volumen calculado por producto en el punto 1 para todas las otras localidades &#xA;update Dwstaging.dbo.STG_SLIM_STOCK &#xA;set Criterium_3     = volumecarton/B.IOQ&#xA;--select Criterium_3    , volumecarton/B.IOQ&#xA;from Dwstaging.dbo.STG_SLIM_STOCK S  inner join SQLAPP2List.wmstools.dbo.productdimensions W on  S.Article_Code = W.localsku&#xA;inner join  (select Article_Code,ioq from Dwstaging.dbo.STG_SLIM_STOCK where Warehouse_Code = 'b1_') B on B.Article_Code = S.Article_Code&#xA;where s.Warehouse_Code &lt;&gt; 'b1_'&#xA;and S.IOQ is null ;&#xA;&#xA;--3.&#x9;En el caso que el producto no se encuentre en b1 Colocar VolumeEach(WMSTools)&#xA;&#xA;update Dwstaging.dbo.STG_SLIM_STOCK &#xA;set Criterium_3     = volumeEach,&#xA;    User_Defined_12 = volumeEach&#xA;--select &#xA;from Dwstaging.dbo.STG_SLIM_STOCK S  inner join SQLAPP2List.wmstools.dbo.productdimensions W on  S.Article_Code = W.localsku&#xA;where Criterium_3 is null ;&#xA;&#xA;&#xA;&#xA;&#xA;--==========================================================================================&#xA;-- Update para arreglar los datos Supplier_Decimal de JBL &#xA;-- Marlon D. Claro &#xA;-- 2020-11-17&#xA;--==========================================================================================&#xA;update Dwstaging.dbo.STG_SLIM_STOCK&#xA;set Supplier_decimal = case when Warehouse_Code='b1_' then 'JBL' else 'B1' end,&#xA;    Supplier_Name    = case when Warehouse_Code='b1_' then 'JBL' else 'Blu Warehouse Panama' end&#xA;where User_Defined_1 like 'jbl%' &#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

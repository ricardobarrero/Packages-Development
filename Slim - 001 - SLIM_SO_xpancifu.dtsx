                <DTS:Executable
                  DTS:refId="Package\Load Slim\Stock\File Stock - Supplier\Update Stock_On_Hand"
                  DTS:CreationName="Microsoft.ExecuteSQLTask"
                  DTS:Description="Cuando el valor es cero se debe dejar el cero."
                  DTS:DTSID="{AB80E0B1-6122-4C82-B05C-618C192EB040}"
                  DTS:ExecutableType="Microsoft.ExecuteSQLTask"
                  DTS:LocaleID="-1"
                  DTS:ObjectName="Update Stock_On_Hand"
                  DTS:ThreadHint="5">
                  <DTS:Variables />
                  <DTS:ObjectData>
                    <SQLTask:SqlTaskData
                      SQLTask:Connection="{AC9E372E-B325-462E-A235-192142687C02}"
                      SQLTask:SqlStatementSource="--================================================================&#xA;--Update Stock_On_Hand cuando esta en 0 deja 0 cuando &lt;&gt; 0 queda Null &#xA;--Marlon D. Claro &#xA;--Date: 2019-05-31&#xA;--=================================================================&#xA;update STG_SLIM_STOCK&#xA;set stock_on_hand  = case when stock_on_hand &lt;&gt; 0 then null else stock_on_hand end" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
                  </DTS:ObjectData>

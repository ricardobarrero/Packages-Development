--select *
from Dwstaging.dbo.STG_Confirmed_Order o 
left join SQLAPP2List.skucentral.dbo.Products p with(nolock)
      on (o.Article_code=p.localsku)
left join intxdw.dbo.dim_product dp with(nolock) on (dp.companyid = case when o.warehouse_code like 'm1%' then 'XUS' 
                                                                                        when o.warehouse_code like 'b1%' then 'XUS'
                                                                                        when o.warehouse_code like 'f1%' then 'XUS'
                                                                                        else substring(Warehouse_code,1,3) end and dp.localsku = o.Article_code)
where (p.BusinessDivisionID='Exclusive' or p.brandid = 'ubq') and dp.LocalLineid not in ('nptx')
and Len(p.LocalSKU) &lt;= 10
and o.Line_number &lt; 9900
and p.TypeID not in ('m')

union

select Record_Type
,Year
,Month
,DATEPART(ISO_WEEK, getdate()) as week
,'global_'as Warehouse_Code
,Article_Code
,'global_' + Confirmed_Order_Number as Confirmed_Order_Number
,convert(varchar,Confirmed_Order_Required_Date,112) as Confirmed_Order_Required_Date
,Confirmed_Order_Required_Quantity
,Exception_Level
,Type_confirmed_order
,Customer_Details
,Production_Details
,Confirmed_Order_Comments
,Original_Quantity
,Supplied_Quantity
,Confirmed_Order_Free_Text_1
,Confirmed_Order_Free_Text_2
,Confirmed_Order_Free_Number_1
,Confirmed_Order_Free_Number_2
,Order_type_number_CO
,Line_Number
--select *
from Dwstaging.dbo.STG_Confirmed_Order o 
left join SQLAPP2List.skucentral.dbo.Products p with(nolock)
      on (o.Article_code=p.localsku)
left join intxdw.dbo.dim_product dp with(nolock) on (dp.companyid = case when o.warehouse_code like 'm1%' then 'XUS' 
                                                                                        when o.warehouse_code like 'b1%' then 'XUS'
                                                                                        else substring(Warehouse_code,1,3) end and dp.localsku = o.Article_code)
left join intxdw.dbo.dim_customer cu with(nolock) on (cu.customerid_sentai =substring(o.Customer_Details,1,charindex('_',o.Customer_Details)-1))
where (p.BusinessDivisionID='Exclusive' or p.brandid = 'ubq') -- Solo productos Exclusivos
and p.TypeID not in ('m')			   -- Excluye productos de tipo promocion o Marketing
and dp.LocalLineid not in ('nptx')	   -- Excluye facturas de tipo NPTX
and Len(p.LocalSKU) &lt;= 10			   -- Excluye productos de tipo RedTag (-b1,-b2,-r1 etc)
and o.Line_number &lt; 9900			   -- Excluye lines de factura &gt;= 9900 son reservadas para productos especiales
and cu.businessunitID not in ('a')	   -- Excluye las transferencias a afiliadas


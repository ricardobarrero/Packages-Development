                  <DTS:ObjectData>
                    <SQLTask:SqlTaskData
                      SQLTask:Connection="{AC9E372E-B325-462E-A235-192142687C02}"
                      SQLTask:SqlStatementSource="-- ===========================================================================================&#xA;-- Paso 4. Inserta a la tabla los proveedores de Asia definidos para b1 en Sku central, pero&#xA;-- para las localidades principales de cada pais&#xA;-- ===========================================================================================&#xA;INSERT INTO [STG_SLIM_SUPPLIER]&#xA;             (Warehouse_code, &#xA;             Article_Code, &#xA;             Supplier_Number, &#xA;             Supplier_Name, &#xA;             Primary_Supplier, &#xA;             Lead_Time, &#xA;             MOQ, &#xA;             IOQ)&#xA;SELECT &#xA;             s.Warehouse_code, &#xA;             s.Article_Code, &#xA;             V.SourceVmiSupplierID, &#xA;             substring(V.SourceVMISuppDesc,1,60), &#xA;             0, &#xA;             V.leadTime*7, &#xA;             v.MinimumOrderQuantity, &#xA;             v.OrderMultiple&#xA;FROM   &#xA;             SQLAPP2List.Skucentral.dbo.ProductLogisticsView l WITH(NOLOCK),&#xA;             (select distinct warehouse_code, Article_Code from STG_SLIM_STOCK WITH(NOLOCK)) s,&#xA;             SQLAPP2List.skucentral.dbo.Products p WITH(NOLOCK),&#xA;             SQLAPP2List.Skucentral.dbo.ProductLogisticsView V WITH(NOLOCK)&#xA;WHERE&#xA;             l.DestinationLocationId                 = ltrim(rtrim(left(s.Warehouse_code, charindex('_', s.Warehouse_code)-1)))&#xA;             and l.localsku                                       = s.Article_Code&#xA;             and l.DestinationLocationId       in ('amxd1','xbol1','xcb1','xcle1','xcr1','xecg1','xgt1','xhn1','xiq1','xjm1','xni1','xpa1', 'xpel1', 'xptm1','xsv1', 'xuy1')&#xA;             and V.destinationlocationId       = 'b1'&#xA;             and v.localsku                     = s.Article_Code&#xA;             and p.localsku                     = s.Article_Code&#xA;             and (p.BusinessDivisionID               = 'Exclusive' or p.brandid = 'ubq')" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
                  </DTS:ObjectData>

                <DTS:Executable
                  DTS:refId="Package\Load Slim\Stock\File Stock - Supplier\UPDATE FIX UBIQUITI"
                  DTS:CreationName="Microsoft.ExecuteSQLTask"
                  DTS:Description="Update para arreglar los datos Supplier_Decimal - Supplier_Name de Ubiquiti "
                  DTS:DTSID="{AC9E983F-7030-41FC-92C1-EE57F433A271}"
                  DTS:ExecutableType="Microsoft.ExecuteSQLTask"
                  DTS:LocaleID="-1"
                  DTS:ObjectName="UPDATE FIX UBIQUITI"
                  DTS:ThreadHint="4">
                  <DTS:Variables />
                  <DTS:ObjectData>
                    <SQLTask:SqlTaskData
                      SQLTask:Connection="{AC9E372E-B325-462E-A235-192142687C02}"
                      SQLTask:SqlStatementSource="-- ===========================================================================================&#xA;-- Paso 5. Update para arreglar los datos Supplier_Decimal - Supplier_Name de Ubiquiti &#xA;-- Solo para la localidad principal XMXD1 y Brand UBIQUITI &#xA;-- Definir si se debe actualizar MOQ??? &amp; IOQ????&#xA;-- ===========================================================================================&#xA;UPDATE Dwstaging.dbo.STG_SLIM_SUPPLIER&#xA;SET          Supplier_number = 'B1', &#xA;             Supplier_Name = 'Blu Warehouse Panama', &#xA;             Lead_time = 35&#xA;FROM   Dwstaging.dbo.STG_SLIM_SUPPLIER o &#xA;             inner join SQLAPP2List.skucentral.dbo.Products p with(nolock) on (o.Article_code=p.localsku)&#xA;WHERE  (p.brandid = 'ubq') &#xA;and          Warehouse_Code = 'xmxd1_'&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
                  </DTS:ObjectData>

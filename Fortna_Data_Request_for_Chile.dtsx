             ,NULL as FREIGHTCHARGE
             ,NULL as BULPCKNO
             ,NULL as BULPCKUOM
             ,'1' as DOOR
             ,NULL as MODE
from FACT_TRANSFER_ORDER P
where companyid = 'xcl' and TO_xferLoc = 'xclf1' and p.TO_TransDate &gt;= '2017-01-01'
and p.TR_transdate &lt;&gt; '1980-01-01'  
and TR_shipqty&lt;&gt;0

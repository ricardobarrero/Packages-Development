          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{D0983F33-F9E2-4AE3-B311-D117FEE8112C}"
              SQLTask:SqlStatementSource="-- =============================================================================================&#xA;-- Inserta la información de stock y medidas a la tabla Frotna_ItemMaster&#xA;-- de los productos que hayan tenido venta y movimiento de inventario desde el 2017 a la fecha&#xA;-- =============================================================================================&#xA;/* Borra la tabla Fortna_ItemMaster */&#xA;DELETE FROM dwstaging.dbo.Fortna_ItemMaster;&#xA;&#xA;/* Inserta a la tabla Fortna_ItemMaster los productos que han tenido venta*/&#xA;INSERT INTO dwstaging.dbo.Fortna_ItemMaster&#xA;SELECT  DISTINCT  upper(P.LocalSKU) as PRODUCT_ID,&#xA;P.ScCategoryName_L1 AS CATEGORY, &#xA;REPLACE (P.ScCategoryName_L2,'&quot;','') as SUBCATEGORY,&#xA;REPLACE (P.DescriptionLocal,'&quot;','') as DESCRIPTION,&#xA;'ea' as UNIOFMESURE,&#xA;PD.LengthEach*2.54  as SKULENGTH,       -- convertir de in a cms&#xA;PD.WidthEach*2.54 as SKUWIDTH,                 -- convertir de in a cms&#xA;PD.HeightEach*2.54 as SKUHEIGHT,        -- convertir de in a cms&#xA;PD.WeightEach/2.2046226218 as SKUWEIGHT, -- convertir de lbs a kg&#xA;(select sum(isnull(oh,0)) from fact_inventory fi with(nolock) where fi.companyid='XSV' and fi.localsku=S.localsku and dw_status &lt;&gt;'d') as QUNTITYONHAND,&#xA;0 AS INNERQUANTITY,0 AS INNERLENGTH, 0 AS INNERWIDTH, 0 AS INNERHEIGHT, 0 AS INNERWEIGHT,&#xA;NULL as CARTONQUANTITY, &#xA;PD.LengthCarton*2.54 as CARTONLENGTH,          -- convertir de in a cms&#xA;PD.WidthCarton*2.54 as CARTONWIDTH,                   -- convertir de in a cms&#xA;PD.HeightCarton*2.54 as CARTONHEIGHT,          -- convertir de in a cms&#xA;PD.WeightCarton/2.2046226218 as CARTONWEIGHT, -- convertir de lbs a kg&#xA;NULL as CARTONSONPALLET,&#xA;P.BusinessDivisionName as BUSINESSDIVISIONNAME,&#xA;NULL as PALLETQUANTITY,&#xA;NULL as BUSINESSCATEGORY&#xA;FROM FACT_PRODUCT_SALE S WITH(NOLOCK) INNER JOIN DIM_PRODUCT P WITH(NOLOCK) ON (S.companyID=P.CompanyID AND S.localsku=P.localsku and p.dw_status &lt;&gt; 'd')&#xA;LEFT JOIN DWStaging.dbo.STG_ProductDimensions PD ON (S.localsku = PD.localSKU)&#xA;WHERE S.CompanyId='XSV' and lineID &lt;&gt; 'NPTX' AND Locationid in ('xsv1')&#xA;AND S.DIM_PK_TIME &gt;= 20170101  &#xA;AND S.TRANSID='soin' &#xA;AND P.DescriptionLocal not like 'Microsoft ESD%'&#xA;order by 1;  --8393&#xA;&#xA;/* Inserta a la tabla Fortna_ItemMaster los productos que han tenido movimiento de inventario en el snapshot desde el 2017*/&#xA;INSERT INTO dwstaging.dbo.Fortna_ItemMaster&#xA;SELECT  DISTINCT  upper(P.LocalSKU) as PRODUCT_ID,&#xA;P.ScCategoryName_L1 AS CATEGORY, &#xA;REPLACE (P.ScCategoryName_L2,'&quot;','') as SUBCATEGORY,&#xA;REPLACE (P.DescriptionLocal,'&quot;','') as DESCRIPTION,&#xA;'ea' as UNIOFMESURE,&#xA;PD.LengthEach*2.54 as SKULENGTH,        -- convertir de in a cms&#xA;PD.WidthEach*2.54 as SKUWIDTH,                 -- convertir de in a cms&#xA;PD.HeightEach*2.54 as SKUHEIGHT,        -- convertir de in a cms&#xA;PD.WeightEach/2.2046226218 as SKUWEIGHT, -- convertir de lbs a kg&#xA;(select sum(isnull(oh,0)) from fact_inventory fi with(nolock) where fi.companyid='XSV' and fi.localsku=S.Product_ID and dw_status &lt;&gt;'d') as QUNTITYONHAND,&#xA;0 AS INNERQUANTITY,0 AS INNERLENGTH, 0 AS INNERWIDTH, 0 AS INNERHEIGHT, 0 AS INNERWEIGHT,&#xA;NULL as CARTONQUANTITY,&#xA;PD.LengthCarton*2.54 as CARTONLENGTH,          -- convertir de in a cms&#xA;PD.WidthCarton*2.54 as CARTONWIDTH,                   -- convertir de in a cms&#xA;PD.HeightCarton*2.54 as CARTONHEIGHT,          -- convertir de in a cms&#xA;PD.WeightCarton/2.2046226218 as CARTONWEIGHT, -- convertir de lbs a kg&#xA;NULL as CARTONSONPALLET,&#xA;P.BusinessDivisionName as BUSINESSDIVISIONNAME,&#xA;NULL as PALLETQUANTITY,&#xA;NULL as BUSINESSCATEGORY&#xA;FROM DWSTAGING.dbo.stg_fortna_inventory S WITH(NOLOCK) INNER JOIN DIM_PRODUCT P WITH(NOLOCK) ON (P.CompanyID = 'XSV' AND S.Product_ID=P.localsku and p.dw_status &lt;&gt; 'd')&#xA;LEFT JOIN DWStaging.dbo.STG_ProductDimensions PD ON (S.Product_ID = PD.localSKU)&#xA;WHERE NOT EXISTS (select 'e' from dwstaging.dbo.Fortna_ItemMaster i where i.PRODUCT_ID = s.Product_ID)&#xA;AND P.DescriptionLocal not like 'Microsoft ESD%'&#xA;order by 1; --1172&#xA;&#xA;&#xA;-- =============================================================&#xA;-- =============================================================&#xA;-- Actualiza Eaches (EP) de la tabla Cubiscan para XUS&#xA;-- de los que quedaron en cero&#xA;-- =============================================================&#xA;update dwstaging.dbo.Fortna_ItemMaster&#xA;set SKULENGTH = EPLength*2.54 -- convertir de in a cms &#xA;from dwstaging.dbo.Fortna_ItemMaster I, dwstaging.dbo.cubiscan c&#xA;where I.PRODUCT_ID = C.ProductID and C.companyid='XUS'&#xA;and (I.SKULENGTH is null or I.SKULENGTH =0)&#xA;&#xA;update dwstaging.dbo.Fortna_ItemMaster&#xA;set SKUWIDTH = EPWidth*2.54 -- convertir de in a cms &#xA;from dwstaging.dbo.Fortna_ItemMaster I, dwstaging.dbo.cubiscan c&#xA;where I.PRODUCT_ID = C.ProductID and C.companyid='XUS'&#xA;and (I.SKUWIDTH is null or I.SKUWIDTH =0)&#xA;&#xA;update dwstaging.dbo.Fortna_ItemMaster&#xA;set SKUHEIGHT = EPHEight*2.54 -- convertir de in a cms &#xA;from dwstaging.dbo.Fortna_ItemMaster I, dwstaging.dbo.cubiscan c&#xA;where I.PRODUCT_ID = C.ProductID and C.companyid='XUS'&#xA;and (I.SKUHEIGHT is null or I.SKUHEIGHT =0)&#xA;&#xA;update dwstaging.dbo.Fortna_ItemMaster&#xA;set SKUWEIGHT = EPWeight/2.2046226218 -- convertir de lbs a kg &#xA;from dwstaging.dbo.Fortna_ItemMaster I, dwstaging.dbo.cubiscan c&#xA;where I.PRODUCT_ID = C.ProductID and C.companyid='XUS'&#xA;and (I.SKUWEIGHT is null or I.SKUWEIGHT =0)&#xA;-- =============================================================&#xA;&#xA;-- =============================================================&#xA;-- Actualiza Cajas (MP) de la tabla Cubiscan para XUS&#xA;-- de los que quedaron en cero&#xA;-- =============================================================&#xA;update dwstaging.dbo.Fortna_ItemMaster&#xA;set CARTONLENGTH = MPLength*2.54&#xA;from dwstaging.dbo.Fortna_ItemMaster I, dwstaging.dbo.cubiscan c&#xA;where I.PRODUCT_ID = C.ProductID and C.companyid='XUS'&#xA;and (I.CARTONLENGTH is null or I.CARTONLENGTH =0)&#xA;&#xA;update dwstaging.dbo.Fortna_ItemMaster&#xA;set CARTONWIDTH = MPWidth*2.54&#xA;from dwstaging.dbo.Fortna_ItemMaster I, dwstaging.dbo.cubiscan c&#xA;where I.PRODUCT_ID = C.ProductID and C.companyid='XUS'&#xA;and (I.CARTONWIDTH is null or I.CARTONWIDTH =0)&#xA;&#xA;update dwstaging.dbo.Fortna_ItemMaster&#xA;set CARTONHEIGHT = MPHEight*2.54&#xA;from dwstaging.dbo.Fortna_ItemMaster I, dwstaging.dbo.cubiscan c&#xA;where I.PRODUCT_ID = C.ProductID and C.companyid='XUS'&#xA;and (I.CARTONHEIGHT is null or I.CARTONHEIGHT =0)&#xA;&#xA;update dwstaging.dbo.Fortna_ItemMaster&#xA;set CARTONWEIGHT = MPWeight/2.2046226218&#xA;from dwstaging.dbo.Fortna_ItemMaster I, dwstaging.dbo.cubiscan c&#xA;where I.PRODUCT_ID = C.ProductID and C.companyid='XUS'&#xA;and (I.CARTONWEIGHT is null or I.CARTONWEIGHT =0)&#xA;-- =======================================================================&#xA;&#xA;-- =============================================================&#xA;-- Actualiza CARTONQUANTITY &#xA;-- WMSTools&#xA;-- =============================================================&#xA;&#xA;-- ======================================================================&#xA;&#xA;update  dwstaging.dbo.Fortna_ItemMaster&#xA;Set CARTONQUANTITY =s.cartonQuantity, CARTONSONPALLET = s.PalletQuantity&#xA;from dwstaging.dbo.Fortna_ItemMaster I,  dwstaging.dbo.STG_ProductSetup s&#xA;where I.PRODUCT_ID = s.localsku&#xA;and s.environmentId = 'XUS'&#xA;-- ======================================================================&#xA;&#xA;-- =============================================================&#xA;-- Actualiza PALLETQUANTITY &#xA;-- XCL WMSTools&#xA;-- =============================================================&#xA;&#xA;&#xA;&#xA;-- =============================================================&#xA;-- Actualiza PALLETQUANTITY &#xA;-- XUS WMSTools&#xA;-- =============================================================&#xA;&#xA;&#xA;update  dwstaging.dbo.Fortna_ItemMaster&#xA;Set dwstaging.dbo.Fortna_ItemMaster.PALLETQUANTITY = s.PalletQuantity&#xA;from (SELECT PalletQuantity,localsku,environmentId FROM  dwstaging.dbo.STG_ProductSetup) s&#xA;where dwstaging.dbo.Fortna_ItemMaster.PRODUCT_ID = s.localsku&#xA;and s.environmentId = 'XUS'&#xA;-- =============================================================&#xA;&#xA;&#xA;-- =============================================================&#xA;-- Actualiza CARTONQUANTITY que quedan null o en 0 &#xA;-- de la tabla de P2 STG_WM_Prod_Unit XCL&#xA;-- =============================================================&#xA;&#xA;&#xA;-- =======================================================================&#xA;&#xA;&#xA;-- =============================================================&#xA;-- Actualiza CARTONQUANTITY que quedan null o en 0 &#xA;-- de la tabla de P2 STG_WM_Prod_Unit XUS&#xA;-- =============================================================&#xA;&#xA;update F&#xA;set F.CARTONQUANTITY=convert (int, substring(W.UNIT_ID,2,4) )&#xA;--SELECT F.CARTONQUANTITY,W.UNIT_ID , convert (int, substring(W.UNIT_ID,2,4) )&#xA;from   dwstaging.dbo.STG_WM_Prod_Unit W INNER JOIN intxdw.dbo.dim_product D ON W.PRODUCT_ID=D.LOCALRECNO and D.companyID='XUS' and D.dw_status &lt;&gt;'D'&#xA;      INNER JOIN  dwstaging.dbo.fortna_itemmaster F ON F.PRODUCT_ID = D.LOCALSKU&#xA;where W.unit_id like 'C%[0-9]%' and W.unit_ID not like '%k%' AND (F.CARTONQUANTITY =0 or F.CARTONQUANTITY is null) &#xA;and W.LocationID='m1'&#xA;&#xA;-- =======================================================================&#xA;&#xA;&#xA;-- =============================================================&#xA;-- Actualiza PALLETQUANTITY que quedan null o en 0 &#xA;-- de la tabla de P2 STG_WM_Prod_Unit XCL&#xA;-- =============================================================&#xA;&#xA;&#xA;-- =======================================================================&#xA;&#xA;-- =============================================================&#xA;-- Actualiza PALLETQUANTITY que quedan null o en 0 &#xA;-- de la tabla de P2 STG_WM_Prod_Unit XUS&#xA;-- =============================================================&#xA;&#xA;update F&#xA;set F.PALLETQUANTITY=convert (int, substring(W.UNIT_ID,2,4) )&#xA;--SELECT F.PALLETQUANTITY,W.UNIT_ID , convert (int, substring(W.UNIT_ID,2,4) )&#xA;from   dwstaging.dbo.STG_WM_Prod_Unit W INNER JOIN intxdw.dbo.dim_product D ON W.PRODUCT_ID=D.LOCALRECNO and D.companyID='XUS' and D.dw_status &lt;&gt;'D'&#xA;      INNER JOIN  dwstaging.dbo.fortna_itemmaster F ON F.PRODUCT_ID = D.LOCALSKU&#xA;where W.unit_id like 'P%[0-9]%' AND (F.PALLETQUANTITY =0 or F.PALLETQUANTITY is null)&#xA;      and  W.locationID ='m1'&#xA;-- =======================================================================&#xA;&#xA;&#xA;&#xA;-- =============================================================&#xA;-- Actualiza BUSINESSCATEGORY  &#xA;-- de la tabla dbo.DIM_BUSINESS_DIVISION&#xA;-- =============================================================&#xA;&#xA;      UPDATE F&#xA;      SET F.BUSINESSCATEGORY=BD.BUSINESSCATEGORY&#xA;      from dwstaging.dbo.Fortna_ItemMaster F inner join dim_product P on F.PRODUCT_ID=P.localSKU &#xA;               inner join  dbo.DIM_BUSINESS_DIVISION BD ON BD.DIM_PK_BUSINESS_DIVISION=P.DIM_PK_BUSINESS_DIVISION" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>

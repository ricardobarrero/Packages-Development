                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT OrderNo as ORDERNUMBER,
P.LocalSKU as SKU,
REPLACE (P.DescriptionLocal,'"','') as DESCRIPTION,
ShipQty as QUANTITY,
UnitPriceUS as SELLPRICE,
(select max(wave_id) from fact_so_pick PI with(nolock) where PI.CompanyID = 'XGT' and  SOTranscode='or' and SOTransNo = S.OrderNo) as WAVE,
S.CarrierID as CARRIER,
C.Description as VASFLAG,
P.ManufSKU as MPN
FROM FACT_PRODUCT_SALE S WITH(NOLOCK) INNER JOIN DIM_PRODUCT P ON (S.companyID=P.CompanyID AND S.localsku=P.localsku  and p.dw_status &lt;&gt; 'd')
INNER JOIN DIM_CARRIER C ON (s.carrierID = C.CarrierID)
INNER JOIN DIM_CUSTOMER D ON (S.companyID=D.CompanyID AND S.customerID=D.CustomerID)
WHERE S.CompanyId='XGT' and lineID &lt;&gt; 'NPTX' and S.BusinessUnitID &lt;&gt; 'm' AND Locationid in ('xgt1','xgtd1')
and so_transid &lt;&gt; 'esd'
AND S.DIM_PK_TIME  between  20170801 and convert (int,convert(varchar(11),getdate()-1,112))  --- &gt;= 20170101 --BETWEEN 20160222 AND 20161231
AND S.TRANSID='soin' --225,814</property>

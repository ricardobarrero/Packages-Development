                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/*FILE ORDERHEADER */
SELECT DISTINCT OrderNo as ORDERNUMBER, 
      (select max(Trans_date) from fact_SalesOrders SO where So.CompanyID = 'XGT' and so.Trans_code='or' and Trans_no = S.OrderNo) AS RECEIVED,
      (select max(Transdate) from fact_so_pick PI where PI.CompanyID = 'XGT' and  SOTranscode='or' and SOTransNo = S.OrderNo) as PICKED,
      Transdate as SHIPPED,
      C.BusinessUnitID as CHANNEL,
      S.CarrierID as ORDERTYPE,
      C.CustomerID_Sentai as CUSTNO,
      S.AddressNo as CSHIPNO,
      '' as OUTBOINDCTNS,
      C.State as ZIP,
      S.LocationID as WHSE,
      '' as SHIPMENTNO,
      '' as BACKORDER,
      S.CarrierID as CARRIER,
      '' as USER_DEFINED,
         S.businessUnitID as BUSINESS_UNIT
FROM FACT_PRODUCT_SALE S WITH(NOLOCK) INNER JOIN DIM_CUSTOMER C ON (S.companyID=C.CompanyID AND S.customerID=C.CustomerID)
WHERE S.CompanyId='XGT' and lineID &lt;&gt; 'NPTX' and S.BusinessUnitID &lt;&gt; 'm' AND Locationid in ('xgt1','xgtd1')
and so_transID &lt;&gt; 'esd'
AND S.DIM_PK_TIME  between  20170801 and convert (int,convert(varchar(11),getdate()-1,112))  --- &gt;= 20170101 --BETWEEN 20160222 AND 20161217
AND S.TRANSID='soin'
Order by 4</property>

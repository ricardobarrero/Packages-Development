                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/*FILE ORDERDETAILS */
SELECT OrderNo as ORDERNUMBER,
P.LocalSKU as SKU,
REPLACE (P.DescriptionLocal,'"','') as DESCRIPTION,
ShipQty as QUANTITY,
UnitPriceUS as SELLPRICE,
(select max(wave_id) from fact_so_pick PI with(nolock) where PI.CompanyID = 'XCL' and  SOTranscode='or' and SOTransNo = S.OrderNo) as WAVE,
S.CarrierID as CARRIER,
case   when s.carrierid ='clu5' then 'Despacho Retail IT'
       when s.carrierid ='3pl' then 'Despacho Retail Mobile' 
       when s.carrierid in ('cll3','cll4') then 'Despacho STGO 24hrs'
       when s.carrierid in ('clo1') then 'Despacho STGO AM/PM'
       when s.carrierid in ('242s') then 'Despacho STGO'
       when s.carrierid ='cle0' then 'In'
       when s.carrierid in ('pick','clt1') then 'Retira Cliente'
       when s.carrierid in ('clo3','clcr','cld0','cld1','cld2','cld3','cld4','cld5','cld6','cld7','cld8','cld9','cldg','clds','cle1','cle2','cle3','cle4','cle5','cle6','cle7','cle8','cle9','clf0','clf1','clf2','clf3',
'clf4','clf5','clf6','clf7','clf8','clf9','clfu','clg0','clg1','clg2','clg3','clg4','clg5','clg6','clg7','clg8','clg9','clh0','clh1','clh2','clh3','clh4','clh5','clh6','clh7','clh8','clh9',
'cli0','cli1','cli2','cli3','cli4','cli5','cli6','cli7','cli8','cli9','clj0','clj1','clj2','clj3','clj4','clj5','clj6','clj7','clj8','clj9','clk0','clk1','clk2','clk3','clk4','clk5','clk6',
'clk7','clk8','clk9','cll0','cll1','cll2','cll5','cll6','cll7','cll8','cll9','clm0','clm1','clm2','clm3','clm4','clm5','clm6','clm7','clm8','clm9','cln1','cln2','cln3','cln4','cln5','cln6',
'cln7','cln8','cln9','clo2','clp1','clp2','clp3','clp4','clp5','clp6','clp7','cls0','cls1','cls2','cls3','cls4','clsa','clsb','clsc','clt0','clt2','clt3','clt4','clt5','clt6','clt7','clt8',
'clt9','clu1','clu2','clu3','clu4','clu6','clu7','clu8','clu9','clv0','clv1','clv2','clv3',
'cla3','cla7','clc4','clcp','cla6','cla8','clrc','clc9','clb8','clb9','cla1','cla4','clc8','clct','clcx','cla9','clb1','clc5','clb0','clc6','cldt','clcl','clcy','clb3','clb7','clcn','cldr',
'clb4','cleh','cldz','clb5','cla2','claa','clcw','clef','clc3') then 'Despacho Provincias' else '' end as VASFLAG,
P.ManufSKU as MPN
FROM FACT_PRODUCT_SALE S WITH(NOLOCK) INNER JOIN DIM_PRODUCT P ON (S.companyID=P.CompanyID AND S.localsku=P.localsku  and p.dw_status &lt;&gt; 'd')
WHERE S.CompanyId='XCL' and lineID &lt;&gt; 'NPTX' and S.BusinessUnitID &lt;&gt; 'm' AND Locationid in ('xcls1')
and so_transid &lt;&gt; 'esd'
AND S.DIM_PK_TIME  between  20170101 and convert (int,convert(varchar(11),getdate()-1,112))  --- &gt;= 20170101 --BETWEEN 20160222 AND 20161231
AND S.TRANSID='soin'</property>

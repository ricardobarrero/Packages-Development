                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/*INVENTORY DESDE SYBASE IQ */
SELECT I.DIM_PK_TIME AS PERIOD, I.LOCALSKU AS SKU, SUM(I.OH) AS QUNTIYONHAND, I.LOCATIONID AS WHSE,
A.ABC_T_Company as ABC,
P.ScCategoryName_L1 AS CATEGORY, P.ScCategoryName_L2 SUBCATEGORY
FROM FACT_WS_INVENTORY I INNER JOIN DIM_PRODUCT P ON (I.COMPANYID=P.COMPANYID AND I.LOCALSKU=P.LOCALSKU)
LEFT JOIN FACT_ABC_WS_PRODUCT A ON (I.COMPANYID=A.COMPANYID AND I.DIM_PK_TIME=A.DIM_PK_TIME AND I.LOCALSKU=A.LOCALSKU AND i.LOCATIONID=A.LOCATIONID)
WHERE I.COMPANYID='XGT' AND I.DIM_PK_TIME &gt;= 20170801 AND I.Locationid in ('xgt1','xgtD1')
AND I.OH &gt; 0
GROUP BY I.DIM_PK_TIME, I.LOCALSKU, I.LOCATIONID, A.ABC_T_Company,P.ScCategoryName_L1, P.ScCategoryName_L2
ORDER BY 1,2</property>

                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/* INBOUNDRECEIPTS SAN FRANCISCO*/
select       
              p.localsku as SKU
             ,p.TR_TransDate as ARRIVALDATETIME
             ,p.TR_ShipQty as QUANTITY
             ,'CD Quilicura' as VENDORID
             ,'Chile' as SOURCEPOINT

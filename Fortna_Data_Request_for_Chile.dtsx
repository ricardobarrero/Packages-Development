                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/*RTETURNS*/
SELECT localSKU AS SKU
             ,transDate as RECEIVED
             ,ShipQty as QUANTITY
             ,TransNo as RETURNNUMBER
             ,C.customerID_SENTAI as CUSTNUMBER
             ,C.customerdesc as CUSTNAME
             ,S.locationID as WHSE
             ,'Regreso a Inventario' AS DISPOSITION
             ,S.OrderQty as QTYEXPECT
             ,NULL as EXPDATE
             ,NULL as SHIPMENTNO
             ,NULL as CARRIER
             ,0 as FREIGHTCHARGE
             ,R.Description as RMADESCRIPTION
FROM FACT_PRODUCT_SALE S LEFT JOIN DIM_CUSTOMER C ON (S.companyID = C.companyid AND s.customerID = c.customerid)
LEFT JOIN DIM_RMA_REASON R ON (S.RMA_ID = R.RMA_ID)
WHERE S.companyID = 'xcl' AND S.locationID ='xclf1' and S.transDate &gt;= '2017-01-01' and transid = 'socr'
ORDER BY 4</property>

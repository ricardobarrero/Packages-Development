             ,s.SupplierGrouptype as INBPUNDTYPE
             ,p.UnitID as UOM
             ,p.PO_OrderQty as QTYEXPECT
             ,p.promise_date_OR as EXPDATE
             ,'' as DEPDATE
             ,PONumber as PONUMBER

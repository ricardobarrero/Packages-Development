                        <property
                          dataType="System.String"
                          description="The SQL command to be executed."
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/*LOCATIONMASTER*/
select sl.FULL_ADDRESS as LOCATION
       ,sl.int_loc_class as DESCRIPTION
       ,case when sl.loc_type = 1 then 'Primary' 
              when sl.loc_type = 2 then 'Secondary'
             when sl.loc_type = 3 then 'Mixed'
             when sl.loc_type = 4 then 'Putaway'
             when sl.loc_type = 5 then 'X-Doc'
             when sl.loc_type = 6 then 'Reserved'
             when sl.loc_type = 7 then 'Shipping'
             when sl.loc_type = 8 then 'Secondary'
             when sl.loc_type = 99 then 'Disabled' end as AREA
       ,upper(s.prod_synonym) as SKU
       ,s.quantity_ea as SKUQTY
       ,NULL as LOCHEIGHT
       ,NULL as LOCWIDTH
       ,NULL as LOCLENGHT
       ,case when sl.LocationID = 'xcls1' then 'Quilicura' 
               when sl.LocationID = 'xclf1' then 'San Francisco' end as WHSE
       ,sl.loc_desc as LOCDESC
       ,case when p.prod_type = 1 then 'Normal' 
             when p.prod_type=2 then 'Serialized' end AS PRODTYPE
from stg_wm_storage_loc sl
inner join stg_vw_storage s on (sl.FULL_ADDRESS = s.full_address )
left join stg_wm_product p on (s.product_id = p.product_id) 
where sl.LocationID = 'xcls1'
union
select sl.FULL_ADDRESS as LOCATION
       ,sl.int_loc_class as DESCRIPTION
       ,case when sl.loc_type = 1 then 'Primary' 
              when sl.loc_type = 2 then 'Secondary'
             when sl.loc_type = 3 then 'Mixed'
             when sl.loc_type = 4 then 'Putaway'
             when sl.loc_type = 5 then 'X-Doc'
             when sl.loc_type = 6 then 'Reserved'
             when sl.loc_type = 7 then 'Shipping'
             when sl.loc_type = 8 then 'Secondary'
             when sl.loc_type = 99 then 'Disabled' end as AREA
       ,upper(p.prod_synonym) as SKU
       ,0 as SKUQTY
       ,NULL as LOCHEIGHT
       ,NULL as LOCWIDTH
       ,NULL as LOCLENGHT
       ,case when sl.LocationID = 'xcls1' then 'Quilicura' 
               when sl.LocationID = 'xclf1' then 'San Francisco' end as WHSE
       ,sl.loc_desc as LOCDESC
       ,NULL as PRODTYPE
from stg_wm_storage_loc sl
inner join stg_wm_product_syn p on (sl.product_id = p.product_id and p.wm_syn_type_id = 'main' )
where sl.loc_type = 1 and  not exists (select 'e' from stg_vw_storage s where s.full_address = sl.full_address)
and sl.LocationID = 'xcls1'</property>

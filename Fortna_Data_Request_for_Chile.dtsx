             ,p.carrierID as CARRIER
             ,ExtCostUS as FREIGHTCHARGE
             ,'' as BULPCKNO
             ,'' as BULPCKUOM
             ,'' as DOOR
             ,'' as MODE
from FACT_PURCHASE P
left join DIM_SUPPLIER S ON (P.companyid = s.companyid and P.supplierID = s.SupplierID)
where p.companyid = 'xcl' and p.transdate &gt;= '2017-01-01'
order by 2

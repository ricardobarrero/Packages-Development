                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/* INBOUNDRECEIPTS */
select       
              p.localsku as SKU
             ,p.transdate as ARRIVALDATETIME
             ,p.ReceiveQty as QUANTITY
             ,p.supplierID as VENDORID
             ,c.countrydesc as SOURCEPOINT
             ,P.transno as SHIPMENTNUMBER
             ,case when p.supplierID like 'XGTN%' then 'LocalSupplier' else 'ForeignSupplier' end  as INBPUNDTYPE
             ,p.UnitID as UOM
             ,p.PO_OrderQty as QTYEXPECT
             ,p.promise_date_OR as EXPDATE
             ,'' as DEPDATE
             ,PONumber as PONUMBER
             ,locationID as WHSE
             ,p.carrierID as CARRIER
             ,NULL as FREIGHTCHARGE
             ,NULL as BULPCKNO
             ,NULL as BULPCKUOM
             ,'1' as DOOR
             ,NULL as MODE
from FACT_PURCHASE P
left join DIM_SUPPLIER S ON (P.companyid = s.companyid and P.supplierID = s.SupplierID)
inner join DIM_COUNTRY C ON (S.CountryID = C.countryid)
where p.companyid = 'xgt' AND Locationid in ('xgt1','xgtd1') and p.transdate &gt;= '2017-01-01'
order by 2</property>

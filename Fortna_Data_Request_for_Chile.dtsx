                          expressionType="Notify"
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  DISTINCT  upper(P.LocalSKU) as PRODUCT_ID,
P.ScCategoryName_L1 AS CATEGORY, REPLACE (P.ScCategoryName_L2,'"','') as SUBCATEGORY,
REPLACE (P.DescriptionLocal,'"','') as DESCRIPTION
FROM FACT_WS_INVENTORY S WITH(NOLOCK) INNER JOIN DIM_PRODUCT P WITH(NOLOCK) ON (S.companyID=P.CompanyID AND S.localsku=P.localsku and p.dw_status &lt;&gt; 'd')
WHERE S.DIM_PK_TIME &gt;= 20170801 AND LOCATIONID IN ('XSV1')
group by upper(P.LocalSKU), P.ScCategoryName_L1, REPLACE (P.ScCategoryName_L2,'"',''), REPLACE (P.DescriptionLocal,'"','')
HAVING (SUM(OH) &lt;&gt; 0 or SUM(OPO) &lt;&gt; 0 ) 
order by 1</property>

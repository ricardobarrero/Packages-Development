                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/* CUSTOMERMASTER */

select 
       case when A.Address is null then C.Address1 else replace(cast(A.Address_no as varchar) + ' ' + A.Address,';',' ') end as CUSSHIPMENTNUMBER
             ,C.customerID_SENTAI as CUSTNUMBER
             ,customerdesc as CUSTNAME
             ,C.city as CUSTCITY
             ,state_name as CUSTSTATE
                ,zip as CUSTZIP
             ,lower(businessUnitID) as CUSTCLASS
                    ,lower(CarrierID) as CARRIERID
            ,S.CLASSIFICATION
from DIM_CUSTOMER C
LEFT JOIN [DWStaging].[dbo].[SENTAI_CUST_ADDRESS] A ON (C.customerID_SENTAI = A.Cust_ID)
--LEFT JOIN [DWStaging].[dbo].[SENTAI_PROVINCE] P ON (C.state
LEFT JOIN DWStaging.dbo.STG_Classification_Customer S on c.customerid_sentai=S.customerid_Sentai
where C.customerID_SENTAI like 'XSV%'
and dw_status &lt;&gt; 'd'
order by 2


/*
/* CUSTOMERMASTER */


select '' as CUSSHIPMENTNUMBER
             ,customerID_SENTAI as CUSTNUMBER
             ,customerdesc as CUSTNAME
             ,city as CUSTCITY
             ,state as CUSTSTATE
             ,zip as CUSTZIP
             ,businessUnitID as CUSTCLASS
from DIM_CUSTOMER
where customerID_SENTAI like 'XCL%'
and dw_status &lt;&gt; 'd'
*/</property>

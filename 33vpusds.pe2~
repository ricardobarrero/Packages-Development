                            <outputColumn
                              refId="Package\Load STG Tables\Load PivotTable Zebra\Data Flow Task\OLE DB Source.Outputs[OLE DB Source Output].Columns[Trans_code]"
                              codePage="1252"
                              dataType="str"
                              errorOrTruncationOperation="Conversion"
                              errorRowDisposition="FailComponent"
                              externalMetadataColumnId="Package\Load STG Tables\Load PivotTable Zebra\Data Flow Task\OLE DB Source.Outputs[OLE DB Source Output].ExternalColumns[Trans_code]"
                              length="5"
                              lineageId="Package\Load STG Tables\Load PivotTable Zebra\Data Flow Task\OLE DB Source.Outputs[OLE DB Source Output].Columns[Trans_code]"
                              name="Trans_code"

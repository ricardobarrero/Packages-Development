              <DTS:PropertyExpression
                DTS:Name="Name">"Mensaje"</DTS:PropertyExpression>
              <DTS:PropertyExpression
                DTS:Name="SqlStatementSource">"Declare @Error varchar (500)
Declare @Paquete varchar (100)
Set @Error=(select Descripcion_Event from  ETL.LOG_Evento where Pk_Evento=(select MAX(Pk_Evento) from ETL.LOG_Evento )) 

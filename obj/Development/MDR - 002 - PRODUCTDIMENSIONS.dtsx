              <DTS:Variables />
              <DTS:PropertyExpression
                DTS:Name="SqlStatementSource">"INSERT INTO [ETL].[LOG_TASK_NAME] (DATE_EXECUTION,PACKAGE_NAME,NAME_TASK,TYPE_EXECUTION,IN_EXECUTION,BEGIN_EXECUTION )



SELECT  CONVERT(DATE,GETDATE(),110),'"+ @[System::PackageName] +"',
'"+ @[System::TaskName] +"',

CASE WHEN "+ (DT_STR, 10, 1252) @[User::VInt_TipoEjecucion] +"=0 THEN 'I'


WHEN "+(DT_STR, 10, 1252) @[User::VInt_TipoEjecucion] +"=1 THEN 'F'

ELSE 'ERROR'
 END,
1,
GETDATE()"</DTS:PropertyExpression>

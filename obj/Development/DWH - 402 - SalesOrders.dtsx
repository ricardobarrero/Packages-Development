                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  DISTINCT 
CASE WHEN PUB."SO-TRANS"."CONTROL-ENT" = 'I' THEN 'XUS' ELSE UPPER(PUB."SO-TRANS"."CONTROL-ENT") END  as CompanyID,
PUB."SO-TRANS"."TRANS-NO",PUB."PO-TRANS-D"."TRANS-NO" AS PONUMBER, 'I' as LinkkType, PUB."SO-PO-XREF"."SO-recno", PUB."SO-PO-XREF"."PO-RECNO"
FROM 

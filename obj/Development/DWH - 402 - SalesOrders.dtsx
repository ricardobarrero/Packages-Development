'"+ @[System::TaskName] +"',

CASE WHEN "+ (DT_STR, 10, 1252) @[User::VInt_TipoEjecucion] +"=0 THEN 'I'


WHEN "+(DT_STR, 10, 1252) @[User::VInt_TipoEjecucion] +"=1 THEN 'F'

ELSE 'ERROR'
 END,
1,
GETDATE()"</DTS:PropertyExpression>

INNER JOIN PUB."SO-PO-XREF" ON (PUB."SO-PO-XREF"."MACHINE-ID"= PUB."SO-TRANS-D"."MACHINE-ID"
      AND PUB."SO-PO-XREF"."SO-RECNO" = PUB."SO-TRANS-D"."SO-RECNO")
INNER JOIN PUB."PO-TRANS-D" ON (PUB."PO-TRANS-D"."MACHINE-ID" = "SO-PO-XREF"."MACHINE-ID"
      AND PUB."PO-TRANS-D"."PO-RECNO" = "SO-PO-XREF"."PO-RECNO")
WHERE  
PUB."SO-TRANS"."MACHINE-ID" = ''
AND PUB."SO-TRANS"."TRANS-CODE" = 'OR'

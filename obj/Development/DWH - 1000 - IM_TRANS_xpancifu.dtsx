                      SQLTask:SqlStatementSource="INSERT INTO [ETL].[LOG_TASK_NAME] (DATE_EXECUTION,PACKAGE_NAME,NAME_TASK,TYPE_EXECUTION,IN_EXECUTION,BEGIN_EXECUTION )&#xA;&#xA;&#xA;&#xA;SELECT  CONVERT(DATE,GETDATE(),110),'DWH - 1000 - IM_TRANS_xpancifu',&#xA;'UPDATE TABLE STG_IM_TRANS',&#xA;&#xA;CASE WHEN 1=0 THEN 'I'&#xA;&#xA;&#xA;WHEN 1=1 THEN 'F'&#xA;&#xA;ELSE 'ERROR'&#xA; END,&#xA;1,&#xA;GETDATE()" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

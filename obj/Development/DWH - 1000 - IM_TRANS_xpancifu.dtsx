              SQLTask:SqlStatementSource="----------------------------------------------------------------------------------------------------------------------------------------------------&#xA;--UPDATE Fecha Factura, Costos y Precios de Venta de la tabla STG_IM_DATA_SOPA&#xA;----------------------------------------------------------------------------------------------------------------------------------------------------&#xA;UPDATE DWStaging.dwh.STG_IM_TRANS&#xA;SET &#xA;SO_Trans_date&#x9;=&#x9;S.AR_TRansDate,&#xA;SO_UnitCost&#x9;&#x9;=&#x9;S.UnitCost,&#xA;SO_UnitCostUS&#x9;=&#x9;S.UnitCostUS,&#xA;SO_UnitPrice&#x9;=&#x9;S.UnitPrice,&#xA;SO_UnitPriceUS&#x9;=&#x9;S.UnitPriceUS&#xA;--select COUNT (*) &#xA;FROM DWStaging.dwh.STG_IM_TRANS D INNER JOIN dwstaging.dwh.stg_im_data_sopa S&#xA;ON (S.CompanyID = D.Companyid &#xA;&#x9;AND S.AR_TransCode = D.SO_Trans_Code&#xA;&#x9;AND S.AR_TransNo = D.SO_trans_No&#xA;&#x9;AND S.LineNumber= D.Link_Line_no)&#xA;&#x9;&#xA;&#x9;&#xA;---------------------------------------------------------------&#xA;--UPDATE DIM_PK_TIME_REAL&#xA;---------------------------------------------------------------&#xA;UPDATE DWStaging.dwh.STG_IM_TRANS&#xA;SET DIM_PK_TIME_REAL =(CASE &#xA;WHEN STG_IM_TRANS.Source_code ='SO' THEN CONVERT(VARCHAR (10),SO_Trans_date,112) &#xA;WHEN STG_IM_TRANS.Source_code &lt;&gt;'SO' THEN CONVERT(VARCHAR (10),Trans_date,112) &#xA;END)  &#xA;&#xA;-----------------------------------------------&#xA;--UPDATE US_Exch_Rate &#xA;---------------------------------------------&#xA;UPDATE DWSTAGING.DWH.STG_IM_TRANS&#xA;SET US_Exch_Rate = 1/(case &#xA;when DWH.STG_IM_TRANS.quantity &gt; 0 then DWH.STG_IM_TRANS.foreign_exch_rate&#xA;when DWH.STG_IM_TRANS.quantity &lt; 0 and DWH.STG_IM_TRANS.Link_exch_rate = 1 then DWH.STG_IM_TRANS.foreign_exch_rate&#xA;when DWH.STG_IM_TRANS.quantity &lt; 0 and DWH.STG_IM_TRANS.Link_exch_rate &lt;&gt; 1 then DWH.STG_IM_TRANS.Link_exch_rate&#xA;end)&#xA;FROM DWH.STG_IM_TRANS&#xA;&#xA;-------------------------------------------------------------------------------------------------------------&#xA;--UPDATE EA_FACTOR - EA_QUANTITY - EA_QUANTITY_LEFT&#xA;-------------------------------------------------------------------------------------------------------------&#xA;UPDATE DWSTAGING.DWH.STG_IM_TRANS&#xA;SET EA_FACTOR = ISNULL(C.&quot;CONV-FACTOR&quot;,0),&#xA;EA_QUANTITY = QUANTITY*ISNULL(C.&quot;CONV-FACTOR&quot;,0),&#xA;EA_QUANTITY_LEFT = QUANTITY_LEFT*ISNULL(C.&quot;CONV-FACTOR&quot;,0)&#xA;FROM DWSTAGING.DWH.STG_IM_TRANS IM, DWSTAGING.DBO.SENTAI_UNIT_CONV C&#xA;WHERE C.&quot;CONV-UNIT&quot; = 'EA'&#xA;AND IM.UNIT_ID &lt;&gt;'EA'&#xA;AND IM.UNIT_ID = C.&quot;UNIT-ID&quot;&#xA;&#xA;&#xA;-------------------------------------------------------------------------------------------------------------&#xA;-- ACTUALIZACIN DE US Y DW_STATUS...&#xA;-------------------------------------------------------------------------------------------------------------&#xA;UPDATE DWSTAGING.DWH.STG_IM_TRANS&#xA;SET COST_LEFTUS = ISNULL(COST_LEFT * FOREIGN_EXCH_RATE,0),&#xA;UNITCOST_LEFT = ISNULL( (COST_LEFT / ( CASE WHEN EA_QUANTITY_LEFT = 0 THEN 1 ELSE EA_QUANTITY_LEFT END)),0),&#xA;UNITCOST_LEFTUS = ISNULL(((COST_LEFT * FOREIGN_EXCH_RATE) /(CASE WHEN EA_QUANTITY_LEFT = 0 THEN 1 ELSE EA_QUANTITY_LEFT END)),0),&#xA;DW_IN_OUT  =CASE WHEN QUANTITY  &gt; 0   THEN 'I' ELSE 'O' END,&#xA;DW_AGING   = CASE WHEN QUANTITY  &gt; 0 AND (TRANS_ID = 'SO' AND SOURCE_CODE = 'SO') THEN 'N' WHEN QUANTITY  &gt; 0 THEN 'Y' ELSE 'N' END,&#xA;DW_USAGE   = CASE WHEN QUANTITY  &lt; 0  OR (SOURCE_CODE ='SO' AND TRANS_ID = 'SO') THEN (CASE WHEN TRANS_ID = 'SO' THEN'S'  WHEN TRANS_ID = 'BM' THEN 'B' WHEN TRANS_ID = 'ST' THEN 'T' ELSE 'O' END) ELSE 'N' END,&#xA;COST_AMT   =COST_AMT1 + COST_AMT2+ COST_AMT3+ COST_AMT4+ COST_AMT5+ COST_AMT6+ COST_AMT7+ COST_AMT8+ COST_AMT9+ COST_AMT10&#xA;           +COST_AMT11+COST_AMT12+COST_AMT13+COST_AMT14+COST_AMT15+COST_AMT16+COST_AMT17+COST_AMT18+COST_AMT19+COST_AMT20,&#xA;COST_AMTUS =(COST_AMT1 + COST_AMT2+ COST_AMT3+ COST_AMT4+ COST_AMT5+ COST_AMT6+ COST_AMT7+ COST_AMT8+ COST_AMT9+ COST_AMT10&#xA;           +COST_AMT11+COST_AMT12+COST_AMT13+COST_AMT14+COST_AMT15+COST_AMT16+COST_AMT17+COST_AMT18+COST_AMT19+COST_AMT20)/US_EXCH_RATE&#x9;&#xA;&#xA;&#xA;--===============================================&#xA;--UPDATE LOCATIONID 'xpl1' POR 'xpel1'&#xA;--===============================================&#x9;&#x9;&#xA;&#x9;&#x9;update dwh.STG_IM_TRANS&#xA;&#x9;&#x9;set location_id = 'xpel1'&#xA;&#x9;&#x9;where DIM_PK_BRANCH is null&#xA;&#x9;&#x9;and location_id = 'xpl1'&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

                      <DTS:PropertyExpression
                        DTS:Name="SqlStatementSource">"INSERT INTO [ETL].[LOG_TASK_NAME] (DATE_EXECUTION,PACKAGE_NAME,NAME_TASK,TYPE_EXECUTION,IN_EXECUTION,BEGIN_EXECUTION )




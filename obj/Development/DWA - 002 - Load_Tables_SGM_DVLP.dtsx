                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT ROW_NUMBER() OVER(ORDER BY  P.CompanyID ASC) AS 'i_Brand_Division_id',P.WINDOWS_USER as AD_User, P.CompanyID as AD_Org_ID_2, P.BusinessDivision_ID AS C_ActivityValue,
  BD.BUSINESSDIVISIONNAME AS divisionname,  
  C.manufbrand_id AS BrandValue, 
  C.manufbrand_name as Brand_Name,
case when c.SGM_ManufBrandGroup_ID like '%/%' then  substring(C.SGM_ManufBrandGroup_ID, CHARINDEX('/',c.SGM_ManufBrandGroup_ID)+1, len(C.SGM_ManufBrandGroup_ID)) ELSE '' END  AS BusinessGroupValue,   
case when c.SGM_ManufBrandGroup_ID like '%/%' then  substring(C.SGM_ManufBrandGroup_Name, CHARINDEX('/',c.SGM_ManufBrandGroup_Name)+1, len(C.SGM_ManufBrandGroup_Name)) ELSE '' END  AS BusinessGroupName,   
'N' as isController, 'N' as OnlyApprovedbyDivision,
1000000 AS "ad_client_id",
0 AS "ad_org_id",
100 AS createdby,
100 AS updatedby

FROM dbo.VIEW_WS_Employee_Product_Assignation_Active  p WITH (NOLOCK)  
   inner join INTXDW.dbo.DIM_BUSINESS_DIVISION BD WITH (NOLOCK) 
   ON BD.businessdivision_id= P.BusinessDivision_ID
   inner join [dbo].[SGM_EMPLOYEES_ACTIVE] EA on EA.WINDOWS_USER= P.WINDOWS_USER
   inner join Docuware_AD_Org AD on P.Companyid=AD.value and AD.c_country_isactive='Y'  
   left join dbo.BO_SEC_BD_MBG_CATALOG  c  with (nolock) on
   P.CompanyID = c.companyid and 
   P.BusinessDivision_ID = C.businessdivision_id and 
   P.SGM_ManufBrandGroup_ID=C.SGM_ManufBrandGroup_ID
   where P.Companyid not like 'C%'
   and EA.PROFILE_ID_GENERICO in ('PM','CM'/*,'BDCor'*/)


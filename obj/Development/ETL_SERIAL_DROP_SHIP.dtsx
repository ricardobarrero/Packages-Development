              SQLTask:SqlStatementSource="--===============================&#xA;--update dim_pk&#xA;--===============================&#xA;&#xA;UPDATE dwh.STG_SERIAL_DROP_SHIP&#xA;SET DIM_PK_PRODUCT = p.dIM_pk_product&#xA;FROM INTXDW.DBO.DIM_PRODUCT P INNER JOIN dwh.STG_SERIAL_DROP_SHIP S ON (P.localSKU=S.LOCALSKU)&#xA;&#xA;UPDATE dwh.STG_SERIAL_DROP_SHIP&#xA;SET DIM_PK_COMPANY = p.DIM_PK_COMPANY&#xA;FROM INTXDW.DBO.DIM_COMPANY P INNER JOIN dwh.STG_SERIAL_DROP_SHIP S ON (P.companyid=S.COMPANYID)&#xA;&#xA;--================================&#xA;-- update fields&#xA;--================================&#x9;   &#xA;&#xA;&#xA;UPDATE S&#xA;&#x9;   SET packno = Link_trans_no,&#xA;&#x9;       Pack_Line = Link_Line_no,&#xA;&#x9;&#x9;   PackDate = trans_date,&#xA;&#x9;&#x9;   ProdRecno = prod_recno,&#xA;&#x9;&#x9;   quantity = i.quantity,&#xA;&#x9;&#x9;   im_Trans_No=trans_no,&#xA;&#x9;&#x9;   im_Trans_Date = trans_date,&#xA;&#x9;&#x9;   dim_pk_time = convert(int, convert(varchar(11),trans_date,112))&#xA;&#x9;    from intxdw.dwh.FACT_DD_IM_TRANS I  inner join dwh.STG_SERIAL_DROP_SHIP S on (I.SO_trans_no=S.Transno and I.companyID='XUS' and I.product_id=S.LocalSKU)&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

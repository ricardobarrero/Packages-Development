      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{6DEBE925-066E-463F-846B-D16B26833084}"
          SQLTask:SqlStatementSource="/*UPDATE DWStaging.dbo.STG_IR_TRANS&#xA;SET Aging_date = CASE WHEN on_batch = 'y' then ir_trans_batch_trans_date &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE CONVERT(DATE, CAST(str(ir_fiscal_yr_prd) + '15' AS CHAR(12)),112) END;*/&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;UPDATE DWStaging.dbo.STG_IR_TRANS&#xA;SET Aging_date = CASE WHEN on_batch = 'y' then ir_trans_batch_trans_date &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;when isdate (substring (convert (varchar (15),ir_fiscal_yr_prd),1,4)+ &#xA;SUBSTRING (convert (varchar (15), ir_fiscal_yr_prd),5,2)+'01')=1 then &#xA;substring (convert (varchar (15),ir_fiscal_yr_prd),1,4)+ SUBSTRING (convert (varchar (15), ir_fiscal_yr_prd),5,2)+'15' end&#xA;from DWStaging.dbo.STG_IR_TRANS&#xA;where substring (convert (varchar (15), ir_fiscal_yr_prd),1,4)&lt;2016&#xA;and substring (CAST(ir_fiscal_yr_prd as varchar(20)),5,2) between 1 and 12" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

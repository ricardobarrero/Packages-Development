      DTS:ThreadHint="1">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{6DEBE925-066E-463F-846B-D16B26833084}"
          SQLTask:SqlStatementSource="UPDATE a &#xA;SET a.DIM_PK_COMPANY= ISNULL(b.DIM_PK_COMPANY,0)&#xA;FROM DWstaging.dbo.STG_IR_TRANS a, INTXDW.DBO.DIM_COMPANY b&#xA;where a.CompanyID = b.companyid;&#xA;&#xA;update B set B.BusinessUnitID = 'u'&#xA;from INTXDW.dbo.DIM_CUSTOMER A,DWstaging.dbo.STG_IR_TRANS B&#xA;WHERE A.companyid=B.CompanyID &#xA;and A.BusinessUnitID = 'na'&#xA;AND UPPER (A.customerID_SENTAI)=UPPER(B.ir_cust_id);&#xA;&#xA;update B set B.BusinessUnitID = A.BusinessUnitID&#xA;from INTXDW.dbo.DIM_CUSTOMER A,DWstaging.dbo.STG_IR_TRANS B&#xA;WHERE A.companyid=B.CompanyID &#xA;and A.BusinessUnitID &lt;&gt;'na'&#xA;AND UPPER (A.customerID_SENTAI)=UPPER(B.ir_cust_id);&#xA;&#xA;update B set B.DIM_PK_CUSTOMER=ISNULL(A.DIM_PK_CUSTOMER,0),&#xA;&#x9;B.DIM_PK_BUSINESSUNIT = A.DIM_PK_BUSINESSUNIT&#xA;&#x9;--B.BusinessUnitID = A.BusinessUnitID&#xA;from INTXDW.dbo.DIM_CUSTOMER A,DWstaging.dbo.STG_IR_TRANS B&#xA;WHERE A.companyid=B.CompanyID &#xA;AND UPPER (A.customerID_SENTAI)=UPPER(B.ir_cust_id);&#xA;&#xA;&#xA;UPDATE DWStaging.dbo.STG_IR_TRANS&#xA;   SET DIM_PK_SUPPLIER = isnull(sp.DIM_PK_SUPPLIER,0)&#xA;  from DWStaging.dbo.STG_IR_TRANS s with(nolock), &#xA;       INTXDW.dbo.DIM_SUPPLIER sp with(nolock)&#xA;WHERE s.companyID = sp.companyID&#xA;  and s.ir_supp_id = sp.SupplierID;&#xA;&#xA;UPDATE DWStaging.dbo.STG_IR_TRANS&#xA;   SET DIM_PK_PRODUCT = isnull(sp.DIM_PK_PRODUCT,0),&#xA;&#x9;DIM_PK_MANUFACTURER =  isnull(sp.DIM_PK_MANUFACTURER,0),&#xA;&#x9;DIM_PK_MANUFACTURER_BRAND =  isnull(sp.DIM_PK_MANUFACTURER_BRAND,0),&#xA;&#x9;DIM_PK_MANUFACTURER_DIVISION =  isnull(sp.DIM_PK_MANUFACTURER_DIVISION,0),&#xA;&#x9;ManufacturerID = sp.ManufacturerID,&#xA;&#x9;ManufacturerBrandID = sp.ManufacturerBrandID,&#xA;&#x9;ManufacturerDivisionID = sp.ManufacturerDivisionID&#xA;  from DWStaging.dbo.STG_IR_TRANS s with(nolock), &#xA;       INTXDW.dbo.DIM_PRODUCT sp with(nolock)&#xA;WHERE s.companyID = sp.companyID&#xA;  and s.Product_ID = sp.LocalSKU&#xA;  and sp.dw_status &lt;&gt; 'd';&#xA;&#xA;UPDATE DWStaging.dbo.STG_IR_TRANS&#xA;   SET&#xA;   DIM_PK_TIME&#x9;= convert(varchar(11),isnull(ir_trans_date,'1/1/1980'),112);&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

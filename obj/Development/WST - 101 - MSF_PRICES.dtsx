        <DTS:Executable
          DTS:refId="Package\DATA_LOAD_PROCESS\UPDATE_FIELDS"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{D683BC16-05B0-42AA-97B5-8ECE418D7B50}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="UPDATE_FIELDS"
          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{1EDF7741-8359-4DA2-9A72-CCA4143BA7FE}"
              SQLTask:BypassPrepare="False"
              SQLTask:SqlStatementSource="---INSERT INTO LICENSEPRODUCTTYPE-----&#xA;&#xA;insert into intcomexstore.dbo.LicenseProductType&#xA;select distinct [TIPO DE LICENCIA], GETDATE() as CreationDate,null&#xA;from SQLAPPList.DWSTAGING.dbo.STG_PRODUCTS_MICROSOFT;&#xA;&#xA;---------INSERT INTO LICENSEPRODUCTFAMILY-------&#xA;&#xA;insert into [IntcomexStore].[dbo].[LicenseProductFamily]&#xA;select DISTINCT [FAMILIA DE PRODUCTOS],GETDATE(),NULL&#xA;FROM  SQLAPPList.DWSTAGING.dbo.STG_PRODUCTS_MICROSOFT;&#xA;&#xA;&#xA;-------INSERT INTO [IntcomexStore].[dbo].[LicenseProductLicenseType]----------&#xA;&#xA;&#xA;&#xA;INSERT INTO [IntcomexStore].[dbo].[LicenseProductLicenseType]&#xA;SELECT DISTINCT [TIPO DE CLIENTE FINAL],GETDATE(),NULL&#xA;FROM SQLAPPList.DWSTAGING.dbo.STG_PRODUCTS_MICROSOFT;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;------UPDATE PRODUCTTYPEID---------&#xA;&#xA;update  B&#xA;SET B.ProductTypeid = A.ProductTypeid&#xA;FROM intcomexstore.dbo.LicenseProductType A, SQLAPPList.DWSTAGING.dbo.STG_PRODUCTS_MICROSOFT B&#xA;where B.[TIPO DE LICENCIA] = A.productTypeName;&#xA;&#xA;&#xA;---UPDATE TYPELICENSEID--------&#xA;&#xA;&#xA;update  B&#xA;set B.typelicenseid = A.typelicenseid &#xA;from intcomexstore.dbo.LicenseProductLicenseType A, SQLAPPList.DWSTAGING.dbo.STG_PRODUCTS_MICROSOFT B&#xA;where A.typelicensename = B.[TIPO DE CLIENTE FINAL];&#xA;&#xA;--------UPDATE FAMILYID----&#xA;update  B&#xA;set B.Familyid = A.Familyid&#xA;--SELECT A.FAMILYID &#xA;from intcomexstore.dbo.LicenseProductFamily A,   SQLAPPList.DWSTAGING.dbo.STG_PRODUCTS_MICROSOFT B&#xA;where A.Familyname = B.[FAMILIA DE PRODUCTOS];&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;--&gt;&gt;&gt; UPDATE P_venta&#xA;&#xA;update a set P_venta=CONVERT (money,           &#xA;ROUND (a.P_venta,2)+cast((10-SUBSTRING (CONVERT(VARCHAR,ROUND(a.P_venta,2)),&#xA;    CHARINDEX('.',CONVERT (varchar,ROUND(a.P_venta,2)),1)+2,&#xA;    LEN((CONVERT(VARCHAR,ROUND(a.P_venta,2))))))as float)/CAST(100 as float)&#xA;    )&#xA;from SQLAPPList.DWSTAGING.dbo.STG_PRODUCTS_MICROSOFT a&#xA;where &#xA;SUBSTRING (CONVERT(VARCHAR,P_venta),CHARINDEX('.',CONVERT (varchar,P_venta),1)+2,LEN((CONVERT(VARCHAR,P_venta))))&lt;&gt;0&#xA;&#xA;update a  set P_venta=round(a.P_venta,2)&#xA;from SQLAPPList.DWSTAGING.dbo.STG_PRODUCTS_MICROSOFT a&#xA;where SUBSTRING (CONVERT(VARCHAR,a.P_venta),CHARINDEX('.',CONVERT (varchar,a.P_venta),1)+2,LEN((CONVERT(VARCHAR,a.P_venta))))=0&#xA;&#xA;&#xA;update a  set P_venta=round(a.[P_venta],2)&#xA;from SQLAPPList.DWSTAGING.dbo.STG_PRODUCTS_MICROSOFT a&#xA;where SUBSTRING (CONVERT(VARCHAR,a.[P_venta]),CHARINDEX('.',CONVERT (varchar,a.[P_venta]),1)+2,LEN((CONVERT(VARCHAR,a.[P_venta]))))=0&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>

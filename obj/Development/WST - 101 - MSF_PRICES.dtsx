          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{1EDF7741-8359-4DA2-9A72-CCA4143BA7FE}"
              SQLTask:SqlStatementSource="TRUNCATE TABLE [IntcomexStore].[dbo]. LICENSEPRODUCT;&#xA;&#xA;truncate table  [IntcomexStore].[dbo].[LicenseProductType];&#xA;&#xA;truncate table  [IntcomexStore].[dbo].[LicenseProductFamily];&#xA;&#xA;TRUNCATE TABLE [IntcomexStore].[dbo].[LicenseProductLicenseType];" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

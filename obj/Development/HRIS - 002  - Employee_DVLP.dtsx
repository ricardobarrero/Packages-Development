                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT c.company_code AS Compañia,  emp_firstname AS Nombre,
                                        emp_lastname AS Apellido,
                                        emp_work_email AS Email
                                        
                                        FROM hs_hr_employee e
                                        INNER JOIN imx_sec_employee ec ON e.employee_id = ec.emp_number
                                        INNER JOIN ohrm_user u ON e.employee_id = u.emp_number 
                                        INNER JOIN imx_sec_companies c ON ec.company_id = c.company_id
                                        INNER JOIN ohrm_job_title et ON e.job_title_code = et.id
                                        WHERE emp_status&lt;&gt; 1 
                                        ORDER BY e.emp_number ASC

/*
SELECT
e.employee_id,e.emp_firstname,e.emp_middle_name,e.emp_lastname,e.emp_work_email ,e.emp_status,st.name,j.id,j.job_title, e.coun_code,
 joined_date, cc.unit_id
FROM hs_hr_employee e
LEFT JOIN ohrm_job_title j ON e.job_title_code = j.id
LEFT JOIN ohrm_employment_status  st  ON st.id = e.emp_status
LEFT JOIN ohrm_subunit cc ON cc.id = e.eeo_cat_code
WHERE st.name NOT LIKE 'Terminated%'
ORDER BY j.job_title
*/


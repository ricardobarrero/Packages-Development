          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="envia correo si la tabla new_products esta vacia"
          DTS:DTSID="{0C3B9D7E-4BCE-49A0-BDA8-9F07EFE60E9F}"

        <DTS:Executable
          DTS:refId="Package\DWH_NewProducts\UPDATE DWH_NewProduct"
          DTS:CreationName="Microsoft.ExecuteSQLTask"
          DTS:Description="Execute SQL Task"
          DTS:DTSID="{AAE2389E-F6F6-4BA3-A4AE-F650690B5CB3}"
          DTS:ExecutableType="Microsoft.ExecuteSQLTask"
          DTS:LocaleID="-1"
          DTS:ObjectName="UPDATE DWH_NewProduct"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{63BD4ECF-9DE7-40A9-B237-85A019A0F616}"
              SQLTask:SqlStatementSource="&#xA;update N&#xA;set N.manufSKU= SUBSTRING (D.manufSKU,1,25),&#xA;    N.BusinessDivisionID = D.BusinessDivisionID_2011,&#xA;&#x9;N.BusinessDivisionName=D.BusinessDivisionName,                     &#xA;&#x9;N.BusinessDivisionName_sp =BD.BD_Description_sp,&#xA;&#x9;N.ManufBrandID=D.ManufacturerBrandID,&#xA;&#x9;N.ManufBrandName = M.Description,&#xA;&#x9;N.descriptionSpa = D.descriptionSpa,&#xA;&#x9;N.descriptionEng = D.descriptionEng,&#xA;&#x9;N.descriptionLocal=D.descriptionLocal,&#xA;&#x9;N.CategoryID =  isnull(C.CategoryidLevel1,'?'),&#xA;&#x9;N.SubCategoryID =  isnull(C.CategoryidLevel2,'?'),&#xA;&#x9;N.CategoryEs = isnull(C.NameLevel1Es,'?'),&#xA;&#x9;N.CategoryEn=isnull(C.NameLevel1En,'?'),&#xA;&#x9;N.SubCategoryEs=isnull(C.NameLevel2Es,'?'),&#xA;&#x9;N.SubCategoryEN=isnull(C.NameLevel2En,'?')&#xA;--select count(*)&#xA;from sqlwstlist.IntcomexStore.dbo.DWH_NewProducts N &#xA;     INNER JOIN intxdw.dbo.dim_product D on (N.localsku = D.localSKU and N.CompanyID = D. companyID)&#xA;&#x9; INNER JOIN intxdw.dbo.DIM_BUSINESS_DIVISION_CPY CPY ON (D.DIM_PK_COMPANY=CPY.DIM_PK_COMPANY and D.DIM_PK_BUSINESS_DIVISION=CPY.DIM_PK_BUSINESS_DIVISION  AND CPY.dw_status  !=  'd'  )&#xA;&#x9; INNER JOIN intxdw.dbo.DIM_PRODUCT_ScCATEGORY C ON (C.DIM_PK_PRODUCT_ScCATEGORY=D.DIM_PK_PRODUCT_ScCATEGORY  AND  C.dw_status  !=  'd' )&#xA;&#x9; INNER JOIN intxdw.dbo.DIM_BUSINESS_DIVISION BD ON (BD.BusinessDivision_ID = D.BusinessDivisionID_2011)&#xA;&#x9; INNER JOIN intxdw.dbo.DIM_MANUFACTURER_BRAND M ON (M.DIM_PK_MANUFACTURER_BRAND = D.DIM_PK_MANUFACTURER_BRAND )&#xA;where N.Dim_Pk_Time =convert (int, convert(varchar(11),getdate(),112)) AND D.manufSKU not in ('FWA ORDENADOR HORIZONTAL 2U PLASTICO DOBLE')&#xA;&#xA;&#xA;&#xA;&#xA;update N&#xA;set N.afs =D.afs,&#xA;    N.unitpriceUS=D.UnitPriceUS&#xA;--select count(*)&#xA;from sqlwstlist.IntcomexStore.dbo.DWH_NewProducts N &#xA;     INNER JOIN intxdw.dbo.fact_inventory D on (N.localsku = D.localSKU and N.CompanyID = D. companyID AND N.LocationID=D.LocationID)&#xA;where N.Dim_Pk_Time =convert (int, convert(varchar(11),getdate(),112)) &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>

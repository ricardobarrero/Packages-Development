                    <property
                      dataType="System.String"
                      description="Specifies the SQL statement used by the component to extract data."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- =========================================================================================
-- Query versión Oscar para extraer productos con aging del DWH con bucket mayor a 60 dias
-- Todo SKU que esta con status Descontinuado, no sea A o B,  Con Aging Qty = Exceso Qty.
-- =========================================================================================
select 
    T.DIM_PK_TIME,
    inv.companyID,
    inv.LocationID,
    p.localrecno,
    p.localSKU,
    p.manufSKU,
    P.BusinessDivisionID_2011,
    P.BusinessDivisionName,
    bd.BD_Description_sp,
    P.ManufacturerBrandID,
    P.ManufBrandName,
    P.product_image,
    P.descriptionSpa,
    P.descriptionEng,
    P.descriptionLocal,
    isnull(CAT.CategoryidLevel1,'?') as Category,
    isnull(CAT.CategoryidLevel2,'?')as SubCategory,
    isnull(CAT.NameLevel1Es,'?') as CategoryEs,
    isnull(CAT.NameLevel1En,'?') as CategoryEn,
    isnull(CAT.NameLevel2Es,'?')as SubCategoryEs,
    isnull(CAT.NameLevel2En,'?')as SubCategoryEn,
    ABC.afs as afs
FROM  dbo.DIM_TIME T 
    INNER join dbo.FACT_DP_INVENTORY_AGING_CPY_LOC_PROD inv  on (inv.DIM_PK_TIME =T.DIM_PK_TIME  AND T.DIM_DATE  = getdate() -1  
        and bucket_181_210_OH+bucket_211_240_OH+bucket_241_270_OH+bucket_271_300_OH+bucket_301_330_OH+bucket_331_360_OH+bucket_999_OH &gt; 0
        and inv.Locationid = (case when inv.companyid ='XUS' then 'm1' else inv.Locationid end ))
    INNER JOIN dbo.DIM_COMPANY cp on (cp.companyid = inv.companyid and cp.active  =  'y' ) 
    INNER JOIN dbo.FACT_ABC_WS_PRODUCT ABC ON (ABC.DIM_PK_TIME =T.DIM_PK_TIME   AND ABC.LOCATIONID = INV.locationid AND ABC.dim_pk_product = inv.dim_pk_product 
          AND ABC.Loc_status_code = 'd' -- Productos con estado descontinuado en Trax
          AND ABC.ABC_T_Company not in ('A', 'B') --Todo SKU que no sea A o B
         AND (bucket_181_210_OH+bucket_211_240_OH+bucket_241_270_OH+bucket_271_300_OH+bucket_301_330_OH+bucket_331_360_OH+bucket_999_OH &gt;0 AND ABC.Excess_Qty &gt;0) -- Con Aging Qty &gt;0 and Exceso Qty &gt;0 
          --AND (bucket_181_210_OH+bucket_211_240_OH+bucket_241_270_OH+bucket_271_300_OH+bucket_301_330_OH+bucket_331_360_OH+bucket_999_OH)* ABC.UnitCostUS &gt;= 1000) -- OH$&gt; 1,000$ 
          )     
    INNER JOIN dbo.DIM_PRODUCT P on (p.dim_pk_company = INV.dim_pk_company and p.dim_pk_product = INV.dim_pk_product AND  p.dw_status  &lt;&gt;  'd' and  P.BusinessDivisionID_2011 &lt;&gt; '?' and substr(p.localsku,1,2) &lt;&gt; 'PP')
    INNER JOIN dbo.DIM_PRODUCT_ScCATEGORY CAT   ON (CAT.DIM_PK_PRODUCT_ScCATEGORY= P.DIM_PK_PRODUCT_ScCATEGORY  AND  cat.dw_status  !=  'd' )
    INNER JOIN dbo.DIM_BUSINESS_DIVISION BD ON (BD.BusinessDivision_ID = P.BusinessDivisionID_2011)</property>

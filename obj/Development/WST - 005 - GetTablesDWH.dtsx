          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2017 CTP2.1; © 2017 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="1">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{718E4D07-1277-4D22-9B22-188BDB4DC0DA}"
              SQLTask:SqlStatementSource="begin&#xA;if (&#xA;SELECT COUNT (*) &#xA;FROM IntcomexStore.dbo.stg_new_products&#xA;--where CustomerID like '%&quot;%'&#xA;) =0&#xA;EXEC msdb.dbo.sp_send_dbmail &#xA;@profile_name = 'Jobs Monitoring',&#xA;@recipients = 'marlon.claro@intcomex.com; smonroy@intcomex.com',&#xA;@body = 'la tabla dbo.stg_new_products de IntcomexStore esta vacia, revisar el Job WST - 001 - ProductActivations  !!!!!!!!', &#xA;@subject = 'Revisar tabla dbo.stg_new_products de IntcomexStore';&#xA;&#xA;END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>

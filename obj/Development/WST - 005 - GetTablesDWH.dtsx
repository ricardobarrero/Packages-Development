                  description="Extracts data from a relational database by using a .NET provider."
                  name="Query IQ"
                  usesDispositions="true"
                  version="4">
                  <properties>
                    <property
                      dataType="System.String"
                      description="Specifies the SQL statement used by the component to extract data."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-- ======================================================================
-- Nuevo Query para extraer productos nuevos del DWH segun fecha de recepción
-- ======================================================================
SELECT
    T.DIM_PK_TIME,
    ABC.companyID,
    ABC.LocationID,
    ABC.localrecno,
    ABC.localSKU,
    ABC.manufSKU,
    P.BusinessDivisionID_2011,
    P.BusinessDivisionName,
    bd.BD_Description_sp,
    P.ManufacturerBrandID,
    P.ManufBrandName,
    P.descriptionSpa,
    P.descriptionEng,
    P.descriptionLocal,
    isnull(CAT.CategoryidLevel1,'?') as Category,
    isnull(CAT.CategoryidLevel2,'?')as SubCategory,
    isnull(CAT.NameLevel1Es,'?') as CategoryEs,
    isnull(CAT.NameLevel1En,'?') as CategoryEn,
    isnull(CAT.NameLevel2Es,'?')as SubCategoryEs,
    isnull(CAT.NameLevel2En,'?')as SubCategoryEn,
    P.product_image,
    sum(ABC.afs) as afs,
    ABC.LastPurchase
--select count(*)
FROM dbo.DIM_TIME T 
   INNER JOIN dbo.FACT_ABC_WS_PRODUCT  ABC    ON (ABC.DIM_PK_TIME =T.DIM_PK_TIME  AND T.WEEK  = DATEPART(WK,DATEADD(DAY, -6-DATEPART(WEEKDAY, GETDATE()), GETDATE()))) --Calcula la semana anterior
   INNER JOIN dbo.DIM_PRODUCT  P ON (P.DIM_PK_PRODUCT = ABC.DIM_PK_PRODUCT AND ABC.IsNew  =  'Y' AND isnull(ABC.Licence_TYPE,'') &lt;&gt; 'Y' AND upper(isnull(ABC.PurchaseType,'')) &lt;&gt; 'SPECIALORDER' AND  ABC.Locationid =(case when ABC.companyid ='XUS' then 'm1' else ABC.Locationid end ) )
   INNER JOIN dbo.DIM_BUSINESS_DIVISION_CPY ON (P.DIM_PK_COMPANY=dbo.DIM_BUSINESS_DIVISION_CPY.DIM_PK_COMPANY and P.DIM_PK_BUSINESS_DIVISION=dbo.DIM_BUSINESS_DIVISION_CPY.DIM_PK_BUSINESS_DIVISION  AND dbo.DIM_BUSINESS_DIVISION_CPY.dw_status  !=  'd'  )
   INNER JOIN dbo.DIM_PRODUCT_ScCATEGORY CAT   ON (CAT.DIM_PK_PRODUCT_ScCATEGORY=P.DIM_PK_PRODUCT_ScCATEGORY  AND  cat.dw_status  !=  'd' )
   INNER JOIN dbo.DIM_BUSINESS_DIVISION BD ON (BD.BusinessDivision_ID = P.BusinessDivisionID_2011)
WHERE
     /* Comment by Sandra M. porque estan saliendo productos como nuevos cuya ultima recepcion no es la primera*/
    --ABC.LastPurchase between CONVERT(DATE,DATEADD(dd, DATEPART(DW,GETDATE())*-1-6, GETDATE())) and CONVERT(DATE,DATEADD(dd, DATEPART(DW,GETDATE())*-1+0, GETDATE())) --Sabado anterior a viernes anterior
    ABC.FirstPurchaseDate between CONVERT(DATE,DATEADD(dd, DATEPART(DW,GETDATE())*-1-6, GETDATE())) and CONVERT(DATE,DATEADD(dd, DATEPART(DW,GETDATE())*-1+0, GETDATE())) --Sabado anterior a viernes anterior
    AND ABC.CompanyID&lt;&gt;'XTE'
    AND exists (SELECT 'e' from dbo.FACT_PURCHASE P WHERE P.COMPANYID = ABC.COMPANYID AND P.LOCATIONID = ABC.LOCATIONID AND P.LOCALSKU = ABC.LOCALSKU AND P.TRANSDATE BETWEEN CONVERT(DATE,DATEADD(dd, DATEPART(DW,GETDATE())*-1-6, GETDATE())) and CONVERT(DATE,DATEADD(dd, DATEPART(DW,GETDATE())*-1+0, GETDATE())))
GROUP BY
    T.DIM_PK_TIME,
    P.BusinessDivisionID_2011,
    isnull(CAT.CategoryidLevel1,'?'),
    isnull(CAT.CategoryidLevel2,'?'),
    P.ManufacturerBrandID,
    ABC.companyID,
    ABC.localrecno,
    ABC.localSKU,
    ABC.manufSKU,
    P.product_image,
    P.descriptionSpa,
    P.descriptionEng,
    P.descriptionLocal,
    isnull(CAT.NameLevel1Es,'?') ,
    isnull(CAT.NameLevel1En,'?'),
    isnull(CAT.NameLevel2Es,'?'),
    isnull(CAT.NameLevel2En,'?'),
    P.ManufBrandName,
    P.BusinessDivisionName,
    bd.BD_Description_sp,
    ABC.LocationID,
    ABC.LastPurchase
ORDER BY ABC.companyid, P.BusinessDivisionName,ABC.localsku

SELECT I.CompanyID, I.LocalSku, I.Localrecno, I.Module, I.Reason, I.RequestedBy, I.RequestedFirstName, I.RequestedLastName, D.DescriptionEs
FROM   [dbo].[DWH_RejectedProducts] I
       INNER JOIN (
          SELECT convert (int,convert(varchar(11),max(lastupload),112)) max_lastupload
          FROM [dbo].[DWH_RejectedProducts]
       ) U
           ON U.max_lastupload = convert (int,convert(varchar(11),I.LastUpload,112))
    INNER JOIN products D on I.LocalSKU = D.LocalSKU
WHERE I.Module = 'NewProducts'
  AND CompanyID = 'XJM'   
  AND Reason='HasImages=0'

*/</property>

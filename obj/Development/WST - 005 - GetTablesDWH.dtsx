                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--=====================================================
-- QUERY PARA SACAR LOS DATOS NewProducts XJM
--=====================================================

SELECT DISTINCT I.CompanyID, I.LocalSku, I.Localrecno, I.Module, I.Reason, I.RequestedBy, I.RequestedFirstName, I.RequestedLastName, substring (D.DescriptionEs,1,255) as DescriptionEs,F.OH,P.BusinessDivisionID_2011,P.ManufBrandName
FROM   [dbo].[DWH_RejectedProducts] I
       INNER JOIN (
          SELECT convert (int,convert(varchar(11),max(lastupload),112)) max_lastupload
          FROM [dbo].[DWH_RejectedProducts]
       ) U
           ON U.max_lastupload = convert (int,convert(varchar(11),I.LastUpload,112))
		   INNER JOIN products D on I.LocalSKU = D.LocalSKU
		   INNER JOIN S91SQLUS.INTXDW.dbo.DIM_PRODUCT P WITH(NOLOCK) ON P.localsku=I.localsku and P.companyid = I.CompanyID 
		   INNER JOIN (SELECT SUM(OH) AS OH,COMPANYID,LOCALSKU,LOCALRECNO FROM S91SQLUS.INTXDW.DBO.FACT_INVENTORY GROUP BY COMPANYID,LOCALSKU,LOCALRECNO ) F 
		   ON i.CompanyID = F.COMPANYID AND I.LocalSku=F.LOCALSKU AND I.Localrecno=F.LOCALRECNO
WHERE I.Module = 'NewProducts'
  AND I.CompanyID = 'XJM'   
  AND Reason='HasImages=0'  order by f.oh desc 




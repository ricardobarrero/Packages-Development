                  componentClassID="Microsoft.ManagedComponentHost"
                  contactInfo="Extracts data from a relational database by using a .NET provider.;Microsoft Corporation; Microsoft SqlServer v10; © 2007 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
                  description="Extracts data from a relational database by using a .NET provider."
                  name="CR"
                  usesDispositions="true"
                  version="4">
                  <properties>
                    <property
                      dataType="System.String"
                      description="Specifies the SQL statement used by the component to extract data."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  
(CASE WHEN NCRE."CONTROL-ENT"='I' THEN 'XUS' ELSE   NCRE."CONTROL-ENT" END) "CONTROL-ENT" ,
NCRE."TRANS-CODE"  "TRANS-CODE-CR",
NCRE."TRANS-NO"    "TRANS-NO-CR",
NCRE."SOURCE-CODE"  "TRANS-SOURCE-CR",
NCRE."TRANS-DATE"  "TRANS-DATE-CR",

FACT."TRANS-CODE"  "TRANS-CODE-IN",
MIN(FACT."TRANS-NO")    "TRANS-NO-IN",
FACT."TRANS-DATE"  "TRANS-DATE-IN",

SO."TRANS-NO"      "TRANS-NO-RM",
SO."TRANS-CODE"    "TRANS-CODE-RM",
SO."TRANS-DATE"    "TRANS-DATE-RM",
SO."RMA-ID",
SO."oper-id"


FROM PUB."AR-TRANS" NCRE 

INNER JOIN PUB."SO-TRANS-X" SOX     


ON  NCRE."CONTROL-ENT" =  SOX."CONTROL-ENT" AND NCRE."MACHINE-ID" = SOX."LINK-MACHINE"
AND NCRE."TRANS-CODE"  = SOX."LINK-CODE" 
AND NCRE."TRANS-NO"     =  SOX."LINK-NO"    AND NCRE."MACHINE-ID" = SOX."MACHINE-ID"
AND SOX."TRANS-CODE" = 'RM' -- RM DE SO TRANS

INNER JOIN PUB."SO-TRANS-X" SOXIM   

ON  SOXIM."CONTROL-ENT" =  SOX."CONTROL-ENT" AND SOXIM."LINK-MACHINE"  = SOX."MACHINE-ID" 
AND SOXIM."TRANS-CODE"     =  SOX."TRANS-CODE"    AND SOXIM."MACHINE-ID" = SOX."MACHINE-ID"
AND SOXIM."TRANS-NO"     =  SOX."TRANS-NO"   
AND SOXIM."LINK-CODE" = 'IN' 

INNER JOIN PUB."AR-TRANS" FACT     

ON  SOXIM."CONTROL-ENT" = FACT."CONTROL-ENT" AND SOXIM."LINK-CODE" = FACT."TRANS-CODE" 
AND SOXIM."LINK-NO"    = FACT."TRANS-NO"    AND SOXIM."MACHINE-ID" = FACT."MACHINE-ID" 


INNER JOIN PUB."SO-TRANS" SO        

ON  SOX."CONTROL-ENT" = SO."CONTROL-ENT" AND SOX."TRANS-CODE" = SO."TRANS-CODE" 
AND SOX."TRANS-NO"    = SO."TRANS-NO"    AND SOX."MACHINE-ID" = SO."MACHINE-ID"

WHERE  NCRE."TRANS-CODE" = 'CR' AND  NCRE."SOURCE-CODE"='SO'  -- NOTA CREDITO
AND NCRE."TRANS-DATE" BETWEEN  '01/01/2010'  AND  '12/31/2010' 
AND NCRE."MACHINE-ID" = ''


GROUP BY 
NCRE."CONTROL-ENT" ,
NCRE."TRANS-NO"    ,
NCRE."TRANS-CODE"  ,
NCRE."SOURCE-CODE" ,
NCRE."TRANS-DATE"  ,

FACT."TRANS-CODE"  ,

FACT."TRANS-DATE" ,

SO."TRANS-NO"     ,
SO."TRANS-CODE"    ,
SO."TRANS-DATE"    ,
SO."RMA-ID",
SO."oper-id"</property>

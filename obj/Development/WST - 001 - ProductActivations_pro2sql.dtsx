              SQLTask:SqlStatementSource="IF EXISTS (SELECT [name] FROM tempdb.dbo.sysobjects WHERE xtype = 'U' and [name] like '#Disp_Kits%')&#xA;&#x9;DROP TABLE #Disp_Kits&#xA;&#x9;&#xA;SELECT DISTINCT C.CompanyID,  C.KitSKU, min (A.Availability/case when C.ComponentQuantity = 0 then 1 else C.ComponentQuantity end) as Availability&#xA;INTO #Disp_Kits&#xA;FROM INTXDW.DBO.DIM_KIT_COMPONENT_SENTAI C &#xA;INNER JOIN intxdw.dbo.Product_Activation_By_Location_tbl L ON (L.CompanyID = C.CompanyID AND L.[Product-ID] = C.[KitSKU])&#xA;INNER JOIN &#xA;(&#xA;&#x9;SELECT [Parent-loc], [Product-ID], case when sum(q.[on-hand] - q.[allocated]) &lt; 0 then 0 else sum(q.[on-hand] - q.[allocated]) end as Availability&#xA;&#x9;FROM dwstaging.dbo.SENTAI_PROD_EXP_LOC_Q Q&#xA;&#x9;WHERE [seq-no] = 0 AND [Product-ID] IN (SELECT DISTINCT ComponentSKU FROM INTXDW.DBO.DIM_KIT_COMPONENT_SENTAI WHERE KitSKU IN &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(SELECT [Product-ID] FROM INTXDW.DBO.Product_Activation_By_Location_tbl WHERE [Org-Code] = 'k'))&#xA;&#x9;&#xA;&#x9;GROUP BY [Parent-loc], [Product-ID]&#xA;) A&#xA;ON (C.ComponentSKU = A.[Product-ID] AND L.[parent-Loc] = A.[Parent-loc])&#xA;WHERE L.[Org-Code] = 'k' --3331 --63651&#xA;--AND C.KitSKU = 'KT011XCL31'&#xA;GROUP BY C.CompanyID,  C.KitSKU, C.Status&#xA;ORDER BY C.CompanyID, C.KitSKU&#xA;&#xA;UPDATE SQLWSTList.IntcomexStore.dbo.ProductActivations SET Availability = D.Availability &#xA;FROM #Disp_Kits D, SQLWSTList.IntcomexStore.dbo.ProductActivations A&#xA;WHERE D.CompanyID = A.CompanyID and D.KitSku = A.Localsku&#xA;&#xA;DROP TABLE #Disp_Kits" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

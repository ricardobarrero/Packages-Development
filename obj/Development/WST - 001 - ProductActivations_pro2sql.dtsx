        <SQLTask:SqlTaskData
          SQLTask:Connection="{DBA44377-8EFC-49F2-8222-C969ED284131}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="-- ========================================================================================================================&#xA;-- SUMA LA DISPONIBILIDAD DE LAS LOCALIDADES PRINCIPALES PARA XMX,XPE, Y XEC PORQUE TIENEN MAS DE UNA LOCALIDAD PRINCIPAL&#xA;-- Y HACE EL INSERT EN UNA TABLA TEMPORAL&#xA;-- ========================================================================================================================&#xA;SELECT  p.[prod_recno], case when p.&quot;loc_group_id&quot; = 'm1' then 'XUS' else upper(substring(p.&quot;loc_group_id&quot; ,1,3)) end as CompanyID,&#xA;&#x9; sum(q.[on_hand] - q.[allocated]) as Availability&#xA;INTO #temp&#xA;FROM SQL4AWOL.ODS.trax.prod_exp e with(nolock),&#xA;&#x9; SQL4AWOL.ODS.trax.prod_exp_loc p with(nolock),&#xA;&#x9; SQL4AWOL.ODS.trax.prod_exp_loc_q q with(nolock)&#xA;WHERE &#x9;p.[prod_recno] = e.[prod_recno]&#xA;&#x9;and p.[product_id] = e.[product_id]&#xA;&#x9;and p.[prod_recno] = q.[prod_recno] &#xA;&#x9;and p.[parent_loc] = q.[parent_loc]&#xA;&#x9;and p.[parent_loc] = q.[loc_id] &#xA;&#x9;and p.[loc_group_id] in ('xmx1','xpe1','xec1')&#xA;&#x9;and (p.[parent_loc] like ('xmx%1') OR P.[parent_loc] like 'xpe%1' OR p.[parent_loc] like 'xec%1')&#xA;&#x9;and p.[status_code] = 'a'&#x9;&#x9;&#x9;&#x9;-- Only active products&#xA;&#x9;and p.&quot;component_type&quot; in ('n','k') &#x9;-- Product Normal&#xA;&#x9;and q.&quot;seq_no&quot;=0&#x9;&#xA;&#x9;and e.[prod_type] &lt;&gt; 'o'&#xA;&#x9;--and p.[Product_ID] NOT LIKE 'PC%XGT%' &#xA;&#x9;GROUP BY P.[prod_recno],  P.&quot;loc_group_id&quot;&#xA;HAVING  sum(q.[on_hand] - q.[allocated]) &gt; 0 &#xA;&#xA;-- ==============================================================================================&#xA;-- HACE EL UPDATE DE LA DISPONIBILIDAD EN LA TABLA INTXDW.DBO.PRODUCT_ACTIVATION_BY_LOCATION_TBL&#xA;-- ==============================================================================================&#xA;&#xA;update P&#xA;set P.Availability = T.Availability&#xA;--select  P.[product_id],p.[companyid],P.Availability , T.Availability&#xA;from intxdw.dbo.Product_Activation_by_location_tbl P , #temp T&#xA;where P.companyid = T.CompanyID&#xA;and P.[prod-recno]=T.[prod_recno] &#xA;and P.Availability &lt;&gt; T.Availability&#xA;&#xA;-- ==========================&#xA;-- ELIMINA LA TABLA TEMPORAL&#xA;--==========================&#xA;drop table #temp" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>

                          description="The SQL command to be executed."
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT OrderNo as ORDERNUMBER, 
      (select max(Trans_date) from fact_SalesOrders SO where So.CompanyID = ? and so.Trans_code='or' and Trans_no = S.OrderNo) AS RECEIVED,
      (select max(Transdate) from fact_so_pick PI where PI.CompanyID = ? and  SOTranscode='or' and SOTransNo = S.OrderNo) as PICKED,
      Transdate as SHIPPED,
      C.BusinessUnitID as CHANNEL,
      S.CarrierID as ORDERTYPE,
      C.CustomerID_Sentai as CUSTNO,
      S.AddressNo as CSHIPNO,
      '' as OUTBOINDCTNS,
      C.State as ZIP,
      S.LocationID as WHSE,
      '' as SHIPMENTNO,
      '' as BACKORDER,
      S.CarrierID as CARRIER,
      --'' as USER_DEFINED,
             S.businessUnitID as BUSINESS_UNIT,
             P.LocalSKU as SKU,
             ShipQty as QUANTITY,
             UnitPriceUS as SELLPRICE,
             D.Description as  VASFLAG,
             P.ManufSKU as MPN,
             P.type_id,
			 S.City,
			 s.SpecialOrder_Flg,
			 P.BusinessDivisionID_2011 as BusinessDivisionID,
			 BD.BusinessCategory,
			 P.ScCategoryName_L1 as ItemCategory
--INTO WORKAREA.DBO.OrderInput2_XCR
FROM FACT_PRODUCT_SALE S WITH(NOLOCK) 
INNER JOIN DIM_PRODUCT P ON (S.companyID=P.CompanyID AND S.localsku=P.localsku and p.dw_status &lt;&gt; 'd') --376649 --382044
INNER JOIN DIM_CUSTOMER C ON (S.companyID=C.CompanyID AND S.customerID=C.CustomerID)
INNER JOIN DIM_CARRIER D ON (s.carrierID=d.CarrierID)
INNER JOIN DIM_BUSINESS_DIVISION BD on (BD.BusinessDivision_ID = P.BusinessDivisionID_2011)
WHERE S.CompanyId=? and S.lineID &lt;&gt; 'NPTX' and S.BusinessUnitID &lt;&gt; 'm' AND s.Locationid = ?
AND S.so_transID &lt;&gt; 'esd' and p.Type_ID not in ('c','l','e')
AND S.DIM_PK_TIME between  ? and ?
AND S.TRANSID='soin'
Order by 1,P.LocalSKU</property>

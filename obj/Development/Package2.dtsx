       pa."trans-code" = 'pa' and         /* Solo packs de venta */
       pa."so-Trans-code" = 'or' and             /* que vengan de una orden de venta (no transferencias) */
       pa."trans-date" = '05/05/2020' and /* FEcha del dia anterior */
       pa."trans-no" &lt; 100000000 and             /* Excluye transacciones temporales */
       pd."im-trans-vals" &lt;&gt; ''             /* Asegura que sean transacciones que afectan inventaraio (no productos no perpetuos, cargos etc.) */
group by 
       pa."control-ent", pa."trans-date"

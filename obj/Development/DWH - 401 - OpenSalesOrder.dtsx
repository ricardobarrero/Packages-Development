                  description="Extracts data from a relational database by using a .NET provider."
                  name="TRAX EXTRACTION"
                  usesDispositions="true"
                  version="4">
                  <properties>
                    <property
                      dataType="System.String"
                      description="Specifies the SQL statement used by the component to extract data."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
PUB."SO-TRANS"."CONTROL-ENT" AS CONTROL_ENT, 
PUB."SO-TRANS"."TRANS-CODE" AS TRANS_CODE, 
PUB."SO-TRANS"."TRANS-NO" AS TRANS_NO, 
PUB."SO-TRANS"."CUST-ID" AS CUST_ID, 
substring(PUB.CUSTOMER."NAME",1,120) AS CUSTOMER_NAME, 
PUB.CUSTOMER."PROF-ID" AS BUID,
CASE PUB.CUSTOMER."PROF-ID" WHEN 'A' THEN 'A' ELSE 'C' END AS PROFITID,
SO_PUB_PROD_EXP."PRODUCT-ID" AS PRODUCT_ID, 
SO_PUB_PROD_EXP.DESCRIPTION AS PRODUCT_DESCRIPTION, 
SUM(PUB."SO-TRANS-D"."ORDER-QTY") AS ORDER_QTY, 
SUM(PUB."SO-TRANS-D"."SHIP-QTY") AS SHIP_QTY, 
SUM(PUB."SO-TRANS-D"."ORDER-QTY" - PUB."SO-TRANS-D"."SHIP-QTY") AS ONSALESORDER, 
 
PROD_EXP_LOC."LINE-ID" AS LINE_ID, 
OSO_PRODUCT_SYN."SYNONYM" AS MPN,
PUB."SO-TRANS"."TRANS-DATE" AS TRANS_DATE, 
SUM(PUB."SO-TRANS-D"."HOME-AMT") AS TOTAL_ORDER_AMT, 
SUM((PUB."SO-TRANS-D"."HOME-AMT" / PUB."SO-TRANS-D"."ORDER-QTY") * PUB."SO-TRANS-D"."SHIP-QTY") AS TOTAL_SHIP_AMT , 
SUM((PUB."SO-TRANS-D"."HOME-AMT" / PUB."SO-TRANS-D"."ORDER-QTY") * (PUB."SO-TRANS-D"."ORDER-QTY" - PUB."SO-TRANS-D"."SHIP-QTY")) AS TOTAL_ONSALESORDER_AMT, 
SUM(PUB."SO-TRANS-D"."ALLOC-QTY") AS ALLOC_QTY, 
SUM((PUB."SO-TRANS-D"."HOME-AMT" / PUB."SO-TRANS-D"."ORDER-QTY") * PUB."SO-TRANS-D"."ALLOC-QTY") AS ALLOC_AMT, 
PUB."SO-TRANS-D"."LOC-ID" AS LOC_ID, 
PUB."SO-TRANS"."SALES-VALS" AS SALES_VALS, 
PUB."SO-TRANS"."OPER-ID" AS OPER_ID ,
  CASE PUB."SO-TRANS"."CONTROL-ENT" WHEN 'I' THEN 'XUS' ELSE UPPER(PUB."SO-TRANS"."CONTROL-ENT") END AS "COMPANYID",
SYSTIMESTAMP AS LASTEXTRACTION,
LOWER(PUB."SO-TRANS"."SOURCE-ID") AS SOURCE_ID,
LOWER(PUB."SO-TRANS"."WAVE-ID") AS WAVE_ID,
PUB."SO-TRANS-D"."PROMISE-DATE",
PUB."SO-TRANS"."TERM-ID",
PUB."SO-TRANS"."CUST-PO",
PUB."SO-TRANS-D"."SO-RECNO",
PUB."SO-TRANS-D"."LINE-NO",
PUB."SO-TRANS-D"."ALLOC-DATE",
PUB."SO-TRANS"."ACCEPT-BO",
PUB."SO-TRANS"."CURRENCY-ID",
CASE PUB."SO-TRANS"."WEB-COMPLETE" WHEN 1 THEN 'Y' ELSE 'F' END as WebComplete,
PUB."SO-TRANS"."EXPIRY-DATE"
FROM 
PUB."SO-TRANS", 
PUB."CUSTOMER", 
PUB."PROD-EXP" 
SO_PUB_PROD_EXP, 
PUB."SO-TRANS-D" LEFT OUTER JOIN PUB."PROD-EXP-SYN" OSO_PRODUCT_SYN ON
  ( PUB."SO-TRANS-D"."PROD-RECNO" = OSO_PRODUCT_SYN."PROD-RECNO" 
     AND OSO_PRODUCT_SYN."SYN-TYPE"='MAN' ), 
PUB."PROD-EXP-LOC" PROD_EXP_LOC,
PUB."PROD-EXP-LOC-Q" PROD_EXP_LOC_Q  
 
WHERE ( PUB."SO-TRANS"."MACHINE-ID"='' ) 
AND ( PUB."SO-TRANS-D"."MACHINE-ID"='' ) AND 
PUB."SO-TRANS"."CONTROL-ENT" IN ( 'I', 'XCB','XCL','XCO','XCR','XEC','XGT','XJM','XMX','XPA','XPE','XSV','XUY','XIQ','XHN','XPT','XCE','XNI','XBO','XTE','AMX','CPE','CMX','CCO','XDO','XCT') 
AND ( PUB."SO-TRANS"."CONTROL-ENT"=PUB."SO-TRANS-D"."CONTROL-ENT" 
AND PUB."SO-TRANS"."MACHINE-ID"=PUB."SO-TRANS-D"."MACHINE-ID" 
AND PUB."SO-TRANS"."TRANS-CODE"=PUB."SO-TRANS-D"."TRANS-CODE" 
AND PUB."SO-TRANS"."TRANS-NO"=PUB."SO-TRANS-D"."TRANS-NO" ) 
AND ( PUB."SO-TRANS-D"."PROD-RECNO"=SO_PUB_PROD_EXP."PROD-RECNO" ) 
AND ( PUB.CUSTOMER."MACHINE-ID"=PUB."SO-TRANS"."MACHINE-ID" --
AND PUB.CUSTOMER."CUST-ID"=PUB."SO-TRANS"."CUST-ID" ) --
AND ( PUB.CUSTOMER."MACHINE-ID"='' ) --

-- ADD ABR-14-2014
AND ( PUB."SO-TRANS-D"."PROD-RECNO" = PROD_EXP_LOC_Q."PROD-RECNO"
AND PUB."SO-TRANS-D"."LOC-ID" = PROD_EXP_LOC_Q."LOC-ID" )
AND (PROD_EXP_LOC_Q."PROD-RECNO"=PROD_EXP_LOC."PROD-RECNO"
AND PROD_EXP_LOC_Q."PARENT-LOC"=PROD_EXP_LOC."PARENT-LOC" )

AND (PROD_EXP_LOC_Q."SEQ-NO" = 0)
AND ( PUB."SO-TRANS"."TRANS-CODE" = 'OR' )
AND ( PUB."SO-TRANS".COMPLETE = 0 ) 
AND ( PUB."SO-TRANS-D".COMPLETE = 0 ) 
AND ( PUB."SO-TRANS-D"."ORDER-QTY"-PUB."SO-TRANS-D"."SHIP-QTY" &gt; 0 ) 
GROUP BY 
PUB."SO-TRANS"."CONTROL-ENT", 
PUB."SO-TRANS"."TRANS-CODE", 
PUB."SO-TRANS"."TRANS-NO", 
PUB."SO-TRANS"."CUST-ID", 
PUB.CUSTOMER.NAME, 
PUB.CUSTOMER."PROF-ID",
CASE PUB.CUSTOMER."PROF-ID" WHEN 'A' THEN 'A' ELSE 'C' END,
SO_PUB_PROD_EXP."PRODUCT-ID", 
SO_PUB_PROD_EXP.DESCRIPTION,  
PROD_EXP_LOC."LINE-ID", 
OSO_PRODUCT_SYN."SYNONYM",
PUB."SO-TRANS"."TRANS-DATE", 
PUB."SO-TRANS-D"."LOC-ID", 
PUB."SO-TRANS"."SALES-VALS", 
PUB."SO-TRANS"."OPER-ID",
PUB."SO-TRANS"."SOURCE-ID",
PUB."SO-TRANS"."WAVE-ID",
PUB."SO-TRANS-D"."PROMISE-DATE",
PUB."SO-TRANS"."TERM-ID",
PUB."SO-TRANS"."CUST-PO",
PUB."SO-TRANS-D"."SO-RECNO",
PUB."SO-TRANS-D"."LINE-NO",
PUB."SO-TRANS-D"."ALLOC-DATE",
PUB."SO-TRANS"."ACCEPT-BO",
PUB."SO-TRANS"."CURRENCY-ID",
  CASE PUB."SO-TRANS"."CONTROL-ENT" WHEN 'I' THEN 'XUS' ELSE PUB."SO-TRANS"."CONTROL-ENT" END,
PUB."SO-TRANS"."WEB-COMPLETE",
PUB."SO-TRANS"."EXPIRY-DATE"</property>

                  <properties>
                    <property
                      dataType="System.String"
                      description="Specifies the SQL statement used by the component to extract data."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  DISTINCT 
CASE WHEN PUB."SO-TRANS"."CONTROL-ENT" = 'I' THEN 'XUS' ELSE UPPER(PUB."SO-TRANS"."CONTROL-ENT") END  as CompanyID,
PUB."SO-TRANS"."TRANS-NO",PUB."PO-TRANS-D"."TRANS-NO" AS PONUMBER, 'I' as LinkkType, PUB."SO-PO-XREF"."SO-recno", PUB."SO-PO-XREF"."PO-RECNO"
FROM 
PUB."SO-TRANS" INNER JOIN PUB."SO-TRANS-D" ON (PUB."SO-TRANS"."MACHINE-ID"=PUB."SO-TRANS-D"."MACHINE-ID"
	AND PUB."SO-TRANS"."CONTROL-ENT"=PUB."SO-TRANS-D"."CONTROL-ENT" 
	AND PUB."SO-TRANS"."TRANS-CODE"=PUB."SO-TRANS-D"."TRANS-CODE" 
	AND PUB."SO-TRANS"."TRANS-NO"=PUB."SO-TRANS-D"."TRANS-NO" ) 
INNER JOIN PUB."SO-PO-XREF" ON (PUB."SO-PO-XREF"."MACHINE-ID"= PUB."SO-TRANS-D"."MACHINE-ID"
	AND PUB."SO-PO-XREF"."SO-RECNO" = PUB."SO-TRANS-D"."SO-RECNO")
INNER JOIN PUB."PO-TRANS-D" ON (PUB."PO-TRANS-D"."MACHINE-ID" = "SO-PO-XREF"."MACHINE-ID"
	AND PUB."PO-TRANS-D"."PO-RECNO" = "SO-PO-XREF"."PO-RECNO")
WHERE  
PUB."SO-TRANS"."CONTROL-ENT" IN ('I','XCB','XCL','XCO','XCR','XEC','XGT','XJM','XMX','XPA','XPE','XSV','XUY','XIQ','XHN','XPT','XCE','XNI','XBO','XTE','AMX','CPE','CMX','CCO','XDO','XCT')
AND PUB."SO-TRANS"."MACHINE-ID" = ''
AND PUB."SO-TRANS"."TRANS-CODE" = 'OR'
AND PUB."SO-TRANS".COMPLETE = 0  
AND PUB."SO-TRANS-D".COMPLETE = 0  
AND PUB."SO-TRANS-D"."ORDER-QTY"-PUB."SO-TRANS-D"."SHIP-QTY" &gt; 0</property>

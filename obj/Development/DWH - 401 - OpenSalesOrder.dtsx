                  <properties>
                    <property
                      dataType="System.String"
                      description="Specifies the SQL statement used by the component to extract data."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  DISTINCT CASE WHEN PUB."SO-TRANS"."CONTROL-ENT" = 'I' THEN 'XUS' ELSE PUB."SO-TRANS"."CONTROL-ENT" END AS CompanyID,
PUB."SO-TRANS"."TRANS-NO",
PUB."SO-PO-CROSS-LINK"."PO-TRANS-NO" as PONumber,
'E' as LinkType,
PUB."SO-PO-CROSS-LINK"."SO-RECNO",
PUB."SO-PO-CROSS-LINK"."PO-RECNO"
FROM
PUB."SO-TRANS" INNER JOIN PUB."SO-TRANS-D"  ON (PUB."SO-TRANS"."MACHINE-ID"=PUB."SO-TRANS-D"."MACHINE-ID"
        AND PUB."SO-TRANS"."CONTROL-ENT"=PUB."SO-TRANS-D"."CONTROL-ENT"
        AND PUB."SO-TRANS"."TRANS-CODE"=PUB."SO-TRANS-D"."TRANS-CODE"
        AND PUB."SO-TRANS"."TRANS-NO"=PUB."SO-TRANS-D"."TRANS-NO" )
INNER JOIN PUB."SO-PO-CROSS-LINK"  ON (PUB."SO-PO-CROSS-LINK"."MACHINE-ID"= PUB."SO-TRANS-D"."MACHINE-ID"
        AND PUB."SO-PO-CROSS-LINK"."SO-RECNO" = PUB."SO-TRANS-D"."SO-RECNO")
WHERE  PUB."SO-TRANS"."MACHINE-ID" = ''
AND PUB."SO-TRANS"."CONTROL-ENT"  IN ( 'I', 'XCB','XCL','XCO','XCR','XEC','XGT','XJM','XMX','XPA','XPE','XSV','XUY','XIQ','XHN','XPT','XCE','XNI','XBO','XTE','CPE','CMX','CCO','XDO','XCT')
AND PUB."SO-TRANS"."TRANS-CODE" = 'OR'
AND PUB."SO-TRANS".COMPLETE = 0</property>

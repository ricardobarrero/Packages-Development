                RemovedInt1,RemovedInt2,RemovedInt3,RemovedInt4,RemovedInt5,RemovedInt6,
                RolledUp, TotalAbandonedLessThanThreshold
               
       FROM
           [dbo].[ICE_Stat_QueueActivity_I]  Stat_QueueActivity inner  JOIN 
              [dbo].[ICE_ACDQueue] ACDQueue
               ON
               Stat_QueueActivity.SwitchID = ACDQueue.SwitchID AND
           Stat_QueueActivity.QueueID = ACDQueue.QueueID AND
             Stat_QueueActivity.CompanyID = ACDQueue.CompanyID  and 
                            CONVERT(VARCHAR, ACDQueue.LastExtraction, 112) = convert(varchar, getdate(),112)                                 
       WHERE
           Stat_QueueActivity.CompanyID in ( 'XCR' , 'XSV', 'XPA','XEC','XMX', 'XGT', 'XUY','XPE', 'XCL','XCB') and                   
                Stat_QueueActivity.StartDateTime BETWEEN @Date_ini and @Date_end --dateadd(d,-1,dateadd(mm,datediff(m,0,getdate()-9),1 )) AND  dateadd(s,-1,dateadd(mm,datediff(m,0,getdate()-9)+1,0)) 
             AND Stat_QueueActivity.QueueID BETWEEN  6000 AND 9999               

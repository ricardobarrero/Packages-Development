                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">declare @Date_ini datetime
declare @Date_end datetime

select @Date_ini = dateadd(d,-1,dateadd(mm,datediff(m,0,getdate()-1),1 )) 
select @Date_end =dateadd(s,-1,dateadd(mm,datediff(m,0,getdate()-1)+1,0))
print @Date_ini
print @Date_end



SELECT CompanyID, year(StartDateTime) AS Year, Month(StartDateTime)  AS Month, Day(StartDateTime)  AS Day, 
--TeamShortName, TeamName, SwitchID,
QueueShortName as TeamShortName,  '' as TeamName, SwitchID, 
 sum( AbandonedInt1+AbandonedInt2+AbandonedInt3+
           AbandonedInt4+AbandonedInt5+AbandonedInt6) AS Abandonadas,
             sum(
             HandledInThisQueueInt1+HandledInThisQueueInt2+
           HandledInThisQueueInt3+ HandledInThisQueueInt4+ 
             HandledInThisQueueInt5+
           HandledInThisQueueInt6 +
                 AbandonedInt1+AbandonedInt2+AbandonedInt3+
           AbandonedInt4+AbandonedInt5+AbandonedInt6
                ) AS Total_Inbound              
from (

       SELECT DISTINCT Stat_QueueActivity.CompanyID,  ACDQueue.QueueShortName,
           Stat_QueueActivity.StartDateTime, Stat_QueueActivity.SwitchID,
             
                    Stat_QueueActivity.IntervalID,
                    Stat_QueueActivity.IntervalDuration,
           Convert(varchar(8),Stat_QueueActivity.QueueID)+' - '+isnull(ACDQueue.QueueName,' ') AS QueueName, 

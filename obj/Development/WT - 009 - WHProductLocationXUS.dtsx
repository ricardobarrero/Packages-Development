          SQLTask:Connection="{B7AD50E2-39B9-400C-AB63-6C11342FB6C1}"
          SQLTask:SqlStatementSource="&#xA;update STG_WM_WHProductLocation&#xA;set EA_quantity = Quantity*Conv_Factor&#xA;from STG_WM_WHProductLocation pl inner join STG_WM_UNIT_CONV uc on (pl.unit_id =uc.Unit_ID and Conv_Unit_ID = 'ea')&#xA;where pl.unit_id like 'c%'&#xA;&#xA;update STG_WM_WHProductLocation&#xA;set EA_quantity = Quantity*cast(substring(Unit_Id,2,3) as int)&#xA;from STG_WM_WHProductLocation pl &#xA;where pl.unit_id like 'p%'&#xA;&#xA;update STG_WM_WHProductLocation&#xA;set EA_quantity = Quantity&#xA;where unit_ID ='ea' &#xA;&#xA;&#xA;/* UPDATE LOCALSKU */&#xA;&#xA;update S&#xA;set S.localsku = I.localSKU&#xA;from [dbo].[STG_WM_WHProductLocation] S , intxdw.dbo.dim_product I&#xA;where S.localrecno=I.localRecno&#xA;and S.companyID=I.companyID&#xA;&#xA;&#xA;&#xA;/*UPDATE LOCALSKU CUANDO ESTAN NULL*/&#xA;/* NO ESTAN EN LA TABLA DIM_PRODUCT   */&#xA;&#xA;UPDATE [dbo].[STG_WM_WHProductLocation] &#xA;SET LocalSKU='SINSKU0000'&#xA;WHERE LocalSKU IS NULL&#xA;&#xA;&#xA;&#xA;/*&#xA;update STG_WM_WHProductLocation&#xA;set EA_quantity = Quantity*Conv_Factor&#xA;from STG_WM_WHProductLocation pl inner join STG_WM_UNIT_CONV uc on (pl.unit_id =uc.Unit_ID and Conv_Unit_ID = 'ea') &#xA;*/" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

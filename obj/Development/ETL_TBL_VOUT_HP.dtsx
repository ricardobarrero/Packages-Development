          <DTS:Variables />
          <DTS:ObjectData>
            <pipeline
              BLOBTempStoragePath=""
              bufferTempStoragePath=""
              version="1">
              <components>
                <component
                  refId="Package\LOAD\LOAD TBL_VOUT_HP\ADO NET Source"
                  componentClassID="Microsoft.ManagedComponentHost"
                  contactInfo="Extracts data from a relational database by using a .NET provider.;Microsoft Corporation; Microsoft SqlServer v10; © 2007 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
                  description="Extracts data from a relational database by using a .NET provider."
                  name="ADO NET Source"
                  usesDispositions="true"
                  version="4">
                  <properties>
                    <property
                      dataType="System.String"
                      description="Specifies the SQL statement used by the component to extract data."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
 convert (int,convert(varchar(11),getdate(),112)) as DIM_PK_TIME,
  dbo.DIM_COMPANY.companyid,
  dbo.FACT_SERIAL_SALE.Serial_No,
  dbo.DIM_PRODUCT.localSKU,
  dbo.DIM_PRODUCT.manufSKU,
  vout.FACT_IM_SALES.AR_TransDate,
  vout.FACT_IM_SALES.AR_TransNo,
  sum(vout.FACT_IM_SALES.EA_ShipQty),
  dbo.FACT_SERIAL_SALE.Quantity,
  vout.fact_im_sales.LineNumber,
  getdate () as LastExtraction
FROM
  vout.FACT_IM_SALES,
  dbo.DIM_PRODUCT,
  dbo.DIM_CUSTOMER,
  dbo.DIM_TIME,
  dbo.DIM_LOCATION,
  dbo.DIM_COMPANY,
  dbo.FACT_SERIAL_SALE
WHERE
  ( dbo.DIM_COMPANY.DIM_PK_COMPANY=vout.FACT_IM_SALES.DIM_PK_COMPANY  )
  AND  ( dbo.DIM_LOCATION.DIM_PK_LOCATION=vout.FACT_IM_SALES.DIM_PK_LOCATION and dbo.DIM_LOCATION.DIM_PK_COMPANY=vout.FACT_IM_SALES.DIM_PK_COMPANY  )
  AND  ( dbo.DIM_PRODUCT.DIM_PK_PRODUCT=vout.FACT_IM_SALES.DIM_PK_PRODUCT and dbo.DIM_PRODUCT.DIM_PK_COMPANY=vout.FACT_IM_SALES.DIM_PK_COMPANY  )
  AND  ( vout.FACT_IM_SALES.DIM_PK_TIME=dbo.DIM_TIME.DIM_PK_TIME  )
  AND  ( vout.FACT_IM_SALES.DIM_PK_CUSTOMER=dbo.DIM_CUSTOMER.DIM_PK_CUSTOMER  )
  AND  ( (dbo.DIM_PRODUCT.locallineID in ('HAWC','HAWL','HAWX','HEAX','HELX','HENX','HEPX','HERX','HEWC','HEWL','HEWX','HNCL','HPAC','HPAL','HPBC','HPCC','HPCL','HPCX','HPFL','HPKC','HPKL','HPLC','HPML','HPNC','HPNL','HPOX','HPPX','HPQC','HPQL','HPQX','HPRC','HPRL','HPRX','HPSC','HPSL','HPSX','HPTC','HPTL','HPVL','HPWC')) and dbo.DIM_PRODUCT.locallineID not IN  ('cnec', 'seax', 'wdcx', 'hitx', 'seal','secx','saox')  )
  AND  ( dbo.DIM_COMPANY.companyID in ('XAM','XAR','XCL','XCO','XCR','XEC','XGT','XIA','XIQ','XJM','XPA','XPE','XPX','XSV','XUS','XUY','XMX','XHS','XCT','XCB','XGB','XHN','XPT','XCE','XNI','XRB')  )
  AND  ( dbo.DIM_PRODUCT.companyID in ('XAM','XAR','XCL','XCO','XCR','XEC','XGT','XIA','XIQ','XJM','XPA','XPE','XPX','XSV','XUS','XUY','XMX','XHS','XCT','XCB','XGB','XHN','XPT','XCE','XNI','XRB')  )
  AND  ( vout.FACT_IM_SALES.companyID in ('XAM','XAR','XCL','XCO','XCR','XEC','XGT','XIA','XIQ','XJM','XPA','XPE','XPX','XSV','XUS','XUY','XMX','XHS','XCT','XCB','XGB','XHN','XPT','XCE','XNI','XRB')  )
  AND  ( dbo.DIM_CUSTOMER.companyID in ('XAM','XAR','XCL','XCO','XCR','XEC','XGT','XIA','XIQ','XJM','XPA','XPE','XPX','XSV','XUS','XUY','XMX','XHS','XCT','XCB','XGB','XHN','XPT','XCE','XNI','XRB')  )
  AND  ( dbo.DIM_PRODUCT.centralSKU&lt;&gt;'ERROR' and dbo.DIM_PRODUCT.dw_status&lt;&gt;'d'  )
  AND  ( vout.FACT_IM_SALES.DIM_PK_COMPANY*=dbo.FACT_SERIAL_SALE.DIM_PK_COMPANY and vout.FACT_IM_SALES.PA_TransNo*=dbo.FACT_SERIAL_SALE.Pack_No and vout.FACT_IM_SALES.lineNumber*=dbo.FACT_SERIAL_SALE.Pack_line  )
  AND  ( case 
         when dbo.DIM_CUSTOMER.businessUnitID = 'A' then 'A'
         when dbo.DIM_CUSTOMER.businessUnitID = 'M' then 'M'
         Else 'C' end in ('A','M','C')  )
  AND  ( dbo.DIM_TIME.DIM_PK_TIME between convert(int, convert(varchar (11),getdate()-7,112)) and convert(int, convert(varchar (11),getdate()-1,112)) 
  AND  dbo.DIM_PRODUCT.localSKU  NOT IN  ('mt001hpx10', 'AO036HEX57', 'MT001HPX10', 'BT100HPX04', 'PC001HPX02', 'MT002HPX01', 'PC003XSV17', 'PC406HPQ04', ' PC327HPX48', 'NT804HPX60', 'NT804HPX15', 'NT500HPX02', 'PC327HPX48', 'NT804HPX61', 'NT804HPX19', 'NT500HPX02', 'PC327HPX48', 'MT001HPX10', 'MT001HPX20', 'MT001HPX35', 'NT804HPX13', 'NT100HPX01', 'PC327HPX48', 'PC327HPX46', 'PC327HPX51')
  )

GROUP BY
  dbo.DIM_COMPANY.companyid, 
  dbo.FACT_SERIAL_SALE.Serial_No, 
  dbo.DIM_PRODUCT.localSKU, 
  dbo.DIM_PRODUCT.manufSKU, 
  vout.FACT_IM_SALES.AR_TransDate, 
  vout.FACT_IM_SALES.AR_TransNo, 
  dbo.FACT_SERIAL_SALE.Quantity,
  vout.fact_im_sales.LineNumber</property>

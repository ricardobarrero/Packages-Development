                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select  companyid
       ,LocalSKU
	   ,sum(W.ReceiveQty) as ReceiveQty
	   ,SL.Sequence
	   ,SL.[Storage Type]
	   ,SL.Location_Type
	   ,SL.LOCATION
	   ,SL.[Pick Priority]
	   ,SL.[Target Length]
	   ,SL.[Target Width]
	   ,SL.[Target Hight]
	   ,SUM(SL.OH) as OH
	   ,SL.Assigned_SKU
	   ,0.0000 as LengthEach
       ,0.0000 as LengthCarton
       ,0.0000 as WidthEach
       ,0.0000 as WidthCarton
       ,0.0000 as HeightEach
       ,0.0000 as HeightCarton
       ,0.0000 as VolumeEach
       ,0.0000 as VolumeCarton
       ,0.0000 as WeightEach
       ,0.0000 as WeightCarton
	   ,'2020-04-12' as DATE_TIME_STAMP_EA
	   ,'2020-04-12' as DATE_TIME_STAMP_CARTON
	   ,0.0000 as AFS
	   ,0.0000 as OPO
	   ,getdate() as LastExtraction
from INTXDW.dbo.FACT_WORKSHEET W INNER JOIN   (select
						   					    P.sequence,
												P.[Storage Type],
												LT.Location_Type,
												L.LOCATION,
												P.[Pick Priority],
												CAST (substring (LT.LOCATION_TYPE,charindex('x',LT.LOCATION_TYPE)-3,charindex('x',ltrim(rtrim(substring (LT.LOCATION_TYPE,charindex('x',LT.LOCATION_TYPE)-2,len(LT.LOCATION_TYPE))))))AS INT)  as "Target Length",
												CAST (substring (ltrim(rtrim(substring (LT.LOCATION_TYPE,charindex('x',LT.LOCATION_TYPE)+1,len(LT.LOCATION_TYPE)))),1,charindex('x',ltrim(rtrim(substring (LT.LOCATION_TYPE,charindex('x',LT.LOCATION_TYPE)+1,len(LT.LOCATION_TYPE)))))-1) AS INT) as "Target Width",
												CAST (substring(ltrim(rtrim(substring (LT.LOCATION_TYPE,charindex('x',LT.LOCATION_TYPE)+1,len(LT.LOCATION_TYPE)))),charindex('x',ltrim(rtrim(substring (LT.LOCATION_TYPE,charindex('x',LT.LOCATION_TYPE)+1,len(LT.LOCATION_TYPE)))))+1,3) AS INT) as "Target Hight",
												LI.ITEM as "Assigned_SKU",
												ON_HAND_QTY as OH

									from        SQL5AWOL.ILS_XCR.dbo.LOCATION L
											 inner join SQL5AWOL.ILS_XCR.dbo.LOCATION_INVENTORY LI on L.LOCATION = LI.LOCATION
											 inner join SQL5AWOL.ILS_XCR.dbo.LOCATION_TYPE LT on L.LOCATION_TYPE = LT.LOCATION_TYPE
											 INNER JOIN SQL5AWOL.ILS_XCR.dbo.ITEM I ON I.Item = LI.Item
											 inner join DWStaging.dbo.STG_SCALE_SCALETYPE_PRIORITY P on P.Location = L.LOCATION COLLATE SQL_Latin1_General_CP1_CI_AS
									where LT.LOCATION_TYPE like 'Module%' and 
											      LT.LOCATION_TYPE &lt;&gt; 'Module CF  120x13' and 
												  LT.location_type not like 'Module Wire Deck%' and
											      (/*LI.PERMANENT = 'N' and */ I.USER_DEF1 IS NULL) 
												  --or not exists (select 1 from SQL5AWOL.ILS_XCR.dbo.LOCATION_INVENTORY S where W.LocalSKU = S.item COLLATE Latin1_General_CI_AS)
												  ) SL  
						ON W.LocalSKU =SL.Assigned_SKU COLLATE Latin1_General_CI_AS      
WHERE CompanyID='XCR' 
      AND LocationID = 'xcrd1'
group by companyid,LocalSKU,SL.Sequence,SL.[Storage Type],SL.Location_Type,SL.LOCATION,SL.[Pick Priority],SL.[Target Length],SL.[Target Width],SL.[Target Hight],SL.Assigned_SKU---,SL.OH

UNION

select  companyid
       ,LocalSKU
	   ,sum(W.ReceiveQty) as ReceiveQty
	   ,'' Sequence
	   ,'' [Storage Type]
	   ,'' Location_Type
	   ,'' LOCATION
	   ,0 [Pick Priority]
	   ,'' [Target Length]
	   ,'' [Target Width]
	   ,'' [Target Hight]
	   ,0 as OH
	   ,'' Assigned_SKU
	   ,0.0000 as LengthEach
       ,0.0000 as LengthCarton
       ,0.0000 as WidthEach
       ,0.0000 as WidthCarton
       ,0.0000 as HeightEach
       ,0.0000 as HeightCarton
       ,0.0000 as VolumeEach
       ,0.0000 as VolumeCarton
       ,0.0000 as WeightEach
       ,0.0000 as WeightCarton
	   ,'1980-01-01' as DATE_TIME_STAMP_EA
	   ,'1980-01-01' as DATE_TIME_STAMP_CARTON
	   ,0.0000 as AFS
	   ,0.0000 as OPO
	   ,getdate() as LastExtraction
from INTXDW.dbo.FACT_WORKSHEET W
where not exists (select 1 from SQL5AWOL.ILS_XCR.dbo.LOCATION_INVENTORY S where W.LocalSKU = S.item COLLATE Latin1_General_CI_AS and S.PERMANENT &lt;&gt; 'N')
and CompanyID = 'XCR'
and LocationID = 'xcrd1'
group by companyid,LocalSKU</property>

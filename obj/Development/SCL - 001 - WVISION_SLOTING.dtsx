                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">declare @fechaini int
declare @fechafin int

select @fechaini = convert(int, convert(varchar(11),getdate()-31,112))
select @fechafin = convert(int, convert(varchar(11),getdate()-1,112))

SELECT 
			 P.LocalSKU as SKU,
             ShipQty as QUANTITY,
			 cast(cast(sum(ShipQty) as decimal)/cast(30 as decimal) as decimal(10,2)) as DailyUnit
--INTO WORKAREA.DBO.STG_DailyUnits
FROM FACT_PRODUCT_SALE S WITH(NOLOCK) 
INNER JOIN DIM_PRODUCT P ON (S.companyID=P.CompanyID AND S.localsku=P.localsku and p.dw_status &lt;&gt; 'd') --376649 --382044
WHERE S.CompanyId='XCR' and S.lineID &lt;&gt; 'NPTX' and S.BusinessUnitID &lt;&gt; 'm' AND s.Locationid in ('xcrd1')
AND S.so_transID &lt;&gt; 'esd' and p.Type_ID not in ('c','l','e')
AND S.DIM_PK_TIME between  @fechaini and @fechafin
AND S.TRANSID='soin'
GROUP BY P.LocalSKU , ShipQty</property>

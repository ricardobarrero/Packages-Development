              SQLTask:Connection="{1289CF0B-D6EB-4445-A702-231D621F491E}"
              SQLTask:SqlStatementSource="--======================================&#xA;-- UPDATE MEASURE&#xA;--=======================================&#xA;&#xA;--select &#xA;update T set &#xA;&#x9;&#x9;&#x9;T.LengthEach = M.length&#xA;&#x9;&#x9;   ,T.WidthEach  = M.Width&#xA;&#x9;&#x9;   ,T.HeightEach = M.Height&#xA;&#x9;&#x9;   ,T.WeightEach = M.Weight&#xA;&#x9;&#x9;   ,T.DATE_TIME_STAMP_EA = M.DATE_TIME_STAMP&#xA;                                   ,T.CaseQTY = M.Conversion_QTY&#xA;&#xA;from dbo.SCALE_INPUT T inner join SQL5AWOL.ILS_XCR.dbo.ITEM_UNIT_OF_MEASURE M&#xA;   on T.LocalSKU COLLATE Latin1_General_CI_AS = M.Item&#xA;where M.QUANTITY_UM = 'EA';&#xA;&#xA;--select &#xA;update T set &#xA;&#x9;&#x9;&#x9;T.LengthCarton = M.length&#xA;&#x9;&#x9;   ,T.WidthCarton = M.Width&#xA;&#x9;&#x9;   ,T.HeightCarton = M.Height&#xA;&#x9;&#x9;   ,T.WeightCarton = M.Weight&#xA;&#x9;&#x9;   ,T.DATE_TIME_STAMP_CARTON = M.DATE_TIME_STAMP&#xA;                                   ,T.CaseQTY = M.Conversion_QTY&#xA;&#xA;from dbo.SCALE_INPUT T inner join SQL5AWOL.ILS_XCR.dbo.ITEM_UNIT_OF_MEASURE M&#xA;   on T.LocalSKU COLLATE Latin1_General_CI_AS = M.Item&#xA;where M.QUANTITY_UM = 'CS'&#xA;&#xA;&#xA;--======================================&#xA;-- UPDATE DailyUnit&#xA;--=======================================&#xA;&#xA;update dbo.SCALE_INPUT &#xA;set DailyUnit = DU.DailyUnit&#xA;from   dbo.SCALE_INPUT SI, dbo.DailyUnits DU&#xA;where DU.sku = SI.LocalSKU&#xA;&#xA;--======================================&#xA;-- UPDATE afs - opo&#xA;--=======================================&#xA;&#xA;&#xA;update .dbo.SCALE_INPUT&#xA;set afs = I.afs,&#xA;    opo = I.opo&#xA;from dbo.SCALE_INPUT SI , S91SQLUS.INTXDW.dbo.FACT_INVENTORY I&#xA;where SI.companyid = I.CompanyID and&#xA;      SI.LocalSKU = I.LocalSKU and &#xA;&#x9;  I.LocationID = 'xcrd1'" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

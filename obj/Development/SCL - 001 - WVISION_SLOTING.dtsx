                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  'XCR' as companyid,L.Location,L.location_sts,L.LOCATION_TYPE,LI.ITEM,SUM(LI.On_hand_qty) as On_hand_qty,Q.on_purch_order,
        CAST (substring (L.LOCATION_TYPE,charindex('x',L.LOCATION_TYPE)-3,charindex('x',ltrim(rtrim(substring (L.LOCATION_TYPE,charindex('x',L.LOCATION_TYPE)-2,len(L.LOCATION_TYPE))))))AS INT)  as "Target Length",
		CAST (substring (ltrim(rtrim(substring (L.LOCATION_TYPE,charindex('x',L.LOCATION_TYPE)+1,len(L.LOCATION_TYPE)))),1,charindex('x',ltrim(rtrim(substring (L.LOCATION_TYPE,charindex('x',L.LOCATION_TYPE)+1,len(L.LOCATION_TYPE)))))-1) AS INT) as "Target Width",
		CAST (substring(ltrim(rtrim(substring (L.LOCATION_TYPE,charindex('x',L.LOCATION_TYPE)+1,len(L.LOCATION_TYPE)))),charindex('x',ltrim(rtrim(substring (L.LOCATION_TYPE,charindex('x',L.LOCATION_TYPE)+1,len(L.LOCATION_TYPE)))))+1,3) AS INT) as "Target Hight"
FROM LOCATION L with(nolock) LEFT JOIN 
     location_inventory LI with(nolock) on (L.LOCATION = LI.Location) LEFT JOIN
	 SQL4AWOL.ODS.trax.prod_exp_loc_q Q with(nolock) on(LI.ITEM = Q.product_id collate SQL_Latin1_General_CP1_CI_AS and Q.loc_id='xcrd1')
where L.LOCATION_TYPE like 'Module%' and 
	  L.LOCATION_TYPE &lt;&gt; 'Module CF  120x13' and 
	  L.location_type not like 'Module Wire Deck%'
group by L.Location,L.location_sts,L.LOCATION_TYPE,LI.ITEM,Q.on_purch_order	  



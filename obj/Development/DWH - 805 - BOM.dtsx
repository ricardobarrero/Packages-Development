          DTS:TaskContact="Execute SQL Task; Microsoft Corporation; Microsoft SQL Server 2008 R2; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
          DTS:ThreadHint="0">
          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{D988F12D-7377-478C-A115-3EE9F9C5E370}"
              SQLTask:SqlStatementSource="--// Actualiza el campo DIM_PK_COMPANY //&#xA;&#xA;UPDATE b SET b.DIM_PK_COMPANY=a.DIM_PK_COMPANY&#xA;FROM INTXDW.DBO.DIM_COMPANY a,DWStaging.dbo.STG_SO_BOM b&#xA;WHERE a.companyid=b.CompanyID&#xA;&#xA;&#xA;--// Actualiza el campo DIM_PK_LOCATION //&#xA;&#xA;&#xA;update b  set b.DIM_PK_LOCATION=a.DIM_PK_LOCATION&#xA;from INTXDW.dbo.DIM_LOCATION a,DWStaging.dbo.STG_SO_BOM b&#xA;where a.companyID=b.CompanyID&#xA;and a.locationID=b.Pack_LocationID&#xA;&#xA;&#xA;--// Actualiza el campo DIM_PK_TIME//&#xA;&#xA;&#xA;update B set  b.DIM_PK_TIME=a.DIM_PK_TIME&#xA;from INTXDW.dbo.DIM_TIME a,DWStaging.dbo.STG_SO_BOM  b&#xA;where a.DIM_DATE=convert(datetime,B.Pack_Transdate)&#xA;&#xA;&#xA;&#xA;--//  Actualiza el campo DIM_PK_PRODUCT//&#xA;&#xA;update b set b.DIM_PK_PRODUCT=a.DIM_PK_PRODUCT&#xA;from INTXDW.dbo.DIM_PRODUCT a,DWStaging.dbo.STG_SO_BOM b&#xA;WHERE b.localSKU=a.localSKU AND a.companyid=b.CompanyID&#xA;&#xA;&#xA;--// Actualiza el campo DIM_PK_CUSTOMER//&#xA;&#xA;&#xA;update B&#xA;set B.dim_pk_customer = A.dim_pk_customer &#xA;from INTXDW.dbo.DIM_CUSTOMER A, dwstaging.dbo.STG_SO_BOM B&#xA;where A.companyid=B.CompanyID and upper (A.customerID_sentai) = upper (B.cust_ID)&#xA;&#xA;&#xA;--//update UPC_Code---//&#xA;&#xA;UPDATE STG_SO_BOM&#xA;SET UPC_CODE = UpcCode&#xA;FROM STG_SO_BOM S, SQLApp2List.SKUCENTRAL.DBO.ProductUpcCodes U&#xA;WHERE S.IM_Prod_recno = Centralrecno&#xA;&#xA;&#xA;&#xA;--// Actualiza  DIM_PK_SALESPERSON   --//&#xA;&#xA;UPDATE b set b.DIM_PK_SALESPERSON=a.DIM_PK_SALESPERSON&#xA;FROM intxdw.dbo.DIM_SALESPERSON a WITH(NOLOCK),DWStaging.dbo.STG_SO_BOM b with(nolock)&#xA;where a.salesID=b.SalesID&#xA;&#xA;&#xA;--// update  campos UnitPrice  , UnitPriceUS , ExtPrice,ExtPriceUS //&#xA;&#xA;&#xA;update a set a.UnitPrice=case when cast(a.UnitCost as float)=0 then 1 else cast(a.UnitCost as float) end/cast(b.Sum_Unitcost as float)* cast(C.UnitPrice as float ),&#xA;             a.UnitPriceUS=(case when cast(a.UnitCost as float)=0 then 1 else cast(a.UnitCost as float) end/cast(b.Sum_Unitcost as float)* cast(C.UnitPrice as float ))/cast(a.Sell_Rate as float),&#xA;             a.ExtPrice=case when cast(a.ExtCost as float)=0 then 1 else cast(a.ExtCost as float) end/cast(b.Sum_Excost as float)* cast(C.ExtPrice as float ),&#xA;             a.ExtPriceUS=(case when cast(a.ExtCost as float)=0 then 1 else cast(a.ExtCost as float) end/cast(b.Sum_Excost as float)* cast(C.ExtPrice as float)) /cast(a.Sell_Rate as float)&#xA;&#xA;FROM stg_so_bom a WITH (NOLOCK),&#xA;&#xA;(SELECT  AR_TransNo,CompanyId,Component_Type,Pack_LineNumber,UnitPrice,&#xA;SUM(UnitCost)Sum_Unitcost,SUM(ExtCost)Sum_Excost FROM stg_so_bom WHERE Component_Type='C'&#xA;GROUP BY AR_TransNo,CompanyId,Component_Type,Pack_LineNumber,UnitPrice) b,&#xA; &#xA;(SELECT UnitPrice,AR_TransNo,CompanyID,Pack_LineNumber,UnitPriceUS,ExtPrice,ExtPriceUS FROM stg_so_bom WHERE  Component_Type='B') C&#xA;&#xA;WHERE a.AR_TransNo=b.AR_TransNo&#xA;and a.CompanyID=b.CompanyID&#xA;and a.Pack_LineNumber=b.Pack_LineNumber&#xA;AND a.AR_TransNo=C.AR_TransNo&#xA;and a.CompanyID=C.CompanyID&#xA;and a.Pack_LineNumber=C.Pack_LineNumber&#xA;and a.Component_Type='C'&#xA;and a.UnitCost&lt;&gt;0" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>

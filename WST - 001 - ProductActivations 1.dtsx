      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{CA4944C3-2431-4834-BAD7-4AEBC1FFA781}"
          SQLTask:SqlStatementSource="UPDATE ProductActivations&#xA;SET UnitID = U.Unit&#xA;FROM ProductActivations PA,&#xA;      (select distinct&#xA;      CompanyID,&#xA;      LocalRecno,&#xA;      stuff((&#xA;        SELECT ',' + T.Unit_ID &#xA;              FROM dbo.STG_VM_PROD_UNIT AS T WHERE T.Localrecno = TE.Localrecno and T.companyid='XCL'&#xA;                          ORDER BY t.Unit_ID&#xA;               for xml path('')&#xA;               ),1,1,'') as Unit,&#xA;      LastExtraction&#xA;      from dbo.STG_VM_PROD_UNIT TE&#xA;      where companyid='XCL') U&#xA;WHERE PA.CompanyID = U.CompanyID ANd PA.Localrecno = U.LocalREcno;&#xA;&#xA;&#xA;UPDATE ProductActivations&#xA;SET UnitID = U.Unit&#xA;FROM ProductActivations PA,&#xA;      (select distinct&#xA;      CompanyID,&#xA;      LocalRecno,&#xA;      stuff((&#xA;        SELECT ',' + T.Unit_ID &#xA;              FROM dbo.STG_VM_PROD_UNIT AS T WHERE T.Localrecno = TE.Localrecno and T.companyid='XUS'&#xA;                          ORDER BY t.Unit_ID&#xA;               for xml path('')&#xA;               ),1,1,'') as Unit,&#xA;      LastExtraction&#xA;      from dbo.STG_VM_PROD_UNIT TE&#xA;      where companyid='XUS') U&#xA;WHERE PA.CompanyID = U.CompanyID ANd PA.Localrecno = U.LocalREcno;&#xA;&#xA;&#xA;&#xA;update s&#xA;Set S.UnitID= SUBSTRING (I.[Unit-vals],1,60)&#xA;from SQLAPPList.dwstaging.dbo.SENTAI_PRODUCT I, intcomexstore.dbo.ProductActivations S&#xA;where I.[Product-ID]=S.LocalsKU and s.CompanyId not in ('XUS','XCL')" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>

                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
	p.[Product-ID] as ProductID, p.[loc-group-id] as LocGroupID, t.[effect-date] as EffectDate, 
	case 
		when p.[loc-group-id] = 'm1' then 'XUS' 
			else upper(substring(p.[loc-group-id],1,3)) end as CompanyID,
	e.[Prod-recno],
	max(upper(p.[price-id])) as PriceID, 
	max(p.[price-amt]) as PriceAmt, 
	max(lower(p.[currency-id])) as CurrencyID	
FROM 
	dwstaging.dbo.sentai_prod_cp_price p with (nolock),
	(select [Product-ID], [loc-group-id], max([effect-date]) as [effect-date] 
	 from dwstaging.dbo.sentai_prod_cp_price with (nolock)
	 group by [Product-ID], [loc-group-id]) t,
	dwstaging.dbo.SENTAI_PRODUCT_EXP e with (nolock)
WHERE 	p.[product-id]= t.[product-id] 
		and p.[loc-group-id] = t.[loc-group-id] 
		and	p.[effect-date] = t.[effect-date] 
		and	p.[Product-ID] = e.[Product-ID] 
		and	p.[expiry-date] &gt;= getdate() 
		and p.[loc-group-id] not in ('a1','c1','xhs1')
AND p.[price-amt] is not null
GROUP BY p.[Product-ID], p.[loc-group-id], t.[effect-date], p.[loc-group-id], e.[prod-recno]</property>

                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  p.[prod-recno], 
	p.[parent-loc], 
	case when p."loc-group-id" = 'm1' then 'XUS' else upper(substring(p."loc-group-id" ,1,3)) end as CompanyID,
	lower(p.[status-code]) as status, p.stocked as stocked, 
	q.[on-hand] - q.[allocated] as Availability, 
	lower(e.[org-code]) as OrgCode,
	p.lastExtraction
--into    #temp
FROM	dwstaging.dbo.sentai_product_exp e with (nolock),
	dwstaging.dbo.sentai_prod_exp_loc p with (nolock), 
	dwstaging.dbo.sentai_prod_exp_loc_q q with (nolock) 
WHERE 	p.[prod-recno] = e.[prod-recno]
	and p.[product-id] = e.[product-id]
	and p.[prod-recno] = q.[prod-recno] 
	and p.[parent-loc] = q.[parent-loc]
	and p.[parent-loc] = q.[loc-id] 
	and p.[loc-group-id] in ('m1','xcl1','xmx1','xcr1','xpa1','xgt1','xsv1','xco1','xpe1','xec1','xar1','xuy1','xjm1')
	and p.[parent-loc] in ('m1','xcle1','xmxd1','xcr1','xpa1','xgt1','xsv1','xcoc1','xpel1', 'xecq1','xarb1','xuy1','xjm1')
	--and p.[status-code] = 'a' -- Only active products
	--and p.stocked=1 	-- Only products stockeables	
	and p."component-type" = 'n' 	-- Component Normal (includes normal, kits and components)
	and q."seq-no"=0	
	and e.[prod-type] &lt;&gt; 'o'</property>

      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{91383727-ACBB-4A55-875E-5BA7717CC73B}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="--========================================================================================================================&#xA;-- SUMA LA DISPONIBILIDAD DE LAS LOCALIDADES PRINCIPALES PARA XMX,XPE, Y XEC PORQUE TIENEN MAS DE UNA LOCALIDAD PRINCIPAL&#xA;-- Y HACE EL INSERT EN UNA TABLA TEMPORAL&#xA;--========================================================================================================================&#xA;SELECT  p.[prod-recno], case when p.&quot;loc-group-id&quot; = 'm1' then 'XUS' else upper(substring(p.&quot;loc-group-id&quot; ,1,3)) end as CompanyID,&#xA;&#x9; sum(q.[on-hand] - q.[allocated]) as Availability&#xA;&#x9;into    #temp&#xA;FROM&#x9;dwstaging.dbo.sentai_product_exp e with(nolock),&#xA;&#x9;dwstaging.dbo.sentai_prod_exp_loc p with(nolock), &#xA;&#x9;dwstaging.dbo.sentai_prod_exp_loc_q q with(nolock)&#xA;WHERE &#x9;p.[prod-recno] = e.[prod-recno]&#xA;&#x9;and p.[product-id] = e.[product-id]&#xA;&#x9;and p.[prod-recno] = q.[prod-recno] &#xA;&#x9;and p.[parent-loc] = q.[parent-loc]&#xA;&#x9;and p.[parent-loc] = q.[loc-id] &#xA;&#x9;and p.[loc-group-id] in ('xmx1','xpe1','xec1')&#xA;&#x9;and (p.[parent-loc] like ('xmx%1') OR P.[parent-loc] like 'xpe%1' OR p.[parent-loc] like 'xec%1')&#xA;&#x9;and p.[status-code] = 'a' -- Only active products&#xA;&#x9;and p.&quot;component-type&quot; in ('n','k') &#x9;-- Product Normal&#xA;&#x9;and q.&quot;seq-no&quot;=0&#x9;&#xA;&#x9;and e.[prod-type] &lt;&gt; 'o'&#xA;&#x9;--and p.[Product-ID] NOT LIKE 'PC%XGT%' -- add 20110331 Pidieron excluir estos productos del webstore&#x9;&#xA;&#x9;GROUP BY P.[prod-recno],  P.&quot;loc-group-id&quot;&#xA;HAVING  sum(q.[on-hand] - q.[allocated]) &gt;0 &#xA;&#xA;--==============================================================================================&#xA;-- HACE EL UPDATE DE LA DISPONIBILIDAD EN LA TABLA INTXDW.DBO.PRODUCT_ACTIVATION_BY_LOCATION_TBL&#xA;--==============================================================================================&#xA;&#xA;update P&#xA;set P.Availability = T.Availability&#xA;--select  P.[product-id],p.[companyid],P.Availability , T.Availability&#xA;from intxdw.dbo.Product_Activation_by_location_tbl P , #temp T&#xA;where P.companyid = T.CompanyID&#xA;and P.[prod-recno]=T.[prod-recno] &#xA;and P.Availability &lt;&gt; T.Availability&#xA;&#xA;--==========================&#xA;-- ELIMINA LA TABLA TEMPORAL&#xA;--==========================&#xA;drop table #temp" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>

                      <properties>
                        <property
                          dataType="System.String"
                          description="Specifies the SQL statement used by the component to extract data."
                          expressionType="Notify"
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  DISTINCT PUB."PO-TRANS"."CONTROL-ENT",
PUB."PO-TRANS"."TRANS-CODE",
PUB."PO-TRANS"."TRANS-NO",
PUB."SO-TRANS-D"."TRANS-NO" AS SO_ORTransNo,
PUB."SO-TRANS"."TRANS-DATE" AS SO_ORTransDate,
'I' as LinkType,
PUB."SO-PO-XREF"."SO-recno", 
PUB."SO-PO-XREF"."PO-RECNO"
FROM
PUB."PO-TRANS" INNER JOIN PUB."PO-TRANS-D"  ON (PUB."PO-TRANS"."MACHINE-ID"=PUB."PO-TRANS-D"."MACHINE-ID"
        AND PUB."PO-TRANS"."CONTROL-ENT"=PUB."PO-TRANS-D"."CONTROL-ENT"
        AND PUB."PO-TRANS"."TRANS-CODE"=PUB."PO-TRANS-D"."TRANS-CODE"
        AND PUB."PO-TRANS"."TRANS-NO"=PUB."PO-TRANS-D"."TRANS-NO" )
INNER JOIN PUB."SO-PO-XREF"  ON (PUB."SO-PO-XREF"."MACHINE-ID"= PUB."PO-TRANS-D"."MACHINE-ID"
        AND PUB."SO-PO-XREF"."PO-RECNO" = PUB."PO-TRANS-D"."PO-RECNO")
INNER JOIN PUB."SO-TRANS-D"  ON (PUB."SO-TRANS-D"."MACHINE-ID" = "SO-PO-XREF"."MACHINE-ID"
        AND PUB."SO-TRANS-D"."SO-RECNO" = "SO-PO-XREF"."SO-RECNO")
INNER JOIN PUB."SO-TRANS"  ON (PUB."SO-TRANS"."MACHINE-ID"=PUB."SO-TRANS-D"."MACHINE-ID"
        AND PUB."SO-TRANS"."CONTROL-ENT"=PUB."SO-TRANS-D"."CONTROL-ENT"
        AND PUB."SO-TRANS"."TRANS-CODE"=PUB."SO-TRANS-D"."TRANS-CODE"
        AND PUB."SO-TRANS"."TRANS-NO"=PUB."SO-TRANS-D"."TRANS-NO" )
WHERE  PUB."PO-TRANS"."MACHINE-ID" = ''
AND PUB."PO-TRANS"."CONTROL-ENT"  IN ( 'I', 'XCB','XCL','XCO','XCR','XEC','XGT','XJM','XMX','XPA','XPE','XSV','XUY','XHN','XPT','XCE','XNI','XPT','XTE','XBO','AMX','CPE','CCO','CMX','XCT','SMX') 
AND PUB."PO-TRANS"."TRANS-CODE" = 'OR'

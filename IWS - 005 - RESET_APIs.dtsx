                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT StatusCode
        , COUNT(1) AS API_Calls_Count
       , CAST(COUNT(1) * 100.0/ SUM(COUNT(1)) OVER () AS NUMERIC(5,2)) AS PercentageOfTotal
       , MIN(StartTimeStamp) AS FromTimeStamp
       , MAX(StartTimeStamp) AS ToTimeStamp
       , MIN(Local_StartTimeStamp) AS FromLocalTimeStamp
       , MAX(Local_StartTimeStamp) AS ToLocalTimeStamp

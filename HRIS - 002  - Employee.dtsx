              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT       e.employee_id,e.emp_firstname,e.emp_middle_name,e.emp_lastname,e.emp_work_email ,e.emp_status,st.name,j.id,j.job_title, e.coun_code,
             joined_date, cc.unit_id, e.work_station AS SubdivisionId, cc.name as SubdivisionName
       FROM hs_hr_employee e
             LEFT JOIN ohrm_job_title j ON e.job_title_code = j.id
             LEFT JOIN ohrm_employment_status  st  ON st.id = e.emp_status
             LEFT JOIN ohrm_subunit cc ON cc.id = e.work_station
       WHERE st.name NOT LIKE 'Terminated%'
       ORDER BY e.employee_id 





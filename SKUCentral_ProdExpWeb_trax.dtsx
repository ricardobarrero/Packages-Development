    <DTS:Executable
      DTS:refId="Package\Generate DWH_ProdExpWeb Table"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Table in SKU Central"
      DTS:DTSID="{05AF20A1-A9A0-4EC8-A520-0F68A93C8970}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Generate DWH_ProdExpWeb Table"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{6C7DCAFC-5B84-42D3-9292-F0A9C1C2EE9B}"
          SQLTask:SqlStatementSource="truncate table skucentral.dbo.DWH_ProdExpWeb;&#xA;&#xA;insert into skucentral.dbo.DWH_ProdExpWeb&#xA;&#xA;select distinct 'm1' as control_ent, &#xA;s.localrecno as prod_recno, &#xA;'   ' as display_web, &#xA;s.BusinessDivisionID as Business_division,&#xA;s.BrandID as Brand_id, &#xA;s.BrandDescription as Brand_name,&#xA;s.ParentCategoryID as Cat_id,&#xA;CategoryEn as category_name, &#xA;substring(categoryid, charindex('.',categoryid)+1, len(categoryid)) as subcategory_id,&#xA;SubCategoryEn as subcategory_name, &#xA;SubCategoryEn as subcategory_name2, &#xA;s.ManufacturerID as Manuf_id, &#xA;s.ManufacturerDescription as Manuf_name,&#xA;getdate()&#xA;from StaticProducts s &#xA;where ( (localrecno in (select distinct &quot;prod-recno&quot; from S91SQLUS.dwstaging.dbo.sentai_product_exp) and CreationDate &gt;= getdate()-10)&#xA;or localrecno in (select distinct localrecno from S91SQLUS.Intxdw.dbo.fact_inventory where opo &gt; 0 or oh&gt;0) )&#xA;and s.BusinessDivisionID is not null&#xA;and s.ParentCategoryID is not null&#xA;order by 2;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;/*&#xA;select distinct 'm1' as control_ent, &#xA;s.localrecno as prod_recno, &#xA;'   ' as display_web, &#xA;s.BusinessDivisionID as Business_division,&#xA;s.BrandID as Brand_id, &#xA;s.BrandDescription as Brand_name,&#xA;s.ParentCategoryID as Cat_id,&#xA;CategoryEn as category_name, &#xA;substring(categoryid, charindex('.',categoryid)+1, len(categoryid)) as subcategory_id,&#xA;SubCategoryEn as subcategory_name, &#xA;SubCategoryEn as subcategory_name2, &#xA;s.ManufacturerID as Manuf_id, &#xA;s.ManufacturerDescription as Manuf_name,&#xA;getdate()&#xA;from StaticProducts s &#xA;where ( (localrecno in (select distinct &quot;prod-recno&quot; from SQLAPPList.dwstaging.dbo.sentai_product_exp) and CreationDate &gt;= getdate()-60)&#xA;or localsku in ('AT216HEW69-B1','CS500GNC05','DH186HPS27','AB002LEN19',&#xA;'AB100HPS91',&#xA;'AT216HEW69-B1',&#xA;'NW232HPS30',&#xA;'NW232HPS65',&#xA;'NW232HPS66',&#xA;'AT216HEW69-B1',&#xA;'DH000HUA41',&#xA;'EN214CIS97',&#xA;'MM104NXT35',&#xA;'MM104NXT36',&#xA;'NT805HPX63'))&#xA;and s.BusinessDivisionID is not null&#xA;and s.ParentCategoryID is not null&#xA;order by 2;&#xA;*/&#xA;&#xA;&#xA;/*&#xA;truncate table DWH_ProdExpWeb;&#xA;&#xA;insert into DWH_ProdExpWeb&#xA;select distinct 'm1' as control_ent, &#xA;s.localrecno as prod_recno, &#xA;'   ' as display_web, &#xA;s.BusinessDivisionID as Business_division,&#xA;s.BrandID as Brand_id, &#xA;s.BrandDescription as Brand_name,&#xA;s.ParentCategoryID as Cat_id,&#xA;CategoryEn as category_name, &#xA;substring(categoryid, charindex('.',categoryid)+1, len(categoryid)) as subcategory_id,&#xA;SubCategoryEn as subcategory_name, &#xA;SubCategoryEn as subcategory_name2, &#xA;s.ManufacturerID as Manuf_id, &#xA;s.ManufacturerDescription as Manuf_name,&#xA;getdate()&#xA;from StaticProducts s &#xA;where localrecno in (select distinct &quot;prod-recno&quot; from SQLAPPList.dwstaging.dbo.sentai_product_exp)&#xA;and CreationDate &gt;= getdate()-60&#xA;and s.BusinessDivisionID is not null&#xA;and s.ParentCategoryID is not null&#xA;order by 2; &#xA;*/&#xA;&#xA;update t&#xA;set display_web = case when l.DisplayOnWeb = 1 then 'yes' else 'no' end&#xA;from DWH_ProdExpWeb t, ProductLocalSettings l&#xA;where t.prod_recno = l.localrecno&#xA;and companyid = 'XUS'&#xA;&#xA;update DWH_ProdExpWeb&#xA;set display_web = 'no' where display_web = ''&#xA;&#xA;-- ==========================================================================================================&#xA;&#xA;update t&#xA;set display_web = case when l.DisplayOnWeb = 1 then 'yes' else 'no' end&#xA;from DWH_ProdExpWeb t, ProductLocalSettings l&#xA;where t.prod_recno = l.localrecno&#xA;and companyid = 'XUS'&#xA;&#xA;update DWH_ProdExpWeb&#xA;set display_web = 'no' where display_web = ''&#xA;&#xA;-- ==========================================================================================================&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>

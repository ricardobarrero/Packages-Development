                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select S.RequestID, S.RequestType, S.companyid, S.localrecno, S.localsku, S.MPN, S.CreationDate, S.CreatedByName, S.ProcessDate , S.Status,convert (char(1),D.stocked) as stocked,D."parent-loc" as LoacationID,I.TypeID,D."status-code"
from ProductRequestsView S WITH(NOLOCK) INNER JOIN SQLAPPList.DWStaging.dbo.Sentai_prod_exp_loc D WITH(NOLOCK) 
       on S.LocalRecno= D."Prod-Recno" and S.LocalSku=D."CP-Detail" 
       and D."parent-loc"  in (select locationid from SQLAppList.intxdw.dbo.dim_location where abc_location = 'Y')  
       and S.CompanyId=(case when D."parent-loc" ='m1' then 'XUS' else SUBSTRING(D."parent-loc" ,1,3) end)
INNER JOIN dbo.products I WITH(NOLOCK) ON I.localsku=S.localsku and I.localrecno=S.LocalRecno
where RequestType = 'par' and isnull(S.status,'') not in ( 'i','x') and S.creationdate &gt;= getdate()-120

union 

select S.RequestID, S.RequestType, S.companyid, S.localrecno, S.localsku, S.MPN, S.CreationDate, S.CreatedByName, S.ProcessDate , S.Status,convert (char(1),D.stocked) as stocked ,D."parent-loc" as LoacationID,I.TypeID,D."status-code"
from ProductRequestsView S WITH(NOLOCK) INNER JOIN SQLAPPList.DWStaging.dbo.Sentai_prod_exp_loc D WITH(NOLOCK) 
       on S.LocalRecno= D."Prod-Recno" and S.LocalSku=D."CP-Detail" 
       and D."parent-loc"  in (select locationid from SQLAppList.intxdw.dbo.dim_location where abc_location = 'Y')   
INNER JOIN products I WITH(NOLOCK) ON I.localsku=S.localsku and I.localrecno=S.LocalRecno
and S.CompanyId=(case when D."parent-loc" ='m1' then 'XUS' else SUBSTRING(D."parent-loc" ,1,3) end)
where RequestType = 'pcr' and isnull(S.status,'') not in ( 'i','x') and S.creationdate &gt;= getdate()-120 


union 

SELECT s.requestid,s.RequestType,jsonlocalsett.CompanyId,s.LocalRecno,s.localsku,s.mpn,s.creationdate,'' as createdbyname,s.ProcessDate,s.Status
,convert (char(1),D.stocked) as stocked, D."parent-loc" as LoacationID ,I.TypeID,D."status-code"
FROM dbo.ProductRequests as s   CROSS APPLY OPENJSON (LocalSettings)
     WITH (
             CompanyId VARCHAR(5)  ,                                  
              PriceId VARCHAR(50),
             CurrencyId varchar(5),
             InitialCost varchar(50)
       ) AS jsonlocalsett
      INNER JOIN SQLAPPList.DWStaging.dbo.Sentai_prod_exp_loc D WITH(NOLOCK) 
       on S.LocalRecno= D."Prod-Recno" and S.LocalSku=D."CP-Detail" 
       and D."parent-loc"  in (select locationid from SQLAppList.intxdw.dbo.dim_location where abc_location = 'Y')  
       and jsonlocalsett.CompanyId=(case when D."parent-loc" ='m1' then 'XUS' else SUBSTRING(D."parent-loc" ,1,3) end)
       INNER JOIN dbo.products I WITH(NOLOCK) ON I.localsku=S.localsku and I.localrecno=S.LocalRecno
WHERE S.creationdate &gt;= getdate()-120
ORDER BY localsku



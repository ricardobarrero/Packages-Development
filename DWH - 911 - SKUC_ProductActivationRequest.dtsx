                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT s.requestid,s.RequestType,jsonlocalsett.CompanyId,s.LocalRecno,s.localsku,s.mpn,s.creationdate,V.createdbyname,S.ProcessDate,s.Status --42532
,convert (char(1),D.stocked) as stocked, D."parent-loc" as LoacationID ,I.TypeID,D."status-code",jsonlocalsett.initialcost
FROM dbo.ProductRequests as s   CROSS APPLY OPENJSON (LocalSettings)
     WITH (
             CompanyId VARCHAR(5)  ,                                  
              PriceId VARCHAR(50),
             CurrencyId varchar(5),
             InitialCost varchar(50)
       ) AS jsonlocalsett 
      INNER JOIN S91SQLUS.DWStaging.dbo.Sentai_prod_exp_loc D WITH(NOLOCK) 
       on S.LocalRecno= D."Prod-Recno" and S.LocalSku=D."CP-Detail" 
       and D."parent-loc"  in (select locationid from S91SQLUS.intxdw.dbo.dim_location where abc_location = 'Y')  
       and jsonlocalsett.CompanyId=(case when D."parent-loc" ='m1' then 'XUS' else SUBSTRING(D."parent-loc" ,1,3) end)
       INNER JOIN dbo.products I WITH(NOLOCK) ON I.localsku=S.localsku and I.localrecno=S.LocalRecno
	   INNER JOIN ProductRequestsView V WITH(NOLOCK)  on V.LocalRecno= D."Prod-Recno" and V.LocalSku=D."CP-Detail" and jsonlocalsett.CompanyId=V.companyid
WHERE S.creationdate &gt;= getdate()-120 --and s.localsku='AB000ADT01'
ORDER BY localsku</property>

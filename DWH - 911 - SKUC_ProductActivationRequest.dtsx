SELECT s.requestid,s.RequestType,jsonlocalsett.CompanyId,s.LocalRecno,s.localsku,s.mpn,s.creationdate,'' as createdbyname,s.ProcessDate,s.Status,convert (char(1),D.stocked) as stocked,D."parent-loc" as LoacationID,I.TypeID,D."status-code"
 FROM dbo.ProductRequests as s  INNER JOIN SQLAPPList.DWStaging.dbo.Sentai_prod_exp_loc D WITH(NOLOCK) 
       on S.LocalRecno= D."Prod-Recno" and S.LocalSku=D."CP-Detail" 
       and D."parent-loc"  in (select locationid from SQLAppList.intxdw.dbo.dim_location where abc_location = 'Y')  
       and S.CompanyId=(case when D."parent-loc" ='m1' then 'XUS' else SUBSTRING(D."parent-loc" ,1,3) end)
INNER JOIN dbo.products I WITH(NOLOCK) ON I.localsku=S.localsku and I.localrecno=S.LocalRecno

    CROSS APPLY OPENJSON (LocalSettings)
                            WITH (
     CompanyId VARCHAR(5)  ,                                  
     PriceId VARCHAR(50),
	 CurrencyId varchar(5),
	 InitialCost varchar(50)
	 ) AS jsonlocalsett
WHERE S.creationdate &gt;= getdate()-120
order by localsku
*/



                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/*BASE_LOCATIONS*/
select distinct
sl.CompanyID
,upper(s.prod_synonym) as SKU
          ,s.unit_id as UOM
          ,s.quantity as UOM_Quantity
          ,case when s.unit_id = 'ea' then pd.LengthEach when s.unit_id like 'c%' then pd.LengthCarton end as UOM_LENGTHT
          ,case when s.unit_id = 'ea' then pd.WidthEach  when s.unit_id like 'c%' then pd.WidthCarton end as UOM_WIDTH
          ,case when s.unit_id = 'ea' then pd.HeightEach when s.unit_id like 'c%' then pd.HeightCarton end as UOM_HEIGHT
          ,sl.FULL_ADDRESS as LOCATION
          ,ps.Units as UNIT_VALS
          ,s.unit_id
          ,sl.loc_type
		  ,pd.VolumeCarton
		  ,pd.VolumeEach
from stg_wm_storage_loc sl
inner join stg_vw_storage s on (sl.FULL_ADDRESS = s.full_address and sl.LocationID=s.LocationID )
left join stg_wm_product p on (s.product_id = p.product_id and s.LocationID=p.LocationID) 
left join STG_ProductSetup ps on (s.PROD_SYNONYM = ps.LocalSku and EnvironmentId = s.CompanyID)
left join STG_ProductDimensions pd on (s.PROD_SYNONYM = pd.LocalSku)
where sl.LocationID IN ('xcle1','m1')

union 

/*BASE_LOCATIONS*/
select 
sl.CompanyID
,upper(sl.LocalSKU) as SKU
          ,'' as UOM
          ,0 as UOM_Quantity
          ,0 as UOM_LENGTHT
          ,0 as UOM_WIDTH
          ,0 as UOM_HEIGHT
          ,'' as LOCATION
          ,'' as UNIT_VALS
          ,'ea' as unit_id
          ,100 as loc_type
		  ,pd.VolumeCarton
		  ,pd.VolumeEach
from INTXDW.dbo.FACT_INVENTORY sl
left join STG_ProductSetup ps on (sl.LocalSKU = ps.LocalSku and EnvironmentId = 'XCR')
left join STG_ProductDimensions pd on (sl.LocalSKU = pd.LocalSku)
where sl.LocationID in ('xcr1','xcrd1')
and sl.dw_status&lt;&gt;'D'</property>

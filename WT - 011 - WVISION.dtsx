                              UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">-----------insertar en BASE_ITEMS lo que no esta esta en BASE_INVENTORY 


select i.CompanyID,i.LocationID,i.Localsku as SKU, i.LocalRecno,p.descriptionlocal as DESCRIPTION,i.oh AS OHTRAX,isnull (P2.OH,0)as OHP2,i.AFS,
i.loc_status_code as STATUS_CODE, isnull(lower(abc.ABC_T_Company),'i') as ABC, (case when i.Stocked=0 Then 'No' when i.Stocked=1 then 'Yes' END) as STOCKED, i.opo as OPO ,
i.Total_RWQty as ONRW,
pd.LengthEach as EA_LENGHT, pd.WidthEach as EA_WIDTH, pd.HeightEach as EA_HEIGHT, pd.VolumeEach as EA_VOLUME,PD.WeightEach AS EA_WEIGHT,
pd.LengthCarton as CARTON_LENGHT, pd.WidthCarton as CARTON_WIDTH, pd.HeightCarton as CARTON_HEIGHT, pd.VolumeCarton as CARTON_VOLUME,PD.WeightCarton as CARTON_WEIGHT,'lb' as WEIGHT_UNIT
from fact_inventory i inner join dim_product p on (i.companyid = p.companyid and i.localsku = p.localsku and p.dw_status &lt;&gt; 'd')
left join Dwstaging.dbo.stg_slim_abc abc on (abc.companyid = i.companyid and abc.LocationID = i.LocationID and abc.LocalSKU = i.LocalSKU)
left join dwstaging.dbo.STG_ProductDimensions pd on (pd.localsku = i.localsku)
left join (SELECT SUM(QUANTITY_EA) as OH,PRODUCT_ID,CompanyID FROM DWSTAGING.DBO.STG_VW_STORAGE where LocationID IN ('xcle1','m1','xcr1','xcrd1') GROUP BY PRODUCT_ID,CompanyID ) P2 ON i.LocalRecno = P2.PRODUCT_ID  and i.CompanyID=P2.CompanyID
where i.LocalSKU in (select SKU from DWSTaging.[dbo].[STG_INVENTORY_LOCATIONS] IL where not exists (select * from Dwstaging.dbo.STG_BASE_ITEMS B where IL.SKU=B.SKU and IL.CompanyID = B.CompanyID) )
and (Stocked='yes' or P2.OH &gt;0 or i.opo &gt;0)
and (i.loc_status_code not in ('i','d') or p2.OH&gt;0)
and P.locallineID &lt;&gt; 'nptx'
and i.LocationID IN ('xcle1','m1','xcr1','xcrd1')
order by 1,2 --8393


                              name="SqlCommand"
                              UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/*BASE_INVENTORY*/
/*  se hace sum de QTY_EA */
select upper (SKU) as SKU
          , LOCATION
          ,case when loc_type = 1 then 'Primary' 
              when loc_type = 2 then 'Secondary'
             when loc_type = 3 then 'Mixed'
             when loc_type = 4 then 'Putaway'
             when loc_type = 5 then 'X-Doc'
             when loc_type = 6 then 'Reserved'
             when loc_type = 7 then 'Shipping'
             when loc_type = 8 then 'Secondary'
             when loc_type = 99 then 'Disabled' 
			 when loc_type = 100 then 'xcr1'end as AREA
        ,sum(QTY_EA) as QTY_EA
FROM dbo.STG_INVENTORY_LOCATIONS
where companyid=?
group by SKU
          , LOCATION
          ,case when loc_type = 1 then 'Primary' 
              when loc_type = 2 then 'Secondary'
             when loc_type = 3 then 'Mixed'
             when loc_type = 4 then 'Putaway'
             when loc_type = 5 then 'X-Doc'
             when loc_type = 6 then 'Reserved'
             when loc_type = 7 then 'Shipping'
             when loc_type = 8 then 'Secondary'
             when loc_type = 99 then 'Disabled' 
			 when loc_type = 100 then 'xcr1' end --5465

UNION

SELECT wmps.prod_synonym AS SKU ,wms.full_address AS LOCATION, 'Primary' AS AREA ,0 AS QTY_EA FROM STG_WM_STORAGE_LOC wms LEFT OUTER JOIN
STG_WM_STORAGE_LOC_PROD wmslp ON wms.zone_no = wmslp.zone_no AND wms.loc_id = wmslp.loc_id LEFT OUTER JOIN
STG_WM_Product_Syn wmps ON wms.product_id = wmps.product_id AND wmps.wm_syn_type_id = 'main'
WHERE wms.CompanyID=? AND
wmslp.zone_no IS NULL AND
wmslp.loc_id IS NULL AND
wms.loc_type = 1 AND
(wms.full_address LIKE 'M%' OR wms.full_address LIKE 'W%')--1296
and not exists (select 1 from dbo.STG_INVENTORY_LOCATIONS L where wmps.prod_synonym = L.sku and wms.full_address=l.LOCATION and l.CompanyID =?)</property>

                <SQLTask:SqlTaskData
                  SQLTask:Connection="{1A38ECE1-E5BD-497C-818B-90619130B9DC}"
                  SQLTask:SqlStatementSource="/******************* UPDATE MEASUREMENTS RED_TAG*******************************/&#xA;&#xA;UPDATE A&#xA;set A.EA_LENGHT  =  B.EA_LENGHT,&#xA;A.EA_WIDTH   =  B.EA_WIDTH,&#xA;A.EA_HEIGHT  =  B.EA_HEIGHT,&#xA;A.EA_VOLUME  =  B.EA_VOLUME,&#xA;A.CARTON_LENGHT  =  B.CARTON_LENGHT,&#xA;A.CARTON_WIDTH   =  B.CARTON_WIDTH,&#xA;A.CARTON_HEIGHT  =  B.CARTON_HEIGHT,&#xA;A.CARTON_VOLUME  =  B.CARTON_VOLUME&#xA;--select A.SKU , B.SKU&#xA;FROM STG_BASE_ITEMS A, STG_BASE_ITEMS B&#xA;WHERE SUBSTRING(A.SKU,1,10)=B.SKU&#xA;AND LEN(A.SKU)&gt;10    ;&#xA;&#xA;&#xA;/******************UPDATE FLAG CUBISCAN********************/&#xA;&#xA;update Dwstaging.dbo.STG_BASE_ITEMS&#xA;set flag = 1&#xA;where flag = 0 and exists (select 'e' from Dwstaging.dbo.STG_NOTINCUBISCAN C where Dwstaging.dbo.STG_BASE_ITEMS.sku = C.Localsku) ;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

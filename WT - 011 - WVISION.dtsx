                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select  
	   li.item as SKU, 
	   li.location, 
	   case when LOCATION_TYPE ='Floor' then '' when LOCATION_TYPE&lt;&gt;'Floor' then SUBSTRING(li.location,1,2) end as AREA,
	   sum(li.ON_HAND_QTY)as SKUQTY
from LOCATION_INVENTORY LI
inner join LOCATION L on (Li.LOCATION = L.LOCATION)
WHERE SUBSTRING(li.location,1,2) IN ('WH','MD')
group by  li.item , 
	   li.location, 
	   case when LOCATION_TYPE ='Floor' then '' when LOCATION_TYPE&lt;&gt;'Floor' then SUBSTRING(li.location,1,2) end
	   ORDER BY LI.LOCATION


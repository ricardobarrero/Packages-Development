                  SQLTask:SqlStatementSource="--- UPDATE UNIT_VALS DE WMSTOOLS XCL&#xA;&#xA;UPDATE [DBO].[STG_INVENTORY_LOCATIONS]&#xA;SET UNIT_VALS = PS.UNITS&#xA;--SELECT COUNT(*)&#xA;FROM [DBO].[STG_INVENTORY_LOCATIONS] I LEFT JOIN STG_PRODUCTSETUP PS ON (I.SKU = PS.LOCALSKU AND ENVIRONMENTID = 'XUS')&#xA;WHERE I.UNIT_VALS IS NULL ;&#xA;&#xA;&#xA;--- UPDATE UNIT_VALS DE TRAX&#xA;&#xA;&#xA;UPDATE [dbo].[STG_INVENTORY_LOCATIONS]&#xA;  SET [UNIT_VALS] = T.[Unit-vals]&#xA;  --SELECT COUNT(*)&#xA;  FROM [dbo].[STG_INVENTORY_LOCATIONS] W , SENTAI_PRODUCT T&#xA;  WHERE W.SKU = T.&quot;Product-ID&quot;&#xA;  AND W.[UNIT_VALS] IS NULL&#xA;&#xA;&#xA;--UPDATE QTY_EA&#xA;&#xA;update STG_INVENTORY_LOCATIONS&#xA;set QTY_EA = case when unit_id = 'ea' then UOM_Quantity end;&#xA;&#xA;update STG_INVENTORY_LOCATIONS&#xA;set QTY_EA = s.UOM_Quantity*uc.Conv_Factor&#xA;from STG_INVENTORY_LOCATIONS s inner join stg_wm_unit_conv uc on (s.unit_id = uc.Unit_ID and uc.Conv_Unit_ID='ea')&#xA;where s.unit_id like 'c%';&#xA;&#xA;&#xA;update STG_INVENTORY_LOCATIONS&#xA;set QTY_EA = s.UOM_Quantity*uc.Conv_Factor&#xA;from STG_INVENTORY_LOCATIONS s inner join stg_wm_unit_conv uc on (s.unit_id = uc.Unit_ID and uc.Conv_Unit_ID='c001')&#xA;where s.unit_id like 'p%'; &#xA;&#xA;update STG_INVENTORY_LOCATIONS&#xA;set QTY_EA = s.UOM_Quantity*uc.Conv_Factor&#xA;from STG_INVENTORY_LOCATIONS s inner join stg_wm_unit_conv uc on (s.unit_id = uc.Unit_ID and uc.Conv_Unit_ID='ea')&#xA;where s.unit_id like 'c%';&#xA;&#xA;update STG_INVENTORY_LOCATIONS&#xA;set QTY_EA = s.UOM_Quantity*uc.Conv_Factor&#xA;from STG_INVENTORY_LOCATIONS s inner join stg_wm_unit_conv uc on (s.unit_id = uc.Unit_ID and uc.Conv_Unit_ID='ea')&#xA;where s.unit_id like 'p%'; &#xA;&#xA;&#xA;--UPDATE QTY_EA PARA XCR SUM(OH) INVENTORY XCR1&#xA;&#xA;update STG_INVENTORY_LOCATIONS&#xA;set QTY_EA = I.qty_ea&#xA;from STG_INVENTORY_LOCATIONS W INNER JOIN (select companyid, localsku,sum(oh) as qty_ea from intxdw.dbo.fact_inventory where companyid='xcr' and dw_status &lt;&gt;'D' and LocationID='xcr1' group by companyid, localsku) I ON W.CompanyID=I.CompanyID AND W.SKU=I.LocalSKU &#xA;WHERE w.CompanyID='XCR';" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />

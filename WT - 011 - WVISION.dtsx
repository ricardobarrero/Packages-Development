              <DTS:Variables />
              <DTS:ObjectData>
                <SQLTask:SqlTaskData
                  SQLTask:Connection="{1A38ECE1-E5BD-497C-818B-90619130B9DC}"
                  SQLTask:SqlStatementSource="UPDATE STG_BASE_ITEMS&#xA;SET CASEQTY = (CASE&#xA;    WHEN P.UNIT_ID='c10k' THEN 10000&#xA;&#x9;WHEN P.UNIT_ID='c1k' THEN 1000&#xA;&#x9;WHEN P.UNIT_ID='c1k2' THEN 1200&#xA;&#x9;WHEN P.UNIT_ID='c1k4' THEN 1400&#xA;    WHEN P.UNIT_ID='c1k5' THEN 1500&#xA;&#x9;WHEN P.UNIT_ID='c1k6' THEN 1600&#xA;&#x9;WHEN P.UNIT_ID='c1k8' THEN 1800&#xA;&#x9;WHEN P.UNIT_ID='c2k'  THEN 2000&#xA;    WHEN P.UNIT_ID='c3k'  THEN 3000&#xA;&#x9;WHEN P.UNIT_ID='c5k'  THEN 5000&#xA;&#xA;    ELSE convert(int,SUBSTRING(P.UNIT_ID,2,3))&#xA;END)&#xA;--select distinct UNIT_ID,convert(int,SUBSTRING(P.UNIT_ID,2,3)) caseqty&#xA;FROM STG_WM_Prod_Unit P INNER JOIN STG_BASE_ITEMS W ON P.PRODUCT_ID = W.LOCALRECNO AND p.LocationID = 'xcle1'&#xA;WHERE P.UNIT_ID LIKE 'c%' and P.ACTIVE=1 ;&#xA;&#xA;UPDATE STG_BASE_ITEMS&#xA;SET CASEQTY = (CASE&#xA;    WHEN P.UNIT_ID='c10k' THEN 10000&#xA;&#x9;WHEN P.UNIT_ID='c1k' THEN 1000&#xA;&#x9;WHEN P.UNIT_ID='c1k2' THEN 1200&#xA;&#x9;WHEN P.UNIT_ID='c1k4' THEN 1400&#xA;    WHEN P.UNIT_ID='c1k5' THEN 1500&#xA;&#x9;WHEN P.UNIT_ID='c1k6' THEN 1600&#xA;&#x9;WHEN P.UNIT_ID='c1k8' THEN 1800&#xA;&#x9;WHEN P.UNIT_ID='c2k'  THEN 2000&#xA;    WHEN P.UNIT_ID='c3k'  THEN 3000&#xA;&#x9;WHEN P.UNIT_ID='c5k'  THEN 5000&#xA;&#xA;    ELSE convert(int,SUBSTRING(P.UNIT_ID,2,3))&#xA;END)&#xA;--select  UNIT_ID,convert(int,SUBSTRING(P.UNIT_ID,2,3)) caseqty,active&#xA;FROM STG_WM_Prod_Unit P INNER JOIN STG_BASE_ITEMS W ON P.PRODUCT_ID = W.LOCALRECNO  AND p.LocationID = 'm1'&#xA;WHERE P.UNIT_ID LIKE 'c%' AND CASEQTY IS NULL AND P.ACTIVE=1 and P.PRODUCT_ID&lt;&gt;'101679';&#xA;&#xA;UPDATE STG_BASE_ITEMS&#xA;SET PALLETQTY = convert(int,SUBSTRING(P.UNIT_ID,2,3))&#xA;--select distinct UNIT_ID,convert(int,SUBSTRING(P.UNIT_ID,2,3)) caseqty&#xA;FROM STG_WM_Prod_Unit P INNER JOIN STG_BASE_ITEMS W ON P.PRODUCT_ID = W.LOCALRECNO AND p.LocationID = 'xcle1'&#xA;WHERE P.UNIT_ID LIKE 'p%' and P.ACTIVE=1 ;&#xA;&#xA;UPDATE STG_BASE_ITEMS&#xA;SET PALLETQTY = convert(int,SUBSTRING(P.UNIT_ID,2,3))&#xA;--select distinct UNIT_ID,convert(int,SUBSTRING(P.UNIT_ID,2,3)) caseqty&#xA;FROM STG_WM_Prod_Unit P INNER JOIN STG_BASE_ITEMS W ON P.PRODUCT_ID = W.LOCALRECNO AND p.LocationID = 'm1'&#xA;WHERE P.UNIT_ID LIKE 'p%' AND PALLETQTY IS NULL AND P.ACTIVE=1 AND W.localRecno not in ('38576','38578','39203','39212') ;&#xA;&#xA;&#xA; &#xA;&#xA;&#xA;UPDATE A&#xA;set A.CASEQTY  =  B.CASEQTY,&#xA;    A.PALLETQTY  =  B.PALLETQTY&#xA;--select A.SKU , B.SKU,A.CASEQTY  ,  B.CASEQTY,A.PALLETQTY  ,  B.PALLETQTY&#xA;FROM STG_BASE_ITEMS A, STG_BASE_ITEMS B&#xA;WHERE SUBSTRING(A.SKU,1,10)=B.SKU&#xA;AND LEN(A.SKU)&gt;10 &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
              </DTS:ObjectData>

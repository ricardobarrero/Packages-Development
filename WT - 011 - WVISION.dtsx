                        <property
                          dataType="System.String"
                          description="The SQL command to be executed."
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 
     p.CompanyID
	,LocationID as Warehouse_ID
	,D.LocalSku as SKU
	,Product_id as SKU_Code
	,FULL_ADDRESS as Primary_Pick_Location
	,cast (MIN_QTY as int) as Trigger_Point
	,cast(REPLEN_QTY as int) as Replenishment_Qty
	,cast(MAX_QTY as int) AS MAX_QTY
	,'' as Conveyable_Code
	,LOC_DESC
	,LOC_TYPE
from STG_WM_STORAGE_LOC P INNER JOIN INTXDW.dbo.DIM_PRODUCT D ON P.PRODUCT_ID=D.localrecno and D.companyID=P.CompanyID and D.dw_status&lt;&gt;'d'
where P.LocationID IN ('xcle1','m1','xcr1','xcrd1') and P.LOC_TYPE=1</property>

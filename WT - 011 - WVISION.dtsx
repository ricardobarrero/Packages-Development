                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--- FILE RW_BATCH
Use INTXDW 
select w.companyid, w.locationid, w.LoteNo, w.Batch_OriginalDoc, w.Localsku, w.Description, w.ManufSKU, w.TransNo as RW_Transno, w.Transdate as RW_Transdate, 
w.TransNo_OR as PO_TransNo, p.transdate as PO_Transdate, p.Order_qty ,(W.OrderQty-W.ReceiveQty) as RW_Qty
from fact_worksheet w left join 
(select distinct companyid, transno, transdate, linenumber, PO_OrderQty - ReceiveQty as Order_qty from fact_purchase_header where companyid IN ('XCL','XUS','XCR')  and dw_status &lt;&gt; 'd') as P on (w.companyid = p.companyid and w.TransNo_OR = p.transno and w.LineNumber = p.linenumber)  --269
where w.companyid IN ('XCL','XUS','XCR') and LoteNo is not null
order by 1</property>
